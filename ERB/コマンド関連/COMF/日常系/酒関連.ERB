;廃止
;@醸造日数(ARG)
;@醸造できた量(ARG)


@酒造
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS ALTER
DAY:10 ++
LOCAL = GET_INT(0, "酒データ", ALC_ID(), "醸造日数")
;speed up from brewer talent
SIF TALENT:MASTER:酒造家 >= 2 && LOCAL > 2
	LOCAL--
IF LOCAL > 0 && LOCAL <= DAY:10
	ALTER = %GET_STR(0, "酒データ", ALC_ID(), "変化先")%
	CALL COLORMESSAGE(@"酒虫之瓶中的%VAR_FERMENT%变成了%ALTER%",C_YELLOW, 2)
	DAY:10 = 0
	VAR_FERMENT = %ALTER%
	CALL BREWING_GUIDE_MAKE_KNOWN(VAR_FERMENT)
ENDIF

@DRUNKEN(C_ID, itemnum)
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM C_ID
#DIM 酒ID
#DIM itemnum
#DIM 酒気増加量

LOCALS = %ITEMNAME:itemnum%

酒ID = ALC_ID(LOCALS)

酒気増加量 = GET_INT(C_ID, "酒データ", 酒ID, "酒気増加量")
;一夜奇稻田を除く
IF LOCALS != "一夜奇稻田"
	IF TALENT:C_ID:酒耐性 == -1
		TIMES 酒気増加量, 1.2
	ELSEIF TALENT:C_ID:酒耐性 == 1
		TIMES 酒気増加量, 0.9
	ELSEIF TALENT:C_ID:酒耐性 == 2
		TIMES 酒気増加量, 0.75
	ELSEIF TALENT:C_ID:酒耐性 == 3
		TIMES 酒気増加量, 0.5
	ELSEIF TALENT:C_ID:酒耐性 == -2
		IF CFLAG:C_ID:不會喝酒 >= 5
			TIMES 酒気増加量, 2.0
		ELSEIF CFLAG:C_ID:不會喝酒 >= 2
			TIMES 酒気増加量, 1.5
		ELSE
			TIMES 酒気増加量, 1.0
		ENDIF
	ENDIF
ENDIF
SIF C_ID == MASTER
	酒気増加量 = 酒気増加量 / (1 + TFLAG:醒酒药)
BASE:C_ID:酒気 = MIN(BASE:C_ID:酒気 + 酒気増加量 ,MAXBASE:C_ID:酒気)
@酒一览表示(ARG, ARGS, 割合)
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM 割合
CALL 酒一覧表示(ARG, ARGS, 割合)
RETURN RESULT

@酒一覧表示(ARG, ARGS, 割合)
;所持数がARG未満だと非表示、ARG以上所有してる酒を選択するとそのアイテム番号をRETURNする
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM ITEM_ID
#DIM 割合
;qol replaced with paginated version (not for shops)
IF ARGS != "売却"
	CALL QOL_ALCOHOLMENU(ARG, 割合)
	RETURN RESULT
ENDIF
$LOOP

FOR ITEM_ID, 0, 600
	IF IS_ALC(ITEM_ID) && ITEM:ITEM_ID >= ARG
		IF ARGS == "売却"
			PRINTFORML [{ITEM_ID}]%ITEMNAME_TR(ITEM_ID), 20, LEFT% (%金額表示(ITEMPRICE:ITEM_ID * 割合 / 100), 7, RIGHT% / 所持: {ITEM:ITEM_ID, 3, RIGHT})
		ELSE
			;CALL PRINT_ITEM_EXPLANATION(ITEM_ID)
			CALL PRINT_ITEM_EXPLANATION2(ITEM_ID);qol custom code
		ENDIF
	ENDIF
NEXT
DRAWLINE 
PRINTL [999] 中止
INPUT
IF IS_ALC(RESULT) && ITEM:RESULT >= ARG
	RETURN RESULT
ELSEIF RESULT == 999
	RETURN 0
ELSE
	GOTO LOOP
ENDIF

