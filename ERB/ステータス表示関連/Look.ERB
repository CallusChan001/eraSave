					ELSEIF 充填率(TARGET,1) >= 100 || 充填率(TARGET,2) >= 100
						PRINTFORM  挤出的精液拉着不透明的白色淫丝下落
					ELSEIF 充填率(TARGET,1) >= 70 || 充填率(TARGET,2) >= 70
						PRINTFORM  溢出的精液顺腿流下
					ELSEIF 充填率(TARGET,1) >= 40 || 充填率(TARGET,2) >= 40
						PRINTFORM  大腿上留有精液的痕迹
					ELSEIF 充填率(TARGET,1) >= 10 || 充填率(TARGET,2) >= 10
						PRINTFORM  羞人的部分挂着精液
					ENDIF
					
				ELSEIF 充填率(TARGET,1) >= 150 || 充填率(TARGET,2) >= 150
					PRINTFORM  周围空气里弥漫着\@TALENT:MASTER:濃厚精液 ? 浓厚且独属于%CALLNAME:MASTER%白浊的气味 # 厚重的精液气味\@ ❤
				ELSEIF 充填率(TARGET,1) >= 100 || 充填率(TARGET,2) >= 100
					PRINTFORM  喷出的\@TALENT:MASTER:濃厚精液 ? 浓厚白浊在脚边堆成了山 # 精液在脚边形成一滩水渍\@
				ELSEIF 充填率(TARGET,1) >= 70 || 充填率(TARGET,2) >= 70
					PRINTFORM  溢出的\@TALENT:MASTER:濃厚精液 ? 粘稠精液在大腿上拉出淫丝 # 精液垂落拉丝\@
				ELSEIF 充填率(TARGET,1) >= 40 || 充填率(TARGET,2) >= 40
					PRINTFORM  大腿上\@TALENT:MASTER:濃厚精液 ? 满是粘稠精液的痕迹 # 满是精液的痕迹\@
				ELSEIF 充填率(TARGET,1) >= 10 || 充填率(TARGET,2) >= 10
					PRINTFORM  羞人的部分\@TALENT:MASTER:濃厚精液 ? 残留着粘稠精液的痕迹 # 残留着精液的痕迹\@
				ENDIF

			ELSEIF EQUIP:下半身内衣１ || EQUIP:下半身内衣２
				;2025年7月10日修改 创口贴、兜裆布、贞操带在两类内裤中重复，可以复用加笔的分枝
				IF EQUIP:下半身内衣２
					表示内容 '= PANTSNAME(EQUIP:TARGET:下半身内衣２, TARGET)
				ELSE
					表示内容 '= GET_STR(TARGET, "下半身内衣_ずらし不可", EQUIP:TARGET:下半身内衣１, "名前")
				ENDIF
				SELECTCASE 表示内容
					CASE "兜襠布", "安全褲"
						IF 充填率(TARGET,1) >= 150 || 充填率(TARGET,2) >= 150 || PALAM:潤滑 >= PALAMLV:7
							PRINTFORM  %表示内容%吸满了溢出的体液不断下坠、羞人的部分被看光光了❤
						ELSEIF 充填率(TARGET,1) >= 100 || 充填率(TARGET,2) >= 100 || PALAM:潤滑 >= PALAMLV:6
							PRINTFORM  %表示内容%沉甸甸的不能再吸收体液了、羞人的部分要露出来了❤
						ELSEIF 充填率(TARGET,1) >= 70 || 充填率(TARGET,2) >= 70 || PALAM:潤滑 >= PALAMLV:4
							PRINTFORM  %表示内容%明显完全湿透了、真诱人❤
						ELSEIF 充填率(TARGET,1) >= 40 || 充填率(TARGET,2) >= 40 || PALAM:潤滑 >= PALAMLV:2
							PRINTFORM  %表示内容%上面染满了水渍
						ELSEIF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛門射精 == 1 || PALAM:潤滑 >= PALAMLV:1
							PRINTFORM  %表示内容%有被体液濡湿的痕迹
						ENDIF

					CASE "Ｃ字袴", "創口貼","御札"
						IF 充填率(TARGET,1) >= 150 || 充填率(TARGET,2) >= 150 || PALAM:潤滑 >= PALAMLV:7
							PRINTFORM  %表示内容%已经剥落了下来和黏液一起垂落、露出了羞人的部分❤
						ELSEIF 充填率(TARGET,1) >= 100 || 充填率(TARGET,2) >= 100 || PALAM:潤滑 >= PALAMLV:6
							PRINTFORM  %表示内容%勉强在上面挂着、羞人的部分能看到一半了❤
						ELSEIF 充填率(TARGET,1) >= 70 || 充填率(TARGET,2) >= 70 || PALAM:潤滑 >= PALAMLV:4
							PRINTFORM  %表示内容%开始剥落了、羞人的部分都要露出来了❤
						ELSEIF 充填率(TARGET,1) >= 40 || 充填率(TARGET,2) >= 40 || PALAM:潤滑 >= PALAMLV:2
							PRINTFORM  %表示内容%完全湿透了
						ELSEIF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛門射精 == 1 || PALAM:潤滑 >= PALAMLV:1
							PRINTFORM  %表示内容%有被体液濡湿的痕迹
						ENDIF

					CASE "貞操帯", "陰唇環", "陰蒂環"
						IF 充填率(TARGET,1) >= 150 || 充填率(TARGET,2) >= 150 || PALAM:潤滑 >= PALAMLV:7
							IF 表示内容 == "貞操帯"
								PRINTFORM  %表示内容%的缝隙不断喷出体液❤
							ELSE
								PRINTFORM  %表示内容%被溢出的体液彻底淹没❤
							ENDIF
						ELSEIF 充填率(TARGET,1) >= 100 || 充填率(TARGET,2) >= 100 || PALAM:潤滑 >= PALAMLV:6
							IF 表示内容 == "貞操帯"
								PRINTFORM  %表示内容%完全无法阻止溢出的精液❤
							ELSE
								PRINTFORM  %表示内容%被溢出的体液搞到完全看不清❤
							ENDIF
						ELSEIF 充填率(TARGET,1) >= 70 || 充填率(TARGET,2) >= 70 || PALAM:潤滑 >= PALAMLV:4
							PRINTFORM  %表示内容%里面悬满了体液
						ELSEIF 充填率(TARGET,1) >= 40 || 充填率(TARGET,2) >= 40 || PALAM:潤滑 >= PALAMLV:2
							PRINTFORM  %表示内容%完全湿透了
						ELSEIF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛門射精 == 1 || PALAM:潤滑 >= PALAMLV:1
							PRINTFORM  %表示内容%被体液打湿
						ENDIF
					CASEELSE
						IF 充填率(TARGET,1) >= 150 || 充填率(TARGET,2) >= 150 || PALAM:潤滑 >= PALAMLV:7
							PRINTFORM  %表示内容%吸满了溢出的体液不断下坠、羞人的部分被看光光了❤
						ELSEIF 充填率(TARGET,1) >= 100 || 充填率(TARGET,2) >= 100 || PALAM:潤滑 >= PALAMLV:6
							PRINTFORM  %表示内容%沉甸甸的不能再吸收体液了、羞人的部分要露出来了❤
						ELSEIF 充填率(TARGET,1) >= 70 || 充填率(TARGET,2) >= 70 || PALAM:潤滑 >= PALAMLV:4
							PRINTFORM  %表示内容%明显完全湿透了、真诱人❤
						ELSEIF 充填率(TARGET,1) >= 40 || 充填率(TARGET,2) >= 40 || PALAM:潤滑 >= PALAMLV:2
							PRINTFORM  %表示内容%上面染满了水渍
						ELSEIF CFLAG:TARGET:膣内射精 == 1 || CFLAG:TARGET:肛門射精 == 1 || PALAM:潤滑 >= PALAMLV:1
							PRINTFORM  %表示内容%有被体液濡湿的痕迹
						ENDIF
				ENDSELECT
			;没穿下半身衣物
			ELSE
				PRINT 赤裸着 
				SIF TEQUIP:陰蒂夾
					PRINT 阴蒂夹吸在阴蒂上震动
				SIF TEQUIP:飛機杯
					PRINT 飞机杯套在阴茎上震动
				IF !CFLAG:诶嘿嘿
					IF CFLAG:TARGET:膣内射精
						IF CFLAG:TARGET:肛門射精 == 1
							IF 充填率(TARGET,1) >= 150 || 充填率(TARGET,2) >= 150
								PRINT 精液从两穴涌泉般喷泻而出
							ELSEIF 充填率(TARGET,1) >= 100 || 充填率(TARGET,2) >= 100
								PRINT 精液从两穴中如关不紧的水龙头一样喷出❤
							ELSEIF 充填率(TARGET,1) >= 70 || 充填率(TARGET,2) >= 70
								PRINT 精液从两穴中不断溢出 
							ELSEIF 充填率(TARGET,1) >= 30 || 充填率(TARGET,2) >= 30
								PRINT 精液从两穴中溢出
							ELSE
								PRINT 精液从私处和肛门里缓缓流出 
							ENDIF
						ELSEIF EX:処女喪失记号
							PRINT 混合着破瓜之血的精液从私处流了出来
						ELSEIF PALAM:潤滑 >= PALAMLV:2
							PRINT 精液和爱液的混合物从私处流了出来
						ELSE
							PRINT 精液从蜜唇中流了出来 
						ENDIF
					ELSEIF CFLAG:TARGET:肛門射精 == 1
						PRINT 精液从肛门里流了出来
					ELSEIF PALAM:潤滑 >= PALAMLV:2
						IF PALAM:潤滑 >= PALAMLV:6
							PRINTFORM \@ EX:処女喪失记号? 混合着破瓜之血的# \@爱液从私处滴落下来
						ELSEIF EX:処女喪失记号
							PRINT 破瓜之血从私处流了出来 
						ELSEIF PALAM:潤滑 >= PALAMLV:4
							PRINT 爱液流到了大腿上
						ELSEIF PALAM:潤滑 >= PALAMLV:2
							PRINT 私处被爱液打湿了
						ENDIF
					ENDIF
				ELSEIF PALAM:潤滑 >= PALAMLV:2 && !(CFLAG:TARGET:膣内射精 == 1 || TEQUIP:Ｖ性交 == PLAYER || TEQUIP:振動棒)
					IF PALAM:潤滑 >= PALAMLV:4
						PRINTFORM \@ EX:処女喪失记号? 混合着破瓜之血的# \@爱液从私处滴落下来
					ELSEIF EX:処女喪失记号
						PRINT 破瓜之血从私处流了出来
					ELSEIF PALAM:潤滑 >= PALAMLV:3
						PRINTFORM 爱液流到了大腿上 
					ELSEIF PALAM:潤滑 >= PALAMLV:2
						PRINTFORM 私处被爱液打湿了 
					ELSEIF TALENT:処女 == 1
						PRINTFORM 露出了未经人事紧紧闭合的蜜缝 
					ENDIF
				ENDIF
			ENDIF
		;穿着下半身衣物，并且也穿着内衣
		ELSE
			着衣状況 = 0
			;如果穿着上下连体的外套，就显示它
			;如果没有，就显示裙子或裤子
			;因为没有特别复杂的顺序，所以合并了循环
			FOR 装备部位, 15, 4, -1
				SIF GROUPMATCH(装备部位, 8, 7)
					CONTINUE
				表示内容 '= LOOK_AT_CLOTHNAME(TARGET, 装备部位)
				SIF !STRLENS(表示内容)
					CONTINUE
				PRINTFORM %表示内容% 
				IF !TEQUIP:TARGET:SWIMMING
					SELECTCASE 湿身度:TARGET
					CASE 10 TO 29
						PRINTFORM 布料因为飞来的雨水似乎起了皱 
					CASE 30 TO 80
						PRINTFORM 布料吸满了水贴在大腿上显得腿型更加优美 
					CASE 80 TO 100
						PRINTFORM 湿透的布料衬托得%HIP(TARGET)%愈发诱人 
					ENDSELECT
				ENDIF
				BREAK
			NEXT
			IF !CFLAG:诶嘿嘿
				;なんか分岐入ってたけどTEQUIP:下半身着衣状況 == 0は↑で处理されるので死に分岐
				;IF TEQUIP:下半身着衣状況 != 0
				;ENDIF
				IF PALAM:潤滑 >= PALAMLV:7
					PRINT  简直是洪水泛滥❤
				ELSEIF PALAM:潤滑 >= PALAMLV:6
					PRINT  腿上流的全是水
