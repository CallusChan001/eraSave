CALL PRINTBUTTON_EX(TFLAG:LOOK非表示 ? "▽[＋][Look]----------" # "▼[－][Look]----------", 3003, TFLAG:LOOK非表示)
PRINTBUTTON "[画像表示]", 816
SIF FLAG:画像表示設定
	SETCOLOR C_ORANGE
PRINTBUTTON "[表示设定]", 3100
RESETCOLOR
IF CFLAG:诶嘿嘿
	IF FLAG:挿絵表示
		PRINTBUTTON "[动画显示-ON-]", 819
	ELSE
		SETCOLOR C_L_GRAY
		PRINTBUTTON "[动画显示-OFF-]", 819
		RESETCOLOR
	ENDIF
ENDIF
PRINTL 

SIF TALENT:MASTER:154 && TALENT:154 != TARGET
	CALL MESSAGE_TALK(14)

IF !TFLAG:LOOK非表示
	;TARGETが大勢居るときはヒマを持て余す
	CALL ANOTHERTARGET_TALK
	CALL 情景描写
ENDIF
IF CFLAG:MASTER:現在位置 == CFLAG:MASTER:初期位置 && 在家召唤告白女神 == 1
	SETCOLOR C_PINK
	PRINTBUTTON "房间里放置着告白之神的信物", 11501
	PRINTL
	RESETCOLOR
ENDIF
IF CFLAG:MASTER:現在位置 == CFLAG:MASTER:初期位置 && 摄影系统开关 && !射影机
	PRINTBUTTON "一个表面有一层薄灰的木盒子静静地摆在桌子上", 11502
	PRINTL
ENDIF
IF TARGET
	IF !TFLAG:LOOK非表示
		PRINT 【上半身】
		IF TEQUIP:1 == 0
			PRINT 赤裸的 
			SIF PALAM:欲情 >= PALAMLV:3
				PRINTFORM %NIPPLE_DESCRIPTION(TARGET)%勃起着
			SIF 湿身度:TARGET > 10 && !TEQUIP:TARGET:SWIMMING
				PRINTFORM 上面满是雨水
		ELSE
			;外側の服から表示
			FOR 装备部位, 17, 6, -1
				SIF GROUPMATCH(装备部位, 15, 12, 11)
					CONTINUE
				表示内容 '= LOOK_AT_CLOTHNAME(TARGET, 装备部位)
				SIF !STRLENS(表示内容)
					CONTINUE
				PRINTFORM %表示内容% 
				IF !TEQUIP:TARGET:SWIMMING
					SELECTCASE 湿身度:TARGET
					CASE 10 TO 29
						PRINTFORM 雨水浸染下有什么东西露了出来 
					CASE 30 TO 80
						PRINTFORM 打湿的衣物间
						IF EQUIP:TARGET:7
							PRINTFORM %BRANAME(EQUIP:TARGET:7, TARGET)%
						ELSEIF EQUIP:TARGET:8
							PRINTFORM %BRANAME(EQUIP:TARGET:8, TARGET)%
						ELSE
							PRINTFORM %OPPAI_DESCRIPTION(TARGET,1)%
						ENDIF
						PRINTFORM 若隐若现 
					CASE 81 TO 100
						PRINTFORM %OPPAI_DESCRIPTION(TARGET,1)%的轮廓已经遮掩不住了 
					ENDSELECT			
				ENDIF
			BREAK
			NEXT
			IF TEQUIP:上半身裸露状態 > 0
				IF TALENT:胸圍 == 2
					PRINTFORM %OPPAI_DESCRIPTION(TARGET,1)%呼之欲出
					;PRINTFORM はち切れんばかりの胸がこぼれ出ている
						SIF 湿身度:TARGET > 10 && !TEQUIP:TARGET:SWIMMING
						PRINTFORM 雨水刚落在乳根处便滑进了深邃的乳沟里 
				ELSEIF TALENT:胸圍 == 1
					PRINTFORM %OPPAI_DESCRIPTION(TARGET,1)%快要蹦出来了
					;PRINTFORM 丰满的胸がこぼれ出ている
					SIF 湿身度:TARGET > 10 && !TEQUIP:TARGET:SWIMMING
						PRINTFORM 乳肉上的点点雨水衬得整对玉乳愈发鲜嫩可口 
				ELSEIF TALENT:胸圍 == 0
					PRINTFORM %OPPAI_DESCRIPTION(TARGET,1)%快要露出来了
					;PRINTFORM 外形良好的胸が露わになっている
					SIF 湿身度:TARGET > 10 && !TEQUIP:TARGET:SWIMMING
						PRINTFORM 位置恰到好处的水滴和姣好双乳相得益彰 
				ELSEIF TALENT:胸圍 == -1
					PRINTFORM %OPPAI_DESCRIPTION(TARGET,1)%若隐若现
					;PRINTFORM 小巧的胸が露わになっている
					SIF 湿身度:TARGET > 10 && !TEQUIP:TARGET:SWIMMING
						PRINTFORM 雨水留下的道道痕迹似能勾勒出乳房的轮廓 
				ELSEIF TALENT:胸圍 == -2
					PRINTFORM %OPPAI_DESCRIPTION(TARGET,1)%若隐若现
					;PRINTFORM 可爱的乳头が露わになっている
					SIF 湿身度:TARGET > 10 && !TEQUIP:TARGET:SWIMMING
						PRINTFORM 雨水丝滑地沿着浅显的轮廓滑了下去 
				ENDIF
				SIF EQUIP:TARGET:ボディースーツ == 0 && PALAM:欲情 >= PALAMLV:5
					PRINTFORM  %NIPPLE_DESCRIPTION(TARGET)%勃起着
			ELSE
				SIF EQUIP:TARGET:ボディースーツ == 0 && PALAM:欲情 >= PALAMLV:MIN((4 + 2 * (GET_INT(TARGET, "上半身内衣_はだけ不可", EQUIP:TARGET:上半身内衣１, "布料厚度"))),15)
					PRINTFORM 即使隔着衣服也能看出%NIPPLE_DESCRIPTION(TARGET)%已经勃起了
			ENDIF
		ENDIF
		IF TEQUIP:乳頭夾 && TEQUIP:揉胸
			PRINTFORM 乳头夹吸附在%NIPPLE_DESCRIPTION(TARGET)%上%OPPAI_DESCRIPTION(TARGET)%也被%CALLNAME:MASTER%揉搓着
		ELSEIF TEQUIP:吸乳头 && TEQUIP:揉胸
			PRINTFORM %CALLNAME:MASTER%一边揉着%OPPAI_DESCRIPTION(TARGET)%一边\@ ABL:MASTER:舌 > 2? 用舌头舔弄# 吸吮\@%NIPPLE_DESCRIPTION(TARGET)%
		ELSEIF TEQUIP:乳頭夾
			PRINTFORM 乳头夹吸附在%NIPPLE_DESCRIPTION(TARGET)%上
		ELSEIF TEQUIP:搾乳機
			PRINT 搾乳机正在搾乳母乳
		ELSEIF TEQUIP:吸乳头
			PRINTFORM %CALLNAME:MASTER%\@ ABL:MASTER:舌 > 2 ? 舔舐着 # 吮吸着 \@%NIPPLE_DESCRIPTION(TARGET)%
		ELSEIF TEQUIP:揉胸
			PRINTFORM %CALLNAME:MASTER%揉搓着%OPPAI_DESCRIPTION(TARGET)%
		ENDIF
		SIF CFLAG:口内精液 == 1
			PRINT 精液垂挂在嘴角上
		SIF CFLAG:脸上精液 == 1
			PRINT 精液浇在了脸上
		SIF CFLAG:手上精液 == 1
			PRINT 手上沾满了精液
		PRINTL
		PRINT 【下半身】
		;TEQUIP:下半身着衣状況 下半身着衣状况(位0=裙子 1=可拉扯 2=不可拉扯 3=不可拉扯的下半身外衣)
		;	(不希望使用位的人用 0=无 1=无内裤裙子 2=只有内裤 3=裙子+内裤 
		;	4=紧身裤(内衣)或无内裤裤子 5=裙子+紧身裤 6=裤子+内裤 7=全装备(此变体无法实现)）
		;CFLAG:パンツ 内裤标志
		;0 穿着内裤
		;1 没穿内裤

		;没穿下半身衣物，或者没穿内裤
		IF TEQUIP:下半身着衣状況 == 0 || CFLAG:没穿内裤 > 0
