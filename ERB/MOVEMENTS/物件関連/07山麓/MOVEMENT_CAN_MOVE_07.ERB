;-----------------------------------------------------------
;地点の接続設定
;ARG    基準の地点
;ARG:1  対象の地点
;RETURNF 0=移動できない,見えない
;        1=移動できる,見えない
;        2=移動できない,見える
;        3=移動できる,見える（直通）
;-----------------------------------------------------------
@CAN_MOVE_7(ARG, ARG:1)
#LOCALSIZE 1
#LOCALSSIZE 1
#FUNCTION
SELECTCASE ARG
CASE P701樹海道入口
	SIF GROUPMATCH(ARG:1, P734小屋)
		RETURNF 1
	SIF ARG:1 == MAXROOM
		RETURNF 2
	SIF GROUPMATCH(ARG:1, P702山道・通往沼澤之道)
		RETURNF 3
CASE P702山道・通往沼澤之道
	SIF GROUPMATCH(ARG:1, P715秋姉妹的家・畑, P716秋姉妹的家・土間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P701樹海道入口, P703玄武之澤)
		RETURNF 3
CASE P703玄武之澤
	SIF GROUPMATCH(ARG:1, P704河童の宣傳站, P709莉莉們的家, P735游泳場, P738森の深奥)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P702山道・通往沼澤之道, P705山道・中腹)
		RETURNF 3
CASE P704河童の宣傳站, P709莉莉們的家, P735游泳場
	SIF GROUPMATCH(ARG:1, P703玄武之澤)
		RETURNF 1
CASE P705山道・中腹
	SIF GROUPMATCH(ARG:1, P703玄武之澤, P707山道・分岐点)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P733正邪的隠匿処)
		RETURNF 1
CASE P707山道・分岐点
	SIF GROUPMATCH(ARG:1, P706厄神的家, P708大蛤蟆之池, P710猫之里, P711休憩所, P731帚木, P736圣域入口)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P705山道・中腹)
		RETURNF 3
CASE P711休憩所
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P712間欠泉地下中樞, P732通向山頂的路, P736圣域入口)
		RETURNF 1
CASE P732通向山頂的路
	SIF GROUPMATCH(ARG:1, P711休憩所, P712間欠泉地下中樞)
		RETURNF 1
CASE P734小屋
	SIF GROUPMATCH(ARG:1, P701樹海道入口)
		RETURNF 1
CASE P733正邪的隠匿処
	SIF GROUPMATCH(ARG:1, P705山道・中腹)
		RETURNF 1
CASE P708大蛤蟆之池
	SIF GROUPMATCH(ARG:1, P707山道・分岐点)
		RETURNF 1
CASE P706厄神的家
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P710猫之里)
		RETURNF 1
CASE P710猫之里
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P706厄神的家, P737招财猫的家)
		RETURNF 1
CASE P737招财猫的家
	SIF GROUPMATCH(ARG:1, P710猫之里)
		RETURNF 1
CASE P712間欠泉地下中樞
	SIF GROUPMATCH(ARG:1, P711休憩所, P732通向山頂的路)
		RETURNF 1
CASE P736圣域入口
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P711休憩所, P741猿猴通路, P743圣域林道)
		RETURNF 1
CASE P716秋姉妹的家・土間
	SIF GROUPMATCH(ARG:1, P702山道・通往沼澤之道, P715秋姉妹的家・畑)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P718秋姉妹的家・居間)
		RETURNF 3
CASE P718秋姉妹的家・居間
	SIF GROUPMATCH(ARG:1, P717秋姉妹的家・寝室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P716秋姉妹的家・土間, P719秋姉妹的家・座敷)
		RETURNF 3
CASE P717秋姉妹的家・寝室
	SIF GROUPMATCH(ARG:1, P718秋姉妹的家・居間, P719秋姉妹的家・座敷)
		RETURNF 1
CASE P719秋姉妹的家・座敷
	SIF GROUPMATCH(ARG:1, P717秋姉妹的家・寝室)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P715秋姉妹的家・畑, P718秋姉妹的家・居間)
		RETURNF 3
CASE P715秋姉妹的家・畑
	SIF GROUPMATCH(ARG:1, P702山道・通往沼澤之道, P716秋姉妹的家・土間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P719秋姉妹的家・座敷)
		RETURNF 3
;仙人的宅邸華扇ちゃん！
CASE P731帚木
	SIF GROUPMATCH(ARG:1, P707山道・分岐点, P730仙人的宅邸・庭)
		RETURNF 1
CASE P730仙人的宅邸・庭
	SIF GROUPMATCH(ARG:1, P731帚木)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P729仙人的宅邸・玄関)
		RETURNF 3
CASE P729仙人的宅邸・玄関
	SIF GROUPMATCH(ARG:1, P720仙人的宅邸・廚房, P721仙人的宅邸・風呂, P722仙人的宅邸・道場, P723仙人的宅邸・二階客間)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P730仙人的宅邸・庭)
		RETURNF 3
CASE P721仙人的宅邸・風呂, P722仙人的宅邸・道場, P720仙人的宅邸・廚房
	SIF GROUPMATCH(ARG:1, P729仙人的宅邸・玄関)
		RETURNF 1
CASE P723仙人的宅邸・二階客間
	SIF GROUPMATCH(ARG:1, P729仙人的宅邸・玄関, P726仙人的宅邸・居間, P724仙人的宅邸・書斎, P725仙人的宅邸・大鷲的房間)
		RETURNF 1
CASE P724仙人的宅邸・書斎
	SIF GROUPMATCH(ARG:1, P723仙人的宅邸・二階客間)
		RETURNF 1
CASE P725仙人的宅邸・大鷲的房間
	SIF GROUPMATCH(ARG:1, P723仙人的宅邸・二階客間)
		RETURNF 1
	;例外的に大鷲的房間から庭にジャンプできる
	SIF GROUPMATCH(ARG:1, P730仙人的宅邸・庭)
		RETURNF 3
CASE P726仙人的宅邸・居間
	SIF GROUPMATCH(ARG:1, P723仙人的宅邸・二階客間, P727仙人的宅邸・華扇私室, P728仙人的宅邸・空的部屋)
		RETURNF 1
CASE P727仙人的宅邸・華扇私室, P728仙人的宅邸・空的部屋
	SIF GROUPMATCH(ARG:1, P726仙人的宅邸・居間)
		RETURNF 1
;深奥エリア
CASE P738森の深奥
	SIF GROUPMATCH(ARG:1, P703玄武之澤, P740山童の宣傳站)
		RETURNF 1
	SIF GROUPMATCH(ARG:1, P739秘天崖)
		RETURNF 3
CASE P739秘天崖
	SIF GROUPMATCH(ARG:1, P738森の深奥)
		RETURNF 3
CASE P740山童の宣傳站
	SIF GROUPMATCH(ARG:1, P738森の深奥)
		RETURNF 1
CASE P741猿猴通路
	SIF GROUPMATCH(ARG:1, P742美天居所,P736圣域入口)
		RETURNF 1
CASE P742美天居所
	SIF GROUPMATCH(ARG:1, P741猿猴通路)
		RETURNF 1
CASE P743圣域林道
	SIF GROUPMATCH(ARG:1, P736圣域入口,P744合欢宅邸,P745月都秘道,P747圣域深道)
		RETURNF 1
CASE P744合欢宅邸
	SIF GROUPMATCH(ARG:1, P743圣域林道)
		RETURNF 1
CASE P745月都秘道
	SIF GROUPMATCH(ARG:1, P743圣域林道)
		RETURNF 1
CASE P746姥芽大宅
	SIF GROUPMATCH(ARG:1, P747圣域深道)
		RETURNF 1
CASE P747圣域深道
	SIF GROUPMATCH(ARG:1, P743圣域林道,P746姥芽大宅,P748魑魅的草棚)
		RETURNF 1
CASE P748魑魅的草棚
	SIF GROUPMATCH(ARG:1, P747圣域深道,P749山中密道)
		RETURNF 1
CASE P749山中密道
	SIF GROUPMATCH(ARG:1, P747圣域深道,P748魑魅的草棚,P750驯子的小屋,P751浅间净秽山)
		RETURNF 1
CASE P750驯子的小屋
	SIF GROUPMATCH(ARG:1, P749山中密道,P751浅间净秽山)
		RETURNF 1

CASE P751浅间净秽山
	SIF GROUPMATCH(ARG:1, P749山中密道,P750驯子的小屋,P752四季竖穴)
		RETURNF 1
CASE P752四季竖穴
	SIF GROUPMATCH(ARG:1, P751浅间净秽山,P753浅间的房间,P754空房间,P755化石丛林荒地,P761浴室,P762空房间)
		RETURNF 1
CASE P753浅间的房间
	SIF GROUPMATCH(ARG:1, P752四季竖穴)
		RETURNF 1
CASE P754空房间
	SIF GROUPMATCH(ARG:1, P752四季竖穴)
		RETURNF 1
CASE P755化石丛林荒地
	SIF GROUPMATCH(ARG:1, P752四季竖穴,P756月海通路,P757巧克力河,P758化石深道)
		RETURNF 1
CASE P756月海通路
	SIF GROUPMATCH(ARG:1, P755化石丛林荒地)
		RETURNF 1
CASE P757巧克力河
	SIF GROUPMATCH(ARG:1, P755化石丛林荒地)
		RETURNF 1
CASE P758化石深道
	SIF GROUPMATCH(ARG:1, P755化石丛林荒地,P759磐永神社本殿)
		RETURNF 1
CASE P759磐永神社本殿
	SIF GROUPMATCH(ARG:1, P755化石丛林荒地)
		RETURNF 3
	SIF GROUPMATCH(ARG:1, P760磐永神社内屋)
		RETURNF 1
CASE P760磐永神社内屋
	SIF GROUPMATCH(ARG:1, P759磐永神社本殿)
		RETURNF 3
CASE P761浴室
	SIF GROUPMATCH(ARG:1, P752四季竖穴)
		RETURNF 1
CASE P762空房间
	SIF GROUPMATCH(ARG:1, P752四季竖穴)
		RETURNF 1

CASE MAXROOM
	SIF ARG:1 == P701樹海道入口
		RETURNF 1
ENDSELECT

RETURNF 0
