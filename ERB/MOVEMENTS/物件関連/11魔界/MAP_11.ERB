	SETCOLOR C_FOREST1 ;这种上定义代码的直接从DIM.ERH中找
	PRINTS SLICED
	RESETCOLOR
CASE "ｖ"
	SETCOLOR C_L_GREEN
	PRINTS SLICED
	RESETCOLOR
CASE "＃", "╋"
	SETCOLOR 0x9664ff
	PRINTS SLICED
	RESETCOLOR
CASE "桃"
	SETCOLOR C_PINK
	PRINTS SLICED
	RESETCOLOR
CASE "石"
	SETCOLOR C_STONE1
	PRINTS SLICED
	RESETCOLOR
;CASE "■", "l", "＝"
;	SETCOLOR C_R_WOOD
;	PRINTS SLICED
;	RESETCOLOR
CASE "□"
	SELECTCASE TFLAG:地圖切換
	CASE 1, 0
		SETCOLOR C_WINDOW
	CASEELSE
		SETCOLOR C_WOOD1
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "●"
;	SELECTCASE TFLAG:地圖切換
;	CASE 1, 3
		SETCOLOR C_P_PURPLE
;	CASEELSE
;		SETCOLOR C_R_WOOD
;	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "～"
	SETCOLOR C_WINDOW
	PRINTS SLICED
	RESETCOLOR
CASE "│", "─", "╷", "○", "┐", "└", "三", "|"
	SELECTCASE TFLAG:地圖切換
	CASE 3
		SETCOLOR C_P_PURPLE
	CASEELSE
		SETCOLOR C_WOOD1
	ENDSELECT
	PRINTS SLICED
	RESETCOLOR
CASE "┃", "━"
	SETCOLOR C_WOOD2
	PRINTS SLICED
	RESETCOLOR
