;-----------------------------------------
;お出かけMapの設定
;ARG   対象の地点
;ARGS  判定文字典数
;-----------------------------------------
@ODEKAKEMAP_SETTING_11(ARG, ARGS)
#FUNCTION
;＼1 2 3 4 5 6 7 8 9 
;01１２　　　　　　　　　
;02３４５　　　　　　　　　
;03　　　　　　　　　
;04　　　　　　　　
;05　　　　　　
;06　　　　　　　　
;07　　　　　　　　　
;08　　　　　　　　　
;09　　　　　　　　
SELECTCASE ARG
CASE 魔界都市
	SIF ARGS == "座標"
		RETURNF 22
	SIF GROUPMATCH(ARGS, "广阔", "路人", "お弁当")
		RETURNF 1
CASE 万魔殿
	SIF ARGS == "座標"
		RETURNF 11
	SIF GROUPMATCH(ARGS, "屋内")
		RETURNF 1
CASE 堕落神殿
	SIF ARGS == "座標"
		RETURNF 31
	SIF GROUPMATCH(ARGS, "屋内")
		RETURNF 1
CASE 冰雪世界
	SIF ARGS == "座標"
		RETURNF 13
	SIF GROUPMATCH(ARGS, "广阔", "訓練")
		RETURNF 1
CASE 法界
	SIF ARGS == "座標"
		RETURNF 33
	SIF GROUPMATCH(ARGS, "广阔")
		RETURNF 1
ENDSELECT
