;==========================================================================
;NAS and ATW/Neodev use different formatting standards for game strings
;This checks the branch that we're using and if it detects NAS then it uses NAS formatting.
;Any other branch will use Neodev formatting
;DO NOT PUT SPACES BEFORE AND AFTER THE FUNCTION WHEN USING ARG:1
;Ex: Remember to get%TW_PORT_STR("Test2")%or else.

;ARGS: Input string
;ARG: Output formatting
;ARG:1: Add a space on Neodev Formatting (Bits)
	;Bit 0: Before
	;Bit 1: After
;==========================================================================
@TW_PORT_STR(ARGS,ARG,ARG:1=3)
#FUNCTIONS
#DIMS DYNAMIC TWP_STR
VARSET TWP_STR
IF EXISTVAR("NAS_VERSION")
	SELECTCASE ARG
	CASE 0
	TWP_STR '= @"【%ARGS%】"
	CASE 1
	TWP_STR '= @"『%ARGS%』"
	ENDSELECT
ELSE
	SIF GETBIT(ARG:1,0)
		TWP_STR += " "
	SELECTCASE ARG
	CASE 0
	TWP_STR += @"[%ARGS%]"
	CASE 1
	TWP_STR += @"\"%ARGS%\""
	ENDSELECT
	SIF GETBIT(ARG:1,1)
		TWP_STR += " "
ENDIF
RETURNF TWP_STR