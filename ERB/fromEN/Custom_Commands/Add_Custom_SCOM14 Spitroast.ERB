@ADD_CUSTOM_SCOM14_V_ACTOR(ACTOR)
#DIM ACTOR
;奉仕快楽フラグ
TCVAR:ACTOR:108 = 1

CSTR:ACTOR:10 += @"{TARGET}/"
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:ACTOR:2 = 1
TCVAR:ACTOR:童貞喪失 = 1
;強度
TCVAR:ACTOR:4 = 1500

CSTR:TARGET:10 += @"{ACTOR}/"
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:TARGET:2 = 13
;強度
TCVAR:TARGET:4 = 1500

;新設定案
;強度　とりあえず2割
;TCVAR:PLAYER:4 = 300
;ソースは半分強で
;	SOURCE:快Ｖ = 200
;	SOURCE:苦痛 = 130(250)

DOWNBASE:ACTOR:体力 += 40
DOWNBASE:ACTOR:気力 += 40
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 100
	SOURCE:ACTOR:快Ｃ = 300
	SOURCE:快Ｖ += 400
	SOURCE:苦痛 += 250

	;処女だった場合は、苦痛のソースと反発のソースを追加する
	IF TALENT:処女 == 1
		SOURCE:苦痛 += 1000
	ENDIF

	;再生処女だった場合は、苦痛のソースと反発のソースを追加する
	IF TALENT:処女 == 2
		SOURCE:苦痛 += 250
	ENDIF

ELSE

	DOWNBASE:体力 += 50
	DOWNBASE:気力 += 60

	SOURCE:ACTOR:快Ｃ += 300
	SOURCE:快Ｖ = 300
	SOURCE:情愛 = 150
	SOURCE:苦痛 = 500
	SOURCE:露出 = 50
	SIF TFLAG:193
		SOURCE:露出 = 300
	SOURCE:反感 = 300
	
	;正常位補正
	;PLAYERがTARGETの膣に挿入している
	IF BEDROOM(CFLAG:ACTOR:現在位置) && TEQUIP:50 == ACTOR
		TCVAR:ACTOR:4 += EXP:愛情経験 * 5
		SOURCE:快Ｖ += EXP:愛情経験
		SIF SOURCE:快Ｖ >= 700
			SOURCE:快Ｖ = 700
		SIF TCVAR:ACTOR:4 >= 3000
			TCVAR:ACTOR:4 = 3000
	ENDIF

	;上のほうで計算した汚れデータ
	SOURCE:不潔 = Y*10 + 60
	CALL 特殊ソース処理
ENDIF
;Vへの苦痛は先に処理しておく
CALL PAIN_CHECK_V(TARGET)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
CALL STAIN_膣内Ｐ挿入

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:ACTOR:挿入経験 ++
EXP:Ｖ経験 ++
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF FLAG:70 == 1 || CFLAG:イタズラ
	IF HETEROSEX(TARGET,ACTOR)
		EXP:PLAYER:(50 + HETEROSEX(TARGET,ACTOR)) += 4
	ENDIF
	IF CFLAG:ACTOR:イタズラ == 2
		EXP:ACTOR:睡眠姦経験 += 1
		EXP:TARGET:睡眠姦経験 += 1
	ENDIF
	SIF !CFLAG:イタズラ
	EXP:ACTOR:時姦経験 += 1
ELSE
	IF HETEROSEX(TARGET,ACTOR)
		EXP:(50 + HETEROSEX(TARGET,ACTOR)) += 4
		EXP:ACTOR:(50 + HETEROSEX(TARGET,ACTOR)) += 4
	ENDIF
	EXP:Ｖ性交経験 ++
	IF !ASSIPLAY
		EXP:愛情経験 += TALENT:恋慕 * 2
		;処女
		SIF TALENT:処女 == 1 && TALENT:恋慕
			EXP:愛情経験 += 10
	ENDIF
ENDIF
Qol_Info_Count_Array:TARGET:Qol_Count_VSex ++ ;qol custom code
SIF FLAG:70 == 1 || CFLAG:イタズラ
	Qol_Info_Count_Array:TARGET:Qol_Count_UncVSex ++ ;qol custom code

@ADD_CUSTOM_SCOM14_M_ACTOR(ACTOR)
#DIM ACTOR
;奉仕快楽
TCVAR:108 = 1

CSTR:ACTOR:10 += @"{TARGET}/"
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:ACTOR:2 = 4
;強度
TCVAR:ACTOR:4 = 1000

CALL QoL_Info_Write(TARGET, Qol_Dates_Bj) ;qol custom code

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
IF FLAG:70 == 1
	BASE:MASTER:TSP -= 50
	SOURCE:快Ｍ = 50

	SOURCE:ACTOR:快Ｃ = 70
	;性技補正
	SOURCE:ACTOR:快Ｃ += MIN(ABL:舌 * 15,100)

	;調教者のTALENT:猫舌をみる
	IF TALENT:猫舌
		TIMES SOURCE:ACTOR:快Ｃ , 0.75
		;調教者が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
		EXP:嗜虐快楽経験 += 1
	ENDIF

ELSE
	DOWNBASE:体力 += 10
	DOWNBASE:気力 += 80
	SOURCE:ACTOR:快Ｃ = 70
	SOURCE:快Ｍ = 50
	SOURCE:情愛 = 100
	SOURCE:性行動 = 300
	SOURCE:達成 = 150
	SOURCE:屈従 = 700
	SOURCE:反感 = 400
	SOURCE:逸脱 = 100

	;上のほうで計算した汚れデータ
	SOURCE:不潔 = Y*10 + 60

	;-------------------------------------------------
	;特殊ソース処理
	;-------------------------------------------------
	;抑圧、抵抗持ちには、抑鬱ソース追加
	SIF TALENT:自己愛 < 0 || TALENT:抵抗
		SOURCE:鬱屈 += 400

	;性技補正
	IF ABL:舌 >= 4
		SOURCE:ACTOR:快Ｃ += 60
	ELSE
		SOURCE:ACTOR:快Ｃ += ABL:舌 * 25
	ENDIF

	;調教者のTALENT:舌使いをみる
	SIF TALENT:舌使い
		TIMES SOURCE:ACTOR:快Ｃ , 1.50

	;調教者のTALENT:猫舌をみる
	SIF TALENT:猫舌
		TIMES SOURCE:ACTOR:快Ｃ , 0.75

ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;奴隷の指⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:ACTOR:2
STAIN:ACTOR:2 |= STAIN:0

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;フェラ経験
;とりあえずM経験追加
;SOURCEに快Ｍがあると自動で経験追加されるので、実質、経験＋２扱いは変ではないかと
;EXP:Ｍ経験 += 1

;同性愛　レズなら戻り値1ゲイなら戻り値2
SIF HETEROSEX(TARGET,ACTOR)
	EXP:ACTOR:(50 + HETEROSEX(TARGET,ACTOR)) += 7

IF !FLAG:70 && !CFLAG:イタズラ
	EXP:口淫経験 += 3
	;奴隷が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
	SIF TALENT:TARGET:猫舌
		EXP:嗜虐快楽経験 += 1
	;同性愛　レズなら戻り値1ゲイなら戻り値2
	SIF HETEROSEX(TARGET,ACTOR)
		EXP:ACTOR:(50 + HETEROSEX(TARGET,ACTOR)) += 7
	IF HETEROSEX(TARGET,ACTOR)
		EXP:(50 + HETEROSEX(TARGET,ACTOR)) += 7
		EXP:ACTOR:(50 + HETEROSEX(TARGET,ACTOR)) += 7
	ENDIF
	;愛情経験
	SIF TALENT:恋慕 && ASSIPLAY == 0
		EXP:愛情経験 += 1
ELSE
	SIF !CFLAG:イタズラ
		EXP:ACTOR:時姦経験 += 1
ENDIF
IF CFLAG:ACTOR:イタズラ == 2
	EXP:ACTOR:睡眠姦経験 += 1
	EXP:TARGET:睡眠姦経験 += 1
ENDIF

;精液汚れ
SIF TCVAR:ACTOR:112 == 1 || TCVAR:ACTOR:112 == 2
	EXP:精飲経験 ++
TCVAR:TARGET:ＦＫ喪失 = 2
Qol_Info_Count_Array:TARGET:Qol_Count_GiveMSex += EXP_UP(GETNUM(EXP, "口淫経験"), TARGET) ;qol custom code

