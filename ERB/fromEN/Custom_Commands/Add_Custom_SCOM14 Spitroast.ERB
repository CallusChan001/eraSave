@ADD_CUSTOM_SCOM_ABLE14
#DIM M_ACTOR
#DIM V_ACTOR
;交互挿入
SIF !TFLAG:100
	RETURN 0
;not during time stop
SIF FLAG:70
	RETURN 0
;一括管理
SIF !TARGET
	RETURN 0
SIF TFLAG:102 != 2
	RETURN 0

M_ACTOR = ADD_CHARA_WHO_TOUCHING(TARGET, SET_M, SET_P, 1)
V_ACTOR = ADD_CHARA_WHO_TOUCHING(TARGET, SET_V, SET_P, 1)

;check intended position for target (SELECTCOM should be set at this point)
IF GROUPMATCH(SELECTCOM, 60, 61, 65, 67, 68)
	V_ACTOR = PLAYER
ELSEIF SELECTCOM == 81
	M_ACTOR = PLAYER
ENDIF

SIF V_ACTOR < 0 || M_ACTOR < 0
	RETURN 0

;フェラチオ実行判定
SIF !TFLAG:100
	RETURN 0
SIF !HAS_PENIS(M_ACTOR) || !HAS_PENIS(V_ACTOR)
	RETURN 0
;ボールギャグ使用中は不可
SIF TEQUIP:ボールギャグ
	RETURN 0



;バイブ挿入中はダメ
SIF TEQUIP:バイブ
	RETURN 0
;オトコだとダメ
SIF !HAS_VAGINA(TARGET)
	RETURN 0
;毒人形だと性器がついてないのでダメ
SIF TALENT:196 == 99
	RETURN 0
;体格差がある場合は経験がないとダメ
SIF Com_Size_Check("strict") ;com custom code
	RETURN 0
;さらに体格差があると禁断の知識が必要
SIF Com_Size_Check("reg") ;com custom code
	RETURN 0
SIF 挿入不可(TARGET) == 3
	RETURN 0
;調教者がオトコ、半人半妖、ふたなり、もしくはペニスバンドがないとダメ
SIF !(TALENT:PLAYER:2 & 2) && ITEM:PBAND == 0
	RETURN 0

RETURN 1

