@Add_Custom_SCOM_Name(ARG)
#FUNCTIONS
#DIM SHORT
SELECTCASE ARG
;24
	CASE 0
		RETURNF "轮流插入"
	CASE 1
		RETURNF "轮流插入菊穴"
	CASE 2
		RETURNF "A乱牡丹"
	CASE 3
		RETURNF "对镜掰开小穴"
	CASE 5
		RETURNF "双女上位"
	CASE 6
		RETURNF "指交两人"
	CASE 7
		LOCAL = ADD_LAST_V_POSE()
		IF LOCAL
			RETURNF @"%ADD_SEX_MODULE_POSE(LOCAL)%同时指交"
		ENDIF
		RETURNF "V插入同时指交"
	CASE 8
		LOCAL = ADD_LAST_V_POSE()
		SIF LOCAL
			RETURNF @"%ADD_SEX_MODULE_POSE(LOCAL)%并指交两人"
		RETURNF "V插入并指交两人"
	CASE 9
		RETURNF "三人乳交"
	CASE 10
		RETURNF "三人口交"
	CASE 11
		RETURNF "双女上位同时指交"
	CASE 12
		RETURNF "双人手交"
	CASE 13
		RETURNF "双手手交"
	CASE 14
		RETURNF "同时口爆侵犯"
	CASE 15
		RETURNF "体外射精"
	CASE 16
		RETURNF "三人叠塔"
	CASE 17
		RETURNF "三穴插入"
	CASE 18, 19
		RETURNF "分身群交"
	CASE 20
		RETURNF "双人腹交"
	CASEELSE
		RETURNF "Custom SCOM"
ENDSELECT
