	RETURN 0

FOR LOCAL, 0, 4
	LOCAL:1 = ADD_CHARA_WHO_TOUCHING(TARGET, SET_A, SET_P, LOCAL)

	IF LOCAL:1 >= 0
		ADD_SEX_MODULE_ACTOR:1 = LOCAL:1
		BREAK
	ENDIF
NEXT
SIF LOCAL:1 < 0
	RETURN 0

FOR LOCAL, 0, 4
	LOCAL:1 = ADD_CHARA_WHO_TOUCHING(TARGET, SET_M, SET_P, LOCAL)

	IF LOCAL:1 >= 0
		ADD_SEX_MODULE_ACTOR:2 = LOCAL:1
		BREAK
	ENDIF
NEXT
SIF LOCAL:1 < 0
	RETURN 0

FOR LOCAL, 0, 4
	LOCAL:1 = ADD_CHARA_WHO_TOUCHING(TARGET, SET_YUBI, SET_P, LOCAL)

	IF LOCAL:1 >= 0
		ADD_SEX_MODULE_ACTOR:3 = LOCAL:1
