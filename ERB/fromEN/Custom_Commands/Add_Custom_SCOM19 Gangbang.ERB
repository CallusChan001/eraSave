	LOCAL:1 = ADD_CHARA_WHO_TOUCHING(TARGET, SET_YUBI, SET_P, LOCAL)

	IF LOCAL:1 >= 0 && ADD_SEX_MODULE_ACTOR:3 != LOCAL:1
		ADD_SEX_MODULE_ACTOR:4 = LOCAL:1
		BREAK
	ENDIF
NEXT
SIF LOCAL:1 < 0
	RETURN 0

