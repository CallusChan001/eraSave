	LOCAL:1 = ADD_CHARA_WHO_TOUCHING(TARGET, SET_V, SET_P, LOCAL)

	IF LOCAL:1 >= 0
		ADD_SEX_MODULE_ACTOR:0 = LOCAL:1
		BREAK
	ENDIF
NEXT
