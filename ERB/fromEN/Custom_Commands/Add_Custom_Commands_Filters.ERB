@QUICKFILTER_CUSTOM(ID_COM)
#FUNCTION
#DIM ID_COM
#DIM CONST BIT_Ｖ系       = 1
#DIM CONST BIT_Ａ系       = 2
#DIM CONST BIT_奉仕系     = 5
#DIM CONST BIT_SM系       = 6
#DIM CONST BIT_セクハラ系 = 7
#DIM CONST BIT_家事系     = 9
#DIM CONST BIT_自慰系     = 13  ;たぶんつかってない
SIF !QUICKFILTER(ID_COM)
	RETURNF 0
SIF IS_CUSTOM_COM_VAGINA(ID_COM)   && !CFLAG:TARGET:Ｖアンフィルタ
	RETURNF 0
SIF IS_CUSTOM_COM_ANAL(ID_COM)     && !CFLAG:TARGET:Ａアンフィルタ
	RETURNF 0
SIF IS_CUSTOM_COM_HOUSI(ID_COM)    && !CFLAG:TARGET:奉仕アンフィルタ
	RETURNF 0
SIF IS_CUSTOM_COM_SM(ID_COM)       && !CFLAG:TARGET:ＳＭアンフィルタ
	RETURNF 0
SIF IS_CUSTOM_COM_KAJI(ID_COM)     && !FLAG:家事アンフィルタ
	RETURNF 0
SIF IS_CUSTOM_COM_SEKUHARA(ID_COM) && !CFLAG:TARGET:セクハラアンフィルタ && (!FLAG:70 || CFLAG:TARGET:344 == 1)
	RETURNF 0
SIF IS_CUSTOM_COM_REVSERVICE(ID_COM)  && !CFLAG:TARGET:RevServiceFilter
	RETURNF 0
SIF IS_CUSTOM_COM_REVSEX(ID_COM)  && !CFLAG:TARGET:RevSexFilter
	RETURNF 0
;NAS customs
SIF IS_CUSTOM_COM_URETHRA(ID_COM)  && !CFLAG:TARGET:UFilter
	RETURNF 0	
SIF IS_CUSTOM_COM_ES(ID_COM)		&& !CFLAG:TARGET:EStimFilter
	RETURNF 0
SIF IS_CUSTOM_COM_ESPER(ID_COM)	&& !ExtraFilters:TARGET:0
	RETURNF 0
SIF IS_CUSTOM_COM_HYPNOSIS(ID_COM)	&& !ExtraFilters:TARGET:1
	RETURNF 0		
SIF IS_CUSTOM_COM_HOSTILE(ID_COM)	&& !ExtraFilters:TARGET:2
	RETURNF 0	
;===========================
RETURNF 1

