;Cum Outside
@ADD_CUSTOM_SCOM15

TCVAR:116 = PLAYER
;奉仕快楽フラグ
TCVAR:PLAYER:108 = 1

CSTR:PLAYER:10 += @"{TARGET}/"
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手 12=Ａ責め 13=Ｖ責め）
TCVAR:PLAYER:2 = 8
TCVAR:PLAYER:童貞喪失 = 1
;強度
TCVAR:PLAYER:4 = 300

CSTR:TARGET:10 += @"{PLAYER}/"
;強度
TCVAR:TARGET:4 = 300

DOWNBASE:PLAYER:体力 += 30
DOWNBASE:PLAYER:気力 += 30

RETURN 1

@ADD_CUSTOM_SCOM_ABLE15
;交互挿入
SIF !TFLAG:100
	RETURN 0
SIF !TARGET
    RETURN 0
SIF CFLAG:うふふ == 0
    RETURN 0
SIF ADD_CHARA_WHO_TOUCHING(TARGET, SET_V, SET_P, 1) != PLAYER && ADD_CHARA_WHO_TOUCHING(TARGET, SET_A, SET_P, 1) != PLAYER
    RETURN 0
SIF !TCVAR:PLAYER:イきそう
	RETURN 0

RETURN 1

@ADD_CUSTOM_MESSAGE_SCOM15
PRINTFORMDL %CALLNAME:MASTER%在射精前拔出%TINKO(PLAYER,0)%，将火热的精液射在%CALLNAME:TARGET%身上。
