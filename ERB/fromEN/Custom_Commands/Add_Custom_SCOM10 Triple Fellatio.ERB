@ADD_CUSTOM_SCOM_SET10
#DIM DYNAMIC CHARAS, 3

CHARAS:0 = ADD_MASTER_POSE_TR(PLAYER, SET_M,SET_P)

SIF CHARAS:0 <= 0
	RETURN 0

FOR LOCAL, 1, 3
	LOCAL:1 = ADD_MASTER_POSE_TR(PLAYER, SET_M,SET_P,LOCAL)

	SIF !LOCAL:1
		CONTINUE
	SIF FINDELEMENT(CHARAS, LOCAL:1) >= 0
		CONTINUE
		
	FOR LOCAL:2, 1, 3
		SIF CHARAS:(LOCAL:2)
			CONTINUE
		CHARAS:(LOCAL:2) = LOCAL:1
		BREAK
	NEXT
NEXT

ADD_SEX_MODULE_ACTOR:0 = PLAYER
FOR LOCAL, 0, 3
	SIF CHARAS:LOCAL <= 0
		RETURN 0
	ADD_SEX_MODULE_CHARA:LOCAL = CHARAS:LOCAL
NEXT

RETURN 1

