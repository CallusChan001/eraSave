	CALL IMG_COLUMN,@"神签_{ARG}",LOCALS,,神签TOID:ARG,400
ELSE
	SPLIT LOCALS,"/",MESSAGE_PRINT
	FOR LOCAL,0,RESULT
		SIF MESSAGE_PRINT:LOCAL != ""
			PRINTFORML %MESSAGE_PRINT:LOCAL%
	NEXT
ENDIF
RETURN 神签GID

