@CKMax(ARG)
#FUNCTION
#LOCALSIZE 4
VARSET LOCAL
FOR LOCAL, 900, 936
	SIF !TFLAG:LOCAL || TFLAG:LOCAL<=(ARG<800?Drus:ARG*100#0)
		CONTINUE
	LOCAL:1=TFLAG:LOCAL-(ARG<800?Drus:ARG*100#0)
	LOCAL:2=LOCAL:1*BASE:mCID:(LOCAL-850)/100
	IF !BASE:mCID:诊断完成
		SIF LOCAL:3<LOCAL:1
			LOCAL:3=LOCAL:1	
	ELSEIF LOCAL:2<BASE:mCID:疾病防御
		CONTINUE
	ELSEIF LOCAL:3<LOCAL:1
		LOCAL:3=LOCAL:1
		SIF Drus:2
			LOCAL:3+=mCID:1/100
	ENDIF
NEXT
RETURNF LOCAL:3
