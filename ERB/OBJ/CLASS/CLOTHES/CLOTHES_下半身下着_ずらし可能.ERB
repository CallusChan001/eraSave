			CASE 14
				SIF (TALENT:ARG:恋慕 > 0)
					RETURNF 1
		ENDSELECT
	ELSEIF DAY:2 == 4
		SELECTCASE DAY:3
			CASE 14, 24, 25
				SIF (TALENT:ARG:恋慕 > 0)
					RETURNF 1
		ENDSELECT
	ENDIF
ENDIF
