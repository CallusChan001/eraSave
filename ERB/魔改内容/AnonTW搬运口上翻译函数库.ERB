			LOCALS = %SPLIT_G("嗯:嗯:嗯:啊嗯:唔:呃唔:哈:哈啊:啊:啊:啊:啊:啊呜:唏:呼哇:嗯哼:嗯:哈:啊:嗯:啊嗯:哈呀:啊啊:哦哦:诶:啊嗯:呀:哦")%
		CASE "mild", "m"
			LOCALS = %SPLIT_G("哼:哦嗯:哈嗯:嗯嗯:嗯嗯:啊嗯:嗯嗯:哈啊:哈啊:啊啊:啊:啊嗯:嗯嗯:嗯:呀:嗯啊:哈哈嘿:哈咦:啊嘿:呢呀:呃哇:唏呀:嗯:嗯呀:呀啊:嗯呼呼")%
		CASE "heavy", "h"
			SELECTCASE RAND:13
				CASE 0
					;aunh
					LOCALS = %"啊"+"啊"*RAND(1,5)+"呜"*RAND:3+"嗯"*RAND:4+"啊"*RAND(1,5)%
				CASE 1
					;oohn
					LOCALS = %"齁"+"哦"*RAND(1,5)+"啊"*RAND(1,4)+"嗯"*RAND:4%
				CASE 2
					;uuongh
					LOCALS = %"呜"+"呜"*RAND(1,5)+"哦"*RAND:3+"嗯"*RAND:4+"嘎"*RAND:2+"啊"*RAND(1,4)%
				CASE 3
					;哈啊/嗯呀
					LOCALS = %"哈"+"嗯"*RAND:5+SPLIT_G("哈"*RAND(3,7)+"啊"*RAND:3 + ":" + "呀"*RAND(1,4)+"啊"*RAND(3,7) )%
				CASE 4
					;嗯啊
					LOCALS = %"嗯"+"嗯"*RAND(2,6)+"嘎"*RAND:3+"啊"*RAND:3+""*RAND(1,3)%
				CASE 5
					;(n) + 呀/hiyahn/hahn
					LOCALS = %SPLIT_G("嗯:哈") + SPLIT_G("咿:呀:") + "啊"*RAND(3,7)+ "嗯"*RAND:4%
				CASE 6
					;mmnh
					LOCALS = %"嗯"+"嗯"*RAND(2,6)+"嗯"*RAND:4+"啊"*RAND:3%
				CASE 7
					;啊嗯
					LOCALS = %"啊"+"啊"*RAND(2,5)+"嗯"*RAND:3%
				CASE 8
					;mmph
					LOCALS = %"嗯"+"嗯"*RAND(2,6)+"噗"*RAND:3+"啊"*RAND(1,5)%
				CASE 9
					;哼(i/u)
					LOCALS = %"哈"+"嗯"*RAND:5+"嘎"*RAND(1,3)+"啊"*RAND:4 + SPLIT_G("呜:咿")*RAND(1,6)%
				CASE 10
					;h/ah(ii/ee)
					LOCALS = %SPLIT_G("哈:啊") + "啊"*RAND:4+"哈啊"*RAND(1,3) + SPLIT_G("呃:咿") *RAND(2,4)%
				CASE 11
					;呢呀/eyahn
					LOCALS = %SPLIT_G("呢呀:诶呀") + "啊"*RAND(2,8)+ "嗯"*RAND(1,4)%
				CASE 12
					;ahyaa/ahyiii
					LOCALS = %SPLIT_G("啊咿") + SPLIT_G("啊:咿")*RAND(3,7)%
			ENDSELECT
	CASEELSE
		RETURNF "Error at MOAN function! Report this!"
	ENDSELECT
	IF nNameCount || nHeartCount
		strOut += LOCALS + (--ARG:1 > 0 ? RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag) + " " # "")
		LOCAL = nInitCount
	ELSE
		IF SPLIT_CHECK(ARGS:1, "dot")
			LOCALS:1 '=  "…"
			LOCAL:1 = 1
		ELSEIF SPLIT_CHECK(ARGS:1, "!")
			LOCALS:1 '=  "!!! "
			LOCAL:1 = 1
		ELSEIF SPLIT_CHECK(ARGS:1, "dot!")
			LOCALS:1 '=  "…!"
			LOCAL:1 = 1
		ELSEIF SPLIT_CHECK(ARGS:1, "rand")
			IF RAND:2
				LOCALS:1 '= SPLIT_G("…:!!!:…!") + ""
				LOCAL:1 = 0 ;reset cap status
				SIF LOCAL:2 ;if non-comma was used, set cap
					LOCAL:1 = 1
				LOCAL:2 = 1 ;note that non-comma mode was used previously
			ELSE
				LOCALS:1 '= ","
				IF LOCAL:2 ;non-comma used, set cap; otherwise lower case
					LOCAL:1 = 1
				ELSE
					LOCAL:1 = 0
				ENDIF
				LOCAL:2 = 0 ;comma was used
			ENDIF
		ELSE
			LOCALS:1 '= ","
		ENDIF
		strOut += (nInitCount || LOCAL:1 ? LOCALS # TOLOWER(LOCALS)) + (--ARG:1 > 0 ? LOCALS:1 # "")
		IF nInitCount
			LOCAL = nInitCount
			nInitCount = 0
		ENDIF
	ENDIF
LOOP ARG:1 > 0

SIF (LOCAL || LOCAL:1) && ARG == 1 ;ignore low capitalization rule
	RETURNF UNCAPITALIZE(strOut)
RETURNF CAP_PROCESS(strOut, ARG)

