@RANDLIPS_CN(ARG = -1, ARGS)
#FUNCTIONS
#DIM DYNAMIC nTemp
#DIMS DYNAMIC strTemp, 3

#DIM DYNAMIC nNameCount
#DIM DYNAMIC nHeartCount

#DIMS DYNAMIC strType
#DIMS DYNAMIC strFlag

strType '= SPLIT_SINGLE(TOLOWER(ARGS:0))

SIF SPLIT_CHECK(ARGS, "name")
	nNameCount = RAND(1, 4)
SIF SPLIT_CHECK(ARGS, "heart")
	nHeartCount = RAND(1, 4)
strFlag '= SPLIT_SINGLE(MATCH_SPLIT(ARGS, "charname_"), 1, "_")
SIF SPLIT_CHECK(ARGS, "const")
	strFlag += ":const"
SIF ARG == -1
	ARG = RAND:4
SELECTCASE ARG
	CASE 0
		strTemp += @"%FSYN_CN("唇部吞咽音")%%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% "
		FOR LOCAL, 0, 2
			strTemp += @"%FSYN_CN("嘴唇闷音")%%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% "
		NEXT
		RETURNF strTemp + RANDKISS_CN(strType)
	CASE 1
		FOR LOCAL, 0, 2
			strTemp += @"%RANDKISS_CN(strType)%%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% "
			SIF PERCENT(30)
				strTemp += @"%FSYN_CN("嘴唇闷音")%%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% "
		NEXT
		RETURNF strTemp + @"%RANDKISS_CN(strType)%%COND_STR(@"%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% %FSYN_CN("嘴唇闷音")%", PERCENT(30))%"
	CASE 2
		DO
			IF STRLENS(strType)
				strTemp:1 '= TOLOWER(strType) == "suck" ? "啾" # "亲"
			ELSE
				strTemp:1 '= SPLIT_G("亲亲:啾")
			ENDIF
			strTemp += SPLIT_G(SLUR(strTemp:1, -2) + ":" + strTemp:1 + ":" + SLUR(SLUR(strTemp:1, -2)))
			strTemp:2 '= RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag) + " "
			strTemp += strTemp:2
		LOOP INRANGE(nTemp++, 0, 6) && (nTemp >= 3 ? RAND:3 # 1)
		RETURNF REPLACE(strTemp, strTemp:2 + "$", "")
	CASE 3
		strTemp += @"%FSYN_CN("唇部吞咽音")%%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% "
		FOR LOCAL, 0, 2
			strTemp += @"%RANDKISS_CN(strType)%%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% "
			SIF PERCENT(30)
				strTemp += @"%FSYN_CN("嘴唇闷音")%%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% "
		NEXT
		RETURNF strTemp + @"%RANDKISS_CN(strType)%%COND_STR(@"%RANDLIPS_NAMEHEART_INSERT(nNameCount, nHeartCount, strFlag)% %FSYN_CN("嘴唇闷音")%", PERCENT(30))%"
ENDSELECT

