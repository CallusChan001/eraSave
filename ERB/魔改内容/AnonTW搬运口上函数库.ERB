;PrintType: 1 = PRINTFORMW, others = PRINTFORML
;ColorType: 1 = Current font colour, others = Default white
;Basically "1, 1" = PRINTFORMW, "1, 0" = PRINTFORMDW, "0, 1" = PRINTFORML, "0, 0" = PRINTFORMDL
;e.g.
;CALL MOUSEOVERFOOTNOTE(@"紅橙黃綠青藍紫ABCDEFG", "紫:F", "幻想鄉最婆的角色:Alleged minimum cup size", 1, 1)
;= PRINTFORMW 紅橙黃綠青藍紫ABCDEFG
;Highlighted text = "紫" & "F", Notes inserted = "幻想鄉最婆的角色" & "Alleged minimum cup size" correspondingly
;----------------------------------------------------------------------
@MOUSEOVERFOOTNOTE(ARGS, IdentifierGroup, FootnoteGroup, PrintType, ColorType)
#DIM PrintType
#DIM ColorType
#DIMS SentenceColor
#DIMS IdentifierGroup
#DIMS DYNAMIC Identifier, 30
#DIMS FootnoteGroup
#DIMS DYNAMIC Footnote, 30
#DIMS OUTPUT

;Splitting input to groups
SPLIT IdentifierGroup, ":", Identifier
SPLIT FootnoteGroup, ":", Footnote

;Setting text colour
IF ColorType == 1
	SentenceColor '= TOSTR(GETCOLOR(),"X6")
ELSE
	SentenceColor '= "C0C0C0"
ENDIF

OUTPUT '= ARGS

;Replace text with html noted text
FOR LOCAL, 0, RESULT
	OUTPUT '= REPLACE(OUTPUT, Identifier:(LOCAL), @"<b>%MO_TALENT(Identifier:(LOCAL), Footnote:(LOCAL), "A3A375")%<font color='#A3A375'>*</font></b>")
NEXT

HTML_PRINT @"<font color='#%SentenceColor%'>%OUTPUT%</font>"

SIF PrintType == 1
	WAIT

