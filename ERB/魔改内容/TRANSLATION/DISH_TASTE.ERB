@DISHNAME_TR(ARGS)
#LOCALSIZE 1
#LOCALSSIZE 1
#FUNCTIONS
SIF ARGS == ""
	ARGS '= DISH_NAME
SIF MAP_HAS(TASTE_MAP, ARGS)
	RETURNF MAP_GET(TASTE_MAP, ARGS)
SIF DISH_COMMENT != "※正在招募中！！※"
	PRINTFORML ※未能找到DISHNAME翻译，请保存日志报错
RETURNF DISH_NAME

@TASTE_TR(ARGS)
#LOCALSIZE 1
#LOCALSSIZE 1
#FUNCTIONS
SIF MAP_HAS(TASTE_MAP, ARGS)
	RETURNF MAP_GET(TASTE_MAP, ARGS)
SIF MAP_HAS(CSV_MAP, ARGS)
	RETURNF MAP_GET(CSV_MAP, ARGS)
SIF ARGS != ""
	PRINTFORML ※未能找到%ARGS%翻译，请保存日志报错
RETURNF ARGS
