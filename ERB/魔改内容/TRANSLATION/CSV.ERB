@STR_TR(ARG)
#LOCALSIZE 1
#LOCALSSIZE 1
#FUNCTIONS
SIF STR:ARG == ""
RETURNF ""
SELECTCASE ARG
	CASE 0
		RETURNF STR:ARG
	CASEELSE
		SIF MAP_HAS(CSV_MAP, STR:ARG)
			RETURNF MAP_GET(CSV_MAP, STR:ARG)
		;#;PRINTFORML ※未能找到STR:%STR:ARG%翻译，请保存日志报错
		RETURNF STR:ARG
ENDSELECT

