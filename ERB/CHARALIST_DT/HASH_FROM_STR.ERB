;特定文字列からハッシュ値を算出する関数
@HASH_FROM_STR(ARGNAME = "")
#FUNCTION
#DIM L_BYTE
#DIM CNT
#DIM CONST PNUM_SMALL = 257 ;PNUM = PRIME NUMBER
#DIM CONST PNUM_LARGE = 100000037 ;ハッシュは最大でもこれ-1までしか出ない
#DIMS ARGNAME

SIF ARGNAME == ""
	THROW 未指定字符串

L_BYTE = 0
FOR CNT , 0 , STRLENSU(ARGNAME)
	L_BYTE = (L_BYTE * PNUM_SMALL + ENCODETOUNI(ARGNAME , CNT)) % PNUM_LARGE
NEXT

RETURNF L_BYTE
