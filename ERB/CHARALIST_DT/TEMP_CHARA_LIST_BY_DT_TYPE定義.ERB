		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
				CALL PRINT_RANK(DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , "MAX_50000")
				;PRINT_RANKは半角２文字使う
				PRINTFORM {DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , ARG_LEN - 2 , RIGHT}
			CASE "IS_FILTER_TARGET"
				;RESULT:0 = 0ならフィルター対象外、非0ならフィルター対象
				;RESULTS:0 = フィルター値として使う値
				;RESULTS:1 = ボタンとして表示する値。空なら数値がそのまま提示される
				;RESULTS:2 = 値の任意入力を許容するか
				RESULTS:0 = 0,100,500,1000,3000,10000
				RESULTS:1 = 
