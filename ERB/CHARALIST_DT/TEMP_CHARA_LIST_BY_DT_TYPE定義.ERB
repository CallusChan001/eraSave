				RETURN 1
			CASEELSE
				THROW 予期しないARG_MODE=%ARG_MODE%
		ENDSELECT
	CASE 11
		;専用処理が必要なもの
		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
				VARSET L_TEMPS
				SPLIT ARG_VAR , "," , L_TEMPS
				CALLFORM LIST_DT_%L_TEMPS:0%個別処理(TOINT(L_TEMPS:1) , DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , 1)
				;HTML_PRINTにすると一列全部ボタン化ができなくなるので不可
				;HTML_PRINT @"%RESULTS , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 , RIGHT%" , 1
				PRINTFORM %RESULTS , ARG_LEN , RIGHT%
			CASE "IS_FILTER_TARGET"
				;これだけのために@GET_TALENTNAMEを加工したのだ！
				VARSET L_TEMPS
				SPLIT ARG_VAR , "," , L_TEMPS
				;第二引数は関係ない
				RESULTS:0 '= GET_TALENTNAME(TOINT(L_TEMPS:1) , 0 , 1)
				RESULTS:1 '= GET_TALENTNAME(TOINT(L_TEMPS:1) , 0 , 2)
				
				;もし空文字があったら"なし"に置き換えておく
				SIF SUBSTRINGU(RESULTS:1 , 0 , 1) == ","
					RESULTS:1 = 无%RESULTS:1%
				SIF SUBSTRINGU(RESULTS:1 , STRLENSU(RESULTS:1) - 1 , 1) == ","
					RESULTS:1 = %RESULTS:1%无
				SIF STRCOUNT(RESULTS:1 , ",,")
					RESULTS:1 '= REPLACE(RESULTS:1 , ",," , ",无,")
				RESULTS:2 = 0
				RESULTS:3 = =
				RETURN 1
			CASEELSE
				THROW 予期しないARG_MODE=%ARG_MODE%
		ENDSELECT
