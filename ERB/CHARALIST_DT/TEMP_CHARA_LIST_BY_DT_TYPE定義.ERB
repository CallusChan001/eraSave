	CASE 9
		;ネガティブ系刻印
		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
				CALL COLOR_STAMP(DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , 3, DEF_LIST_MARKNEGATIVE, C_RED)
			CASE "IS_FILTER_TARGET"
