		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
				PRINTFORM %" " * MAX(((ARG_LEN - 2) / 2) , 0)%
				CALL COLOR_STAMP(DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1), STRLENS(DEF_LIST_MARKHEART) == 1 ? 2 # 1, DEF_LIST_MARKHEART, C_PINK)
