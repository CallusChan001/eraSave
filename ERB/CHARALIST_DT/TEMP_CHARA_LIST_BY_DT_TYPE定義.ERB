			CASE "IS_FILTER_TARGET"
				RETURN 0
			CASEELSE
				THROW 予期しないARG_MODE=%ARG_MODE%
		ENDSELECT
	CASE 13
		;相手との関係を一括で見る
		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
