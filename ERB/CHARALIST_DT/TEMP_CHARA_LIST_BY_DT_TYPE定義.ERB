			CASE "PRINT_CELL"
				CALL COLOR_STAMP(DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1), 3, DEF_LIST_MARKNEUTRAL, GETCOLOR())
			CASE "IS_FILTER_TARGET"
				;RESULT:0 = 0ならフィルター対象外、非0ならフィルター対象
