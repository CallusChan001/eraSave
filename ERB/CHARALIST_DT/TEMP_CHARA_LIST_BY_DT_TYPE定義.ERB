					PRINTFORM %GET_TALENTNAME(DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , TALENT:DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , DEF_DT_CID , 1):DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1)) , ARG_LEN%
					;PRINTFORM {DT_COLUMN_EXIST(ARG_DTNAME , ARG_COLUMN_NAME)} COL:{DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1)} CID:{TALENT:DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , DEF_DT_CID , 1)}
				ELSE
					PRINTFORM %" " * ARG_LEN%
				ENDIF
			CASE "IS_FILTER_TARGET"
				;文が汚い…
				RESULTS:0 = {DEF_CULC_RELATION_NULL},{GETNUM(TALENT , "恋人")},{GETNUM(TALENT , "恋慕")},{GETNUM(TALENT , "恋慕")},{GETNUM(TALENT , "愛欲")},{GETNUM(TALENT , "炮友")},{GETNUM(TALENT , "炮友")},{GETNUM(TALENT , "思慕")}
				RESULTS:1 = 无,%GET_TALENTNAME(GETNUM(TALENT , "恋人") , 1)%,%GET_TALENTNAME(GETNUM(TALENT , "恋慕") , 1)%,%GET_TALENTNAME(GETNUM(TALENT , "恋慕") , 2)%,%GET_TALENTNAME(GETNUM(TALENT , "愛欲") , 1)%,%GET_TALENTNAME(GETNUM(TALENT , "炮友") , 1)%,%GET_TALENTNAME(GETNUM(TALENT , "炮友") , 2)%,%GET_TALENTNAME(GETNUM(TALENT , "思慕") , 1)%
				RESULTS:2 = 0
				RESULTS:3 = =
				RETURN 1
			CASEELSE
				THROW 予期しないARG_MODE=%ARG_MODE%
		ENDSELECT
	CASE 14
		;TYPE=1のように２個１組で処理する文字列。ただし２個目はソート用で表示はされない
		;挙動が大変怪しいので使うときはちゃんと動いてるか確認を
		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
				PRINTFORM %DT_CELL_GETS(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , ARG_LEN , RIGHT%
			CASE "IS_FILTER_TARGET"
				RESULTS:0 = 
				RESULTS:1 = 
				RESULTS:2 = 
				RESULTS:3 = 
				RETURN 0
			CASEELSE
				THROW 予期しないARG_MODE=%ARG_MODE%
		ENDSELECT
	CASE 15
		;TYPE=14とは違い、ソート用の列を持たない文字列
		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
				PRINTFORM %DT_CELL_GETS(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , ARG_LEN , RIGHT%
			CASE "IS_FILTER_TARGET"
				RESULTS:0 = 
				RESULTS:1 = 
				RESULTS:2 = 
				RESULTS:3 = 
				RETURN 0
			CASEELSE
				THROW 予期しないARG_MODE=%ARG_MODE%
		ENDSELECT
	CASEELSE
		;COLUMN_TYPEに該当なしならとりあえずデータを直接表示
		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
				IF DT_COLUMN_EXIST(ARG_DTNAME , ARG_COLUMN_NAME) == 0
					THROW 列%ARG_COLUMN_NAME%はDataTable"%ARG_DTNAME%"に存在しないよ
				ELSEIF DT_COLUMN_EXIST(ARG_DTNAME , ARG_COLUMN_NAME) == 5
					;列が文字列設定
					PRINTFORM %DT_CELL_GETS(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , ARG_LEN , RIGHT%
				ELSE
					;列が数値設定
					PRINTFORM {DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1) , ARG_LEN , RIGHT}
				ENDIF
			CASE "IS_FILTER_TARGET"
