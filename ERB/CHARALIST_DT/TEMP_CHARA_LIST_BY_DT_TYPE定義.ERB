				RESULTS:3 = <=,<,>=,>,=
				RETURN 1
			CASEELSE
				THROW 予期しないARG_MODE=%ARG_MODE%
		ENDSELECT
	CASE 7
		;素質などのbool型
		SELECTCASE ARG_MODE
			CASE "PRINT_CELL"
				IF DT_CELL_GET(ARG_DTNAME , ARG_ROW_ID , ARG_COLUMN_NAME , 1)
					PRINTFORM %DEF_LIST_BOOLTRUE , ARG_LEN , RIGHT%
				ELSE
					PRINTFORM %DEF_LIST_BOOLFALSE , ARG_LEN , RIGHT%
