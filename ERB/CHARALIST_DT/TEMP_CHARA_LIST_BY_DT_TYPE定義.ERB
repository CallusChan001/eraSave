			CASE 6
				;好感度などのランク形式表示
				CALL PRINT_RANK(DT_CELL_GET(L_DTNAME , L_CHR_TEMP , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0) , "MAX_50000")
				;PRINT_RANKは半角２文字使う
				PRINTFORM {DT_CELL_GET(L_DTNAME , L_CHR_TEMP , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0) , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 - 2 , RIGHT}
			CASE 7
				;素質などのbool型
				IF DT_CELL_GET(L_DTNAME , L_CHR_TEMP , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0)
					PRINTFORM %DEF_LIST_BOOLTRUE , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 , RIGHT%
				ELSE
					PRINTFORM %DEF_LIST_BOOLFALSE , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 , RIGHT%
				ENDIF
			CASE 8
				;ポジティブ系刻印
				CALL COLOR_STAMP(DT_CELL_GET(L_DTNAME , L_CHR_TEMP , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0), 3, DEF_LIST_MARKPOSITIVE, 0xFFFF00)
			CASE 9
				;ネガティブ系刻印
				CALL COLOR_STAMP(DT_CELL_GET(L_DTNAME , L_CHR_TEMP , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0), 3, DEF_LIST_MARKNEGATIVE, C_RED)
			CASE 10
				;ポジでもネガでもない系刻印
				CALL COLOR_STAMP(DT_CELL_GET(L_DTNAME , L_CHR_TEMP , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0), 3, DEF_LIST_MARKNEUTRAL, GETCOLOR())
			CASE 11
				;専用処理が必要なもの
				VARSET L_ELEMENT
				SPLIT COLUMN_VAR:L_SELECTED_COLUMN_GROUP:CNT_0 , "," , L_ELEMENT
				CALLFORM LIST_DT_%L_ELEMENT:0%個別処理(TOINT(L_ELEMENT:1) , DT_CELL_GET(L_DTNAME , L_CHR_TEMP , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0) , 1)
				;HTML_PRINTにすると一列全部ボタン化ができなくなるので不可
				;HTML_PRINT @"%RESULTS , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 , RIGHT%" , 1
				PRINTFORM %RESULTS , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 , RIGHT%
			CASE 12
				;はぁとまぁく。恋慕とか用。TYPE=13に恋慕とか炮友とか一括して見られる欄作ったけど、個別に見たい場合はここで
				PRINTFORM %" " * MAX(((COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 - 2) / 2) , 0)%
				CALL COLOR_STAMP(DT_CELL_GET(L_DTNAME , L_CHR_TEMP , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0), STRLENS(DEF_LIST_MARKHEART) == 1 ? 2 # 1, DEF_LIST_MARKHEART, C_PINK)
				PRINTFORM %" " * MAX(((COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 - 2) / 2 + COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 % 2) , 0)%
			CASE 13
				;相手との関係を一括で見る
				IF INRANGE(DT_CELL_GET(L_DTNAME , CHRLIST:CNT_CHR , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0 , 1) , 0 , VARSIZE("TALENT"))
					;クッソ長くてごめんね。{}使うと半角スペースが混じって上手くいかなかった
					PRINTFORM %GET_TALENTNAME(DT_CELL_GET(L_DTNAME , CHRLIST:CNT_CHR , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0 , 1) , TALENT:DT_CELL_GET(L_DTNAME , CHRLIST:CNT_CHR , DEF_DT_CID , 1):DT_CELL_GET(L_DTNAME , CHRLIST:CNT_CHR , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0 , 1)) , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0%
				ELSE
					PRINTFORM %" " * COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0%
				ENDIF
			CASEELSE
				;COLUMN_TYPEに該当なしならとりあえずデータを直接表示
				SIF DT_COLUMN_EXIST(L_DTNAME , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0) == 0
					THROW 列%COLUMN_NAME:CNT_0:CNT_1%はDataTable%L_DTNAME%に存在しないよ

				IF DT_COLUMN_EXIST(L_DTNAME , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0) == 5
					;列が文字列設定
					PRINTFORM %DT_CELL_GETS(L_DTNAME , CHRLIST:CNT_CHR , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0 , 1) , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 , RIGHT%
				ELSE
					;列が数値設定
					PRINTFORM {DT_CELL_GET(L_DTNAME , CHRLIST:CNT_CHR , COLUMN_NAME:L_SELECTED_COLUMN_GROUP:CNT_0 , 1) , COLUMN_LEN:L_SELECTED_COLUMN_GROUP:CNT_0 , RIGHT}
				ENDIF
		ENDSELECT
		[SKIPEND]
