@LIST_DT_TALENT個別処理(ARG_ELEMENT = -1 , ARG_VALUE = 0 , ARG_SHORT = 0)
#DIM ARG_ELEMENT
#DIM ARG_VALUE
#DIM ARG_SHORT
IF ARG_ELEMENT == -1
	RESULTS = 没有忘记传递值吧？
	RETURN -1
ENDIF

;获取TW现有函数中的素质名
RESULTS '= GET_TALENTNAME(ARG_ELEMENT , ARG_VALUE)
;如果没有指定省略，则直接返回
SIF ARG_SHORT == 0
	RETURN 1

;以下是省略处理
;部分素质名含义重复或过长，因此进行省略处理
SELECTCASE ARG_ELEMENT
	CASE 13
		;自尊心高·低
		RESULTS '= REPLACE(RESULTS , "自尊心" , "")
	CASE 15
		;心情
		RESULTS '= REPLACE(RESULTS , "心情" , "")
	CASE 30
		;贞操观念·无所谓
		RESULTS '= REPLACE(RESULTS , "贞操" , "")
	CASE 40
		;对疼痛强·弱
		;RESULTS '= REPLACE(RESULTS , "痛" , "")
	CASE 41
		;易湿润度。想省略但想不到好的缩写
		
	CASE 50
		;习得快·慢
		;RESULTS '= REPLACE(RESULTS , "習得" , "")
	CASE 61
		;污秽耐性
		IF RESULTS == "洁癖症"
			RESULTS = 洁癖
		ELSEIF RESULTS == "污臭敏感"
			RESULTS = 敏感
		ELSEIF RESULTS == "污臭钝感"
			RESULTS = 迟钝
		ELSEIF RESULTS == "污臭无视"
			RESULTS = 无视
		ENDIF
	CASE 70
		;否定快感·顺从快感
		RESULTS '= REPLACE(RESULTS , "快感" , "")
	CASE 81
		;性别偏好
		IF RESULTS == "两面通吃"
			RESULTS = 双性恋
		ELSEIF RESULTS == "讨厌男人"
			RESULTS = 厌男
		ELSEIF RESULTS == "讨厌女人"
			RESULTS = 厌女
		ELSEIF RESULTS == "不信任人类"
			RESULTS = 不信
		ENDIF
	CASE 101
		;Ｃ感度
		RESULTS '= REPLACE(RESULTS , "Ｃ" , "")
	CASE 102
		;Ｖ感度
		RESULTS '= REPLACE(RESULTS , "Ｖ" , "")
	CASE 103
		;Ａ感度
		RESULTS '= REPLACE(RESULTS , "Ａ" , "")
	CASE 104
		;Ｂ感度
		RESULTS '= REPLACE(RESULTS , "Ｂ" , "")
	;↓虽然做了分支，但实际用起来太乏味了，所以排除了这个分支
	[SKIPSTART]
	CASE 105
		;胸围
		IF RESULTS == "絶壁"
			RESULTS = 平坦
		ELSE
			RESULTS '= REPLACE(RESULTS , "乳" , "")
		ENDIF
	[SKIPEND]
	CASE 106
		;Ｍ感度
		RESULTS '= REPLACE(RESULTS , "Ｍ" , "")
	;↓虽然做了分支，但实际用起来太乏味了，所以排除了这个分支
	[SKIPSTART]
	CASE 108
		;臀围
		;这个可以省略吗？怎么省略好呢……
		IF ARG_VALUE == -2
			RESULTS = 纤臀
		ELSEIF ARG_VALUE == -1
			RESULTS = 小臀
		ELSEIF ARG_VALUE == 0
			RESULTS = 并臀
		ELSEIF ARG_VALUE == 1
			RESULTS = 润臀
		ELSEIF ARG_VALUE == 2
			RESULTS = 丰臀
		ENDIF
	[SKIPEND]
	CASE 121
		;酒精耐性
		IF RESULTS == "不会喝酒"
			RESULTS = 不会
		ELSEIF RESULTS == "不擅喝酒"
			RESULTS = 弱
		ELSEIF RESULTS == "擅于喝酒"
			RESULTS = 强
		;ELSEIF RESULTS == "酒豪"
		;	;两个字，就这样
		;	;RESULTS = 豪饮
		;ELSEIF RESULTS == "海量"
		;	RESULTS = 海量
		ENDIF
	CASE 130
		;恢复速度
		RESULTS '= REPLACE(RESULTS , "恢复" , "")
	;CASE 143
	;	;动物耳朵系
	;	RESULTS '= REPLACE(RESULTS , "ミミ" , "")
	;	RESULTS '= REPLACE(RESULTS , "耳" , "")
	CASE 149
		;母乳/哺乳。@GET_TALENTNAME是CASE 130，但CSV中母乳相关是149
		IF RESULTS == "可以哺乳"
			RESULTS = 哺乳
		ELSEIF RESULTS == "泌乳体质"
			RESULTS = 泌乳
		ENDIF
	CASE 161
		;音乐知识。音乐知识是排他的呢
		RESULTS '= REPLACE(RESULTS , "知识" , "")
	CASE 162
		;乐感
		SIF RESULTS == "绝对音感"
			RESULTS = 绝对
	;CASE 250
	;	;丁丁形状。看着有点好笑
	;	RESULTS '= REPLACE(RESULTS , "チンポ" , "")
	;	RESULTS '= REPLACE(RESULTS , "チン" , "")
	CASE 274
		;叠石系
		RESULTS '= REPLACE(RESULTS , "叠石" , "")
	CASEELSE
		;RESULTS = 
		;RETURN 0
ENDSELECT

RETURN 1


