@CHANGE_MONTH
#LOCALSIZE 1
#LOCALSSIZE 1
#DIM 神簽代
#DIM IFS_COST
;日期が月を跨いだら月変更    日期跨月变更
IF DAY:日期 > 31
	DAY:日期 = 1
	DAY:季節 ++
	;冬の次は春
	IF DAY:季節 > 季节_冬
		DAY:季節 = 季节_春
		FLAG:年始貸付金 = FLAG:地主貸付金
		;(天候パッチ)長期予報を行う    （天气补丁）进行长期预报/年预报
		CALL WEATHER_FORECAST_1YEAR
	ENDIF
	;夏になったらバレンタインフラグリセット    夏天重置情人节标记
	SIF DAY:季節 == 季节_夏
		CVARSET CFLAG, 383
	;徴収発生
	SIF FLAG:地主
		FLAG:年貢 = 1
	IF FLAG:澆水代行
		FLAG:澆水代行 = 0
		PRINTFORMW 浇水代理的期限到期了。
	ENDIF
	IF FLAG:新聞購読
		IF MONEY < 3000 && MONEY:2 < 10
			PRINTFORML 因为缺钱所以文文。新闻的订阅停止了。
		ELSE
			PRINTL
			PRINTFORML ★继续订阅文文。新闻吗？（每月%金額表示(1500)% or 5筹码）
			CALL ASK_M("解约",1,@"更新（%金額表示(1500)%）",MONEY >= 1500,"更新（5筹码）",MONEY:2 >= 5)
			SELECTCASE RESULT
				CASE 0
					PRINTFORML 停止了订阅。
					FLAG:新聞購読 = 0
				CASE 1
					CALL PAY_MONEY_YEN(3000, 2)
					PRINTFORML 本月继续订阅。
				CASE 2
					CALL PAY_MONEY_CM(10, 2)
					PRINTFORML 本月继续订阅。
			ENDSELECT
		ENDIF
	ENDIF
	IF FLAG:神簽契約
		神簽代 = 50
		SIF !MAIN_MAP
			神簽代 = 30
		IF MONEY:2 < 神簽代
			PRINTFORML 因为筹码不足、所以神签契约被中止了
			FLAG:神簽契約 = 0
		ELSE
			PRINTFORML 要继续神签契约吗？
			CALL ASK_YN
			IF !RESULT
				PRINTFORML 更新了契约
				CALL PAY_MONEY_CM(神簽代, 2)
			ELSE
				FLAG:神簽契約 = 0
				PRINTFORML 结束了契约
			ENDIF
		ENDIF
	ENDIF
	;虫采集服务续订
	SIF FLAG:蟲の採集サービス
		CALL IFS_SUBSCRIBE
	;堇子的超能力加护续订
	SIF ENCOUNTDLC
		CALL SP_ENCOUNT
ENDIF
;異常値を固定修正
SIF DAY:3 < 1
	DAY:3 = 1
SIF DAY:2 < 1 || DAY:2 > 4
	DAY:2 = 1

