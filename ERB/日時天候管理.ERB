;定時遭遇位置再設定    该函数不变更本地、来访、宴会、招待、同行、仕事、睡眠的角色
@TIMEZONE_MOVE
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS 移動前
#DIMS 移動後
FOR LOCAL,1,CHARANUM
	SIF 出現状態(LOCAL) != 1
		CONTINUE
	SIF !CHARA_HOLIDAY(LOCAL)
		CONTINUE
	SIF CFLAG:LOCAL:招待
		CONTINUE
	SIF CFLAG:LOCAL:同行 || CHK_DATENOW(CFLAG:LOCAL:约会中)
		CONTINUE
	SIF !CFLAG:LOCAL:前一次的位置
		CONTINUE
	SIF CFLAG:LOCAL:睡眠
		CONTINUE
	;你と同じ場所にいるときは確率が下がる    和你处在同一位置时概率降低
	IF CFLAG:MASTER:現在位置 == CFLAG:LOCAL:現在位置
		IF TALENT:LOCAL:恋慕
			;恋慕は5分の1
			SIF RAND:5
				CONTINUE
		ELSEIF TALENT:LOCAL:思慕
			;思慕は4分の1
			SIF RAND:4
				CONTINUE
		ELSE
			;それ以外は3分の1
			SIF RAND:3
				CONTINUE
		ENDIF
	ELSE
		;見てない場所なら2分の1
		SIF RAND:2
			CONTINUE
	ENDIF
	CALL 遭遇フラグ決定(LOCAL)
NEXT
;全角色決定後、連動角色を設定する
FOR LOCAL, 1, CHARANUM
	VARSET RESULT
	;新增剧本侧接口，调用剧本侧设定的角色移动数值
	CALL KOJO_CHECK(LOCAL, "CHARADATA", "MOVE", 1)
	IF RESULT == -1
		;剧本侧禁用该机制，但是请作者务必清楚自己在做什么
		VARSET RESULT
	ELSEIF RESULT || RESULT:1
		;采用剧本设定的值
	ELSE
		;剧本没有采用CHECK函数或者剧本需要是结果为0，从而采用原版移动数值
		CALLFORM CHARAMOVE_DATA_{NO:LOCAL}(1)
	ENDIF
	;依存連動
	IF RESULT
		CFLAG:LOCAL:遭遇位置 = CFLAG:RESULT:遭遇位置
	;対等連動
	ELSEIF RESULT:1
		;連動設定する
		CFLAG:LOCAL:遭遇位置 = CFLAG:連動角色設定(RESULT:1, RESULT:2, RESULT:3):遭遇位置
	ENDIF
NEXT
