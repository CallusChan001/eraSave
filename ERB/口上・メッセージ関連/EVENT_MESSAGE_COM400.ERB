	SIF GET_HEAT_THUNDERSTORM() == 1
		DEBUGPRINTFORML 熱雷発生フラグ
	SIF GET_FRONTAL_THUNDERSTORM() == 1
		DEBUGPRINTFORML 界雷発生フラグ
