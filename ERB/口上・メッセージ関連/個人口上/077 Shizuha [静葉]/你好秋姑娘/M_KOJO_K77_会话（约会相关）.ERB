;约会相关对话，分道中和地图内两种情况
@K77_DATETALK
#DIM TALK_OPTIONNUM = 3
IF TFLAG:约会道中
    SIF GET_TARGETNUM() == 2
        LOCAL = K77_GETNO_ANOTHERONE()
    SELECTCASE CFLAG:约会中
        ;博丽神社
        CASE 0
            SIF GROUPMATCH(LOCAL, 1, 10, 11, 26, 78)
                TALK_OPTIONNUM = 5
        ;命莲寺
        CASE 1
            SIF GROUPMATCH(LOCAL, 39, 55, 56, 92, 78)
                TALK_OPTIONNUM = 5
        ;人间之里
        CASE 2
            SIF GROUPMATCH(LOCAL, 29, 34, 70, 80, 78)
                TALK_OPTIONNUM = 5
        ;雾之湖~红魔馆
        CASE 3
            SIF GROUPMATCH(LOCAL, 12, 13, 15, 16, 78)
                TALK_OPTIONNUM = 5
        ;迷途竹林
        CASE 4
            SIF GROUPMATCH(LOCAL, 52, 61, 62, 68, 78)
                TALK_OPTIONNUM = 5
        ;魔法之森
        CASE 5
            SIF GROUPMATCH(LOCAL, 4, 11, 17, 102, 78)
                TALK_OPTIONNUM = 5
        ;三途之川~冥界
        CASE 6
            SIF GROUPMATCH(LOCAL, 23, 26, 30, 66, 78)
                TALK_OPTIONNUM = 5
        ;妖怪之山（山麓）
        CASE 7
            SIF GROUPMATCH(LOCAL, 29, 42, 51, 79, 78)
                TALK_OPTIONNUM = 5
        ;妖怪之山（山顶）
        CASE 8
            SIF GROUPMATCH(LOCAL, 31, 32, 33, 34, 78)
                TALK_OPTIONNUM = 5
        ;地底
        CASE 9
            SIF GROUPMATCH(LOCAL, 36, 37, 38, 49, 78)
                TALK_OPTIONNUM = 5
        ;梦之世界~月
        CASE 10
            SIF GROUPMATCH(LOCAL, 98, 99, 109, 110, 78)
                TALK_OPTIONNUM = 5
    ENDSELECT
    ;默认对话数为3，当三人约会时对话数为5，之后根据指定目标触发新的对话（CASE 3和CASE 4）
    SELECTCASE RAND:TALK_OPTIONNUM
        CASE 0, 1, 2
            CALL K77_DATETALK_ONTHEROAD
        CASE 3, 4
            CALL K77_DATETALK_WITHOTHER(LOCAL)
    ENDSELECT
ELSE
    CALL K77_DATETALK_ARRIVAL
ENDIF

