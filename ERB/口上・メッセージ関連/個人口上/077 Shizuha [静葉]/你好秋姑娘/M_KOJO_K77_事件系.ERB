;==================================================
@M_KOJO_ENCOUNTER_K77
;是否启用（= 0，不启用；= 1，启用）
LOCAL = 1
;-------------
IF LOCAL
    ;在秋姐妹家居住
    IF CFLAG:MASTER:初期位置 == 719
        CALL K77_ENCOUNTER_LIVINGTOGETHER
    ELSE
        SELECTCASE CFLAG:MASTER:現在位置
        ;在秋姐妹家
			;田地
			CASE 715
				CALL K77_ENCOUNTER_IN_HERFIELD
			;土间
			CASE 716
				CALL K77_ENCOUNTER_IN_HERDIRTYFLOOR
			;寝室
			CASE 717
				CALL K77_ENCOUNTER_IN_HERBEDROOM
			;居间
			CASE 718
				CALL K77_ENCOUNTER_IN_HERLIVINGROOM
			;座敷（杂物间）
			CASE 719
				CALL K77_ENCOUNTER_IN_HERROOM
			CASEELSE
		;不在秋姐妹家
				IF K77_LOCATION_IS_YOUKAIMOUNTAIN_OUTSIDE(MASTER)
					;山上
					CALL K77_ENCOUNTER_IN_YOUKAIMOUNTAIN
				ELSEIF INROOM(CFLAG:MASTER:現在位置)
					;室内
					CALL K77_ENCOUNTER_IN_CASUALINDOOR
				ELSE
					;室外
					CALL K77_ENCOUNTER_IN_CASUALOUTDOOR
				ENDIF
        ENDSELECT
    ENDIF
ENDIF
RETURN 1

