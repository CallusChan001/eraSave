;==================================================
;与角色相遇
;--------------------------------------------------
;ERB\イベント関連\情事発覚.ERB
;==================================================
@M_KOJO_EVENT_K77_1(ARG, ARG:1)
;口上角色编号 = TARGET
;--------------------------------------------------
;是否启用（= 0，不启用；= 1，启用）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;MASTER进入区域见到了TARGET
	;-------------------------------------------------
	IF ARG == 1
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「是%CALLNAME:MASTER%呢」
					PRINTFORMDL 能看到这样的静叶，让你感到由衷的欣慰……
				CASE 1
					PRINTFORML 「啊，亲爱的过来了！」
					PRINTFORMDL 静叶上前跟你寒暄起来……
			ENDSELECT
		ELSEIF TALENT:思慕 || TALENT:炮友
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMDL 静叶，你好哦
					PRINTFORML 「%CALLNAME:MASTER%也好哦！」
					PRINTFORMDL 静叶微笑着面对你
				CASE 1
					PRINTFORML 「是%CALLNAME:MASTER%呢！」
					PRINTFORMDL 和静叶打过招呼后，能感觉到她语气变得很是畅快
			ENDSELECT
		ELSE
			PRINTFORMDL 见到了秋姑娘啊……
			PRINTFORMDL 现在的她看起来很是文静
		ENDIF
	RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET进入区域见到了MASTER
	;若TARGET与MASTER同时进入该区域也算作此种情况
	;-------------------------------------------------
	IF ARG == 2
		IF TALENT:恋人 || TALENT:恋慕
			SELECTCASE RAND:2
				CASE 0
					PRINTFORML 「哎呀，是%CALLNAME:MASTER%呢！」
					PRINTFORMDL 静叶看到你就高兴地迎过来
					PRINTFORML 「亲爱的，今天有什么事呢？」
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%！」
					PRINTFORMDL 静叶笑着向你打着招呼
					PRINTFORMDL 这就是变得乐观开朗的秋姑娘啊……
			ENDSELECT
		ELSEIF TALENT:思慕 || TALENT:炮友
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMDL 秋神正站在一旁看着你
					PRINTFORML 「啊，是%CALLNAME:MASTER%呢！」
					PRINTFORMDL 她高兴地向你走过来
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%好哦」
					PRINTFORMDL 静叶向你挥了挥手
			ENDSELECT
		ELSE
			PRINTFORML 「……」
			PRINTFORMDL 静叶只是时不时瞟向你，没有跟你打招呼……
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET进入浴场与MASTER共浴
	;如果MASTER性别为女（扶她不行），则一定可以共浴
	;否则需要进行一些亲密度与素质的判定
	;-------------------------------------------------
	;当 催眠强度 > 50 时，一定可以共浴
	;但到底是因为本来就能共浴还是完全因为催眠，就很难简单检测了
	;@風呂逐出条件 中写了判定条件，若有需要可以模仿自定义一个检测函数
	;-------------------------------------------------
	IF ARG == 3
		IF TALENT:恋人 || TALENT:恋慕
			IF !RAND:2
				PRINTFORML 「啊，%CALLNAME:MASTER%也过来了呢」
				PRINTFORMDL 面色红润的静叶邀请你过来和她一起共浴
				PRINTFORML 「能这样看着%CALLNAME:MASTER%的身体，真是幸福呢」
			ELSE
				PRINTFORML 「是%CALLNAME:MASTER%啊，没想到你也会来呢」
				PRINTFORMDL 现在的静叶对你没有任何遮挡，只有偶尔冒出的雾气出来遮挡视线
				PRINTFORMDL ……不过依然能从中看见她的曼妙身材
			ENDIF
		ELSE
			PRINTFORML 「呀，%CALLNAME:MASTER%也在吗？！」
			PRINTFORMDL 静叶差点就要陷进池水里了
			IF !RAND:2
				PRINTFORML 「偶然过来的吗？……我明白了」
				PRINTFORML 「那就……一起吧？」
				PRINTFORMDL 静叶羞涩着靠近了你
			ELSE
				PRINTFORMDL 没想到她反应那么大
				PRINTFORMDL 可以……一起吗？
				PRINTFORMDL 你慢慢靠近静叶，她倒也没有赶你出去
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET进入浴场，见到MASTER又离开了
	;-------------------------------------------------
	IF ARG == 4
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORML （啊，那是%CALLNAME:MASTER%吧！……）
			PRINTFORML （是在洗浴呢，那副身体……）
			PRINTFORML （呜！不、不敢再看下去了！！）
			PRINTFORMDL 静叶忍住羞耻心离开了……
		ELSE
			PRINTFORML 「啊，没想到这里还有人啊！」
			PRINTFORML 「对不起，打扰了……」
			PRINTFORMDL 听到了静叶的声音，回过头一看人影就已经不见了……
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET进入浴场并且把MASTER赶出去了
	;在不满足共浴条件的情况下 TARGET持有把柄且MASTER性别为女或扶她时触发
	;-------------------------------------------------
	IF ARG == 5
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;在外出的途中遇见了
	;-------------------------------------------------
	IF ARG == 6
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORML 「呀，亲爱的在这里呢！」
			PRINTFORMDL 静叶好像想跟你一块走
			PRINTFORML 「诶，有事情要忙吗？那要带我的话就跟我说哦！」
		ELSEIF TALENT:思慕 || TALENT:炮友
			PRINTFORML 「%CALLNAME:MASTER%今天出门了呢，是有什么工作吗？」
			PRINTFORML 「……这样啊，祝你好运」
			PRINTFORMDL 静叶很客气地向你问好
		ELSE
			PRINTFORML 「……」
			PRINTFORMDL 静叶只是看了你一眼，随后就什么反应也没有了
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;在与其他人约会的时候遇见了
	;ARG:1是约会的对象
	;-------------------------------------------------
	IF ARG == 7
		IF !GROUPMATCH(ARG:1, 1, 11, 29, 31, 32, 33, 42, 51, 65, 78)
			IF TALENT:恋人 || TALENT:恋慕
				PRINTFORML 「在跟别的女人约会么……」
				PRINTFORMDL 静叶看着你们
				PRINTFORML （要是不过来找我的话，哼哼……）
			ELSEIF TALENT:思慕 || TALENT:炮友
				PRINTFORML 「是%CALLNAME:MASTER%嘛？」
				PRINTFORML 「哎呀，还是跟别人在一起呢，你也好哦」
				PRINTFORML 静叶跟%CALLNAME:(ARG:1)%也做了问候
			ELSE
				PRINTFORML （和别人一起么？）
				PRINTFORMDL 静叶对你们有些好奇
				PRINTFORML （是关系很好的情侣么？）
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;在主地图当日初次相遇
	;请注意，在此处口上触发后，会显示 ARG == 1 OR ARG == 2 内的口上内容
	;-------------------------------------------------
	IF ARG == 8
		PRINTFORML 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;在外出期间当日初次相遇
	;ARG:1 是约会对象，和多人约会时，ARG:1为角色编号靠前的约会对象
	;请注意，在此处口上触发后，会显示 ARG == 6 OR ARG == 7 内的口上内容
	;-------------------------------------------------
	IF ARG == 9
		;-------------------------------------------------
		;单独外出时相遇
		;-------------------------------------------------
		IF ARG:1 == 0
			PRINTFORML 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;与灵梦约会时相遇（为示例，请按实际需要增改）
		;-------------------------------------------------
		IF ARG:1 == 1
			PRINTFORML
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;与指定之外的角色约会时相遇（其他情况）
		;-------------------------------------------------
		IF ARG:1
			PRINTFORML
			RETURN 1
		ENDIF
	ENDIF
ENDIF
RETURN 1

;==================================================
;MASTER醒来时（早安口上）
