        IF CFLAG:MASTER:初期位置 != 717 && CFLAG:MASTER:現在位置 == CFLAG:MASTER:初期位置 
            SELECTCASE RAND:3
                CASE 0
                    IF TIME > 1200 && (TALENT:思慕 || TALENT:炮友) && !CFLAG:78:同室 && GET_TARGETNUM() == 1
