;80,拉手
;约会出发后道中时，牵手后会让牵手变成拉手（会走得更快，减少到达目的地所需的时间）
;TFLAG:193(1=大成功 0=成功 （开心地快步走）-1=失败)（无辜地快步走）
;==================================================
@M_KOJO_SUCCESS_SCOM_K77_80
;成否判定
;TFLAG:192(-2=强制中止指令、-1＝强制失败、0=根据COM文件决定、1=强制成功or大成功
TFLAG:192 = 0

;==================================================

@M_KOJO_MESSAGE_MESSAGECHECK_SCOM_K77_80
RETURN 0

@M_KOJO_MESSAGE_SCOM_K77_80
CALL M_KOJO_MESSAGE_SCOM_K77_80_1
RETURN RESULT
@M_KOJO_MESSAGE_SCOM_K77_80_1
;-------------------------------------------------
;启用（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	; 大成功，跟在MASTER身边开心地快步走
	IF TFLAG:193 == 1
        IF TALENT:恋慕 || TALENT:恋人
            SELECTCASE RAND:3
                CASE 0
                    PRINTFORML 「果然、这样才有恋爱的感觉啊！」
                    PRINTFORML 「那么，快点走吧！」
                    PRINTFORMDL 等等——
                    PRINTFORMDW 话音刚落，静叶就拉起了你
                CASE 1
                    PRINTFORML 「是不是有种想全力奔跑的感觉？」
                    PRINTFORMDL 确实呢，当然还是要看路的
                    PRINTFORML 「……话说，一起摔倒什么的会不会很有意思？」
                    PRINTFORMDW 绝对不好受吧（而且我更怕一人拉着另一人倒下去的场面）
                CASE 2
                    PRINTFORML 「哎呀，可别滑倒了哦——」
                    PRINTFORMDL 静叶兴奋地跟在你后面
                    PRINTFORML 「手别松啊——」
                    PRINTFORMDW 说这话的应该是我才对吧
            ENDSELECT
        ELSEIF TALENT:思慕 || TALENT:炮友
            SELECTCASE RAND:2
                CASE 0
                    PRINTFORML 「%CALLNAME:MASTER%今天很高兴呢」
                    PRINTFORMDL 是吗？
                    PRINTFORMW 「当然啊，我也是呢！」
                CASE 1
                    PRINTFORML 「要是走的再快些，我就要跟不上了」
                    PRINTFORMDL 哦……你放慢脚步
                    PRINTFORMW 「——好，现在到我拉着你了！」
            ENDSELECT
        ELSE
            PRINTFORML 「啊……」
            PRINTFORMDW 不知为何，静叶好像很愿意跟着你
        ENDIF
	; 失败，一脸无辜地快步走	
	ELSEIF TFLAG:193 == -1
        IF TALENT:恋慕 || TALENT:恋人
            SELECTCASE RAND:3
                CASE 0
                    PRINTFORML 「%CALLNAME:MASTER%，这样下去你不累吗？」
                    PRINTFORMDL 因为要赶路啊，所以顾不得那么多啦！
                    PRINTFORML 「等等啊，这又不是什么急事吧……」
                    PRINTFORMDW 静叶撇开了你的手
                CASE 1
                    PRINTFORML 「停——停！！你走得太快啦！」
                    PRINTFORMDL 什么？
                    PRINTFORML 「你这样的话我都快站不稳啦！」
                    PRINTFORMDW 哦，抱歉静叶……
                CASE 2
                    PRINTFORML 「别拉了别拉了！我都要被你拉痛了！！」
                    PRINTFORMDL ……你赶紧停下
                    PRINTFORMDL 看了看静叶的腕关节，还真的红了些许
                    PRINTFORMDW ……然后花了好几分钟对静叶道歉
            ENDSELECT
        ELSEIF TALENT:思慕 || TALENT:炮友
            SELECTCASE RAND:2
                CASE 0
                    PRINTFORML 「%CALLNAME:MASTER%，好像不懂要对女孩子轻柔些呢……」
                    PRINTFORML 「\@ TALENT:MASTER:性別 == 1 ? 或许你应该多学学别人是怎么做的 # 看起来你的常识跟我的常识有点不一样\@……」
                    PRINTFORMDW 真对不起……
                CASE 1
                    PRINTFORML 「如果是赶路的话，用不着这么快的呀啊啊啊——」
                    PRINTFORML 「停——我快摔倒了！」
                    PRINTFORMDW 静叶对你有些不满
            ENDSELECT
        ELSE
            PRINTFORML 「……」
            PRINTFORMDW 静叶似乎有点不情愿被你拉着
        ENDIF
	; 普通成功
	ELSE
        IF TALENT:恋慕 || TALENT:恋人
            SELECTCASE RAND:3
                CASE 0
                    PRINTFORML 「现在不打算慢慢走了吗……」
                    PRINTFORML 「总之别太快了，我的体力应该是跟不上你的」
                    PRINTFORMDL 秋神不是可以飞吗？
                    PRINTFORMW 「飞也是要力气的！」
                CASE 1
                    PRINTFORML 「还以为你会多花点时间欣赏风景呢」
                    PRINTFORMDL 之后再说吧，到哪里不是欣赏呢？
                    PRINTFORML 「也是，反正你也不会一直走下去」
                    PRINTFORMDW 静叶笑了笑
                CASE 2
                    PRINTFORML 「呼呼，风吹过来了——」
                    PRINTFORMDL 说着说着静叶就领先你一步了
                    PRINTFORML 「现在是我拉着你了！」
                    PRINTFORMDW 但是我也还是拉着你的，因为力的作用是相互的！
            ENDSELECT
        ELSEIF TALENT:思慕 || TALENT:炮友
            SELECTCASE RAND:2
                CASE 0
                    PRINTFORML 「是因为什么希望早一点过去吗？」
                    PRINTFORML 「总之……别太着急」
                    PRINTFORMDW 静叶提醒着你
                CASE 1
                    PRINTFORML 「啊咧，%CALLNAME:MASTER%是有什么急事吗？」
                    PRINTFORML 「……只是想快点过去而已？哦……」
                    PRINTFORMDW 静叶好像有点失望
            ENDSELECT
        ELSE
            PRINTFORML 「……」
            PRINTFORMDW 静叶什么也没有说，只是被你拉着前进
        ENDIF
	ENDIF
ENDIF
RETURN 1