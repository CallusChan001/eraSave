                        CALL CHARA_TEXT(1, @"「话说这个人是……？」", "L")
                        PRINTFORMDL 灵梦问着你旁边的静叶
                    CASE 5
                        CALL CHARA_TEXT(1, @"「你是秋神来着？」", "L")
                        PRINTFORMDL 灵梦这样问着静叶，看起来她对这位的记性不是很好
                        PRINTFORMDL 也许是因为当时的交战一下子就结束了吧
                    CASE 6
                        CALL CHARA_TEXT(1, @"「嗯，你们……」", "L")
                        PRINTFORMDL 望着你和静叶的灵梦好像在思考什么……
                ENDSELECT
            ENDIF
        ENDIF
    ;魔理沙
    CASE 11
        IF TALENT:11:恋人 || TALENT:11:恋慕
            IF TALENT:恋人 || TALENT:恋慕
                SELECTCASE RAND:4
                    CASE 0
                        CALL CHARA_TEXT(11, @"「%CALLNAME:MASTER%～也过来陪陪我吧」", "L")
