    CASE 33 ;Suwako
        RETURNF "諏訪子"
    CASE 34 ;Tenshi
        RETURNF "天子"
    CASE 35 ;Iku
        RETURNF "衣玖"
    CASE 64 ;Yuugi
        RETURNF "勇儀"
    CASE 69 ;Mamizou
        RETURNF "猯藏"
    CASE 129 ;Kutaka
        RETURNF "久侘歌"
