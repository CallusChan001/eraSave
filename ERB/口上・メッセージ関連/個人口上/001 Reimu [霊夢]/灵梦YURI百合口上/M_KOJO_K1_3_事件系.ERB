;==================================================
;对方解除了推倒状态
;==================================================
@M_KOJO_YURI百合_EVENT_K1_30(ARG,ARG:1)
;口上角色编号 = TARGET
;--------------------------------------------------
;启用（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	;-------------------------------------------------
	;对方累了
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;满足了所以放开了
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「呼……呼……」
		PRINTFORMDL 满足了的灵梦放过了%CALLNAME:MASTER%
		PRINTFORMDL 笑眯眯的来回抚摸着%CALLNAME:MASTER%的头发
		PRINTFORML 「辛苦了，现在就好好休息一下吧。」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;满足了然后被反过来推倒了
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「还想要吗？」
		PRINTFORMDL 被%CALLNAME:MASTER%推倒的灵梦双手环住了%CALLNAME:MASTER%的脖颈
		PRINTFORML 「可以哦，直到%CALLNAME:MASTER%满足为止我都会陪%CALLNAME:MASTER%的。」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;要求继续进攻，做到坏掉
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	    PRINTFORMDL 听到%CALLNAME:MASTER%所说话语的灵梦瞳孔微微缩小，俯身压住了%CALLNAME:MASTER%
		PRINTFORML 「呵呵，好高兴，%CALLNAME:MASTER%这么渴求着我什么的。」
		PRINTFORML 「我会让%CALLNAME:MASTER%露出舒服到融化一样的表情的。」
		PRINTFORML 「一起做到坏掉吧？」
		PRINTFORMDL 灵梦与%CALLNAME:MASTER%再次十指相扣……
		RETURN 1
	ENDIF
ENDIF
RETURN 1


