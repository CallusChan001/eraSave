;==================================================
;81,口交
;==================================================
@M_KOJO_MESSAGE_COM_K63_81

CALL M_KOJO_MESSAGE_COM_K63_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K63_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「想让我舔你的鸡鸡…？人类真是喜欢做奇怪的事情啊」
		PRINTFORMW 「我可以帮你做，但你没问题吗？我觉得我长着犬牙会很危险……」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		SELECTCASE ABL:舌
			CASE 0,1
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML （气味好浓啊，要是指出来他会伤心吗……）
						PRINTFORML （但如果要做这种事，总该说一声吧？）
						PRINTFORML （……难道是鼻子太不灵敏没注意到吗？）
					CASE 1
						PRINTFORML 「果然还是不敢放进嘴里啊—」
						PRINTFORML 「只舔舔就好吗？」
					CASE 2
						PRINTFORML 「咧咯……啾……」
						PRINTFORML 「难得我帮你做，你也要表现得更舒服一点嘛」
				ENDSELECT
				;能力低いうちは経験に下駄を履かせて次の段階までの移行を早める
				;愚痴ばっか聞かされてもつまんないからね
				EXP:口淫経験 += 1
			CASE 2
				SELECTCASE RAND:3
					CASE 0
						PRINTFORML 「看起来很开心？是啊」
						PRINTFORML 「毕竟这里是，非常私密、敏感又脆弱的部分吧？」
						PRINTFORML 「你把这样毫无防备的地方交给我，感觉还不赖呢」
						PRINTFORMDL 她露出犬牙一笑，然后将脸凑近%CALLNAME:MASTER%的股间，开始服侍起来。
					CASE 1
						CALL HPH_PRINT,@"「这个有褶皱的地方很舒服吧？啾HPH 呶啵啵HPH」","L"
						PRINTFORMDL 影狼%TEXTR("沾满唾液的/有些粗糙的")%火热的舌头，温柔地爱抚着龟头
					CASE 2
						CALL HPH_PRINT,@"「系带这里喜欢吗？咧咯～～～HPH」","L"
						PRINTFORMDL %TEXTR("沾满唾液的/有些粗糙的")%舌头一次次地舔舐着龟头系带附近。长长的舔舐、缓慢而温柔。
				ENDSELECT
			CASEELSE
				IF STAIN:PLAYER:2 & 汚れ_精液
					PRINTFORML 「真是的，被精液弄得黏糊糊的……」
					PRINTFORML 「那些没能进入雌性体内的可怜种子，我就让它们变成我的血肉吧♪」
				ELSE
					IF !TCVAR:374
						CALL K63_PRINT_IMG("penis_awe")
						IF TALENT:妊娠願望 && ESTRUS_CYCLE(63) == 150
								PRINTFORML 「呐，你真的想用嘴做吗？」
								CALL HPH_PRINT,@"「我，今天可是，想全部用下面接住呢……HPH」","L"
								PALAM:潤滑 += 10000
								CALL COLORMESSAGE(@"影狼湿透了！润滑＋10000",C_YELLOW,1)
						ELSEIF !RAND:5
							CALL HPH_PRINT,@"「还是一如既往地气味好浓郁……HPH」","L"
							PRINTFORML 「啊！不是说臭哦！？我没有讨厌！」
						ELSEIF TALENT:MASTER:濃厚精液
							CALL HPH_PRINT,@"「想要交尾的急切气味……HPH 是不是忍得太久了……HPH」","L"
							PRINTFORMDL 影狼将脸凑近"肉棒闻了闻气味，然后抚摸着阴囊。尾巴因期待而摇摆。
							PRINTFORMDL 她用指尖舀起滴落的先行液，涂抹在龟头上。
							CALL HPH_PRINT,@"「阴囊也沉甸甸的……HPH 忍得很辛苦吗？我会好好满足你的HPH」","L"
							PALAM:潤滑 += 5000
							CALL COLORMESSAGE(@"润滑＋5000",C_YELLOW,1)
						ENDIF
							PRINTFORMDL 影狼将%TEXTR("脸凑近/手放在")%肉棒上，%TEXTR("陶醉不已/温柔地抚摸着阴囊/摇晃着尾巴")%
							CALL HPH_PRINT,@"%TEXTR("已经鼓鼓的了……感觉就像是非我不可……/啊啊……欧金金好硬……呵呵，今天也精力充沛呢")%HPH","L"
							PRINTFORMDL %TEXTR("指尖抚摸着前端/轻轻亲吻着前端/舌尖舀起先行液/")%，肉棒抽动了一下
							PRINTFORML 「%TEXTR("乖哟乖哟，我现在就让你舒服/你平时总是让我开心，这次我也要让你开开心心♪/呵呵，精子真可怜……本来有机会变成宝宝的，现在却要到我的肚子里去了♪")%」
						ENDIF
						TCVAR:374 = 1
					ENDIF
					PRINTFORMDL 影狼撩开头发，从龟头开始慢慢含入口中。
				SELECTCASE	RAND:2
					CASE 0
						CALL K63_PRINT_IMG("oral")
						PRINTFORMDL 温暖湿润的黏膜，温柔地包裹着%CALLNAME:MASTER%的宝贝。
						CALL HPH_PRINT,@"「%TEXTR("嗯唔/嗯嗯/嗯啾")%、%TEXTR("噗滋/噗哧/噗噗")%HPH %TEXTR("噗咕/噗咕/噗滋滋")%HPH %TEXTR("呜呜/嗤呜/噗叽噗叽")%HPH」","L"
						PRINTFORMDL 舌头仔细地舔舐着系带，每温柔地舔舐一次龟头，甜美的酥麻感就扩散到腹股沟——

					CASE 1
						PRINTFORMDL 可爱的唇将%CALLNAME:MASTER%的宝贝含到根部。
						PRINTFORMDL 然后慢慢地前后移动脸部，发出下流吮吸声地猛攻着。
						CALL HPH_PRINT,@"「咕啵HPH 咕啵HPH 咕啵HPH 咕啵HPH」","L"
						PRINTFORMDL 她伸出双臂，按住%CALLNAME:MASTER%开始抬起的腰部。
						CALL HPH_PRINT,@"「咕噜噜噜HPH 咕噜噜HPH 咕噜噜噜噜噜HPH 咕呜～～～HPH 咕咕咕～～HPH 」","L"
						PRINTFORMDL 狼女捕获雄性的攻势越发激烈——
						CALL K63_PRINT_IMG("oral",1)
				ENDSELECT
				CLEARBIT STAIN:PLAYER:2, 汚れ_精液
		ENDSELECT
		PRINTFORMDW 
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMDW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

