;==================================================
;弾幕勝負
;==================================================
;
@M_KOJO_MESSAGE_COM_K11_DANMAKU(ARGS, 相手残機,ARG:1)
#DIM 相手残機
#DIM nSeriousMode
#DIM kingCrimson
;全体の流れ
;①【戦闘前】→②ダイスロールとプレイヤー選択肢、ハンデをつけた場合は【ハンデ口上】→③対応スキルが発動した場合【スキル口上】→④被弾判定、キャラが被弾したなら【被弾口上】→
;どちらかの残機が0になっていなければ②に戻る→⑤【戦闘後口上】

;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
SIF FLAG:口上色
	TRYCALLFORM M_KOJO_COLOR_K{NO:TARGET}
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;戦闘前
	;HANDICAP_FIXED = 0
	;HANDICAP_RAND = 0
	;ハンデをつけることができ、数値が大きいほど你が不利になる　数値が負なら你が有利に
	;キャラの能力に関わらずクッソ強くしたり弱くしたりできる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "戦闘前"
		HANDICAP_FIXED = 0
		HANDICAP_RAND = 0
		nSeriousMode = 0
		kingCrimson = 0
		IF MARK:反発刻印 >= 3 && !(TALENT:恋慕 || TALENT:恋人) ;恋慕不会触发反三弹幕战
			PRINTFORMW 「这样好吗？可别说我没警告过你%K11_ZE()%～」
			PRINTFORMDW 魔理沙的脸庞上流露出一抹叛逆的笑，眼神也随之变得冷峻
			PRINTFORMDW 你本能地感觉到了不妙…
			PRINTFORMW 「既然你这么执意想和我打一场的话…那就如你所愿好啦，%CALLNAME:MASTER%！」
			PRINTFORMW 「胜负还没有落定，不过…对于接下来要用弹幕狠狠痛扁你的事情，自己还是相当有信心的哟！」
			PRINTFORMDW 魔理沙手中的八卦炉开始逐渐积聚魔力…
			PRINTFORMW 「…听好了，面对你这样的家伙，我可没有要手下留情的打算%K11_ZE("daze")%！」
			PRINTFORMW 「那么，是时候让你也稍微体验下我遭遇的痛苦了————给我做好觉悟吧…！！」
			PRINTFORMDW 凶恶的弹幕风暴迎面而来！
			HANDICAP_FIXED = 1000
			HANDICAP_RAND = 2500
			nSeriousMode = 1
			相手残機 = 4 ;反三沙沙残机强制+1
		ELSE
			IF TALENT:恋人
				PRINTFORMW 「将自己的爱人作为弹幕战的对手吗…」
				PRINTFORMW 「从没想过自己会亲眼见证这一刻的到来%K11_ZE("daze")%～」
				IF ABL:戦闘能力 <= ABL:MASTER:戦闘能力 && RAND:3
					PRINTFORMW 「像这样在一起的修炼做得足够多的话，有朝一日我们说不定会成为幻想乡最强的恋人组合呢…」
					PRINTFORMDW 意识到自己无意间让那样的话语脱口而出的魔理沙，脸蛋不由微微发烫
				ELSE
					PRINTFORMW 「哼哼…没错，唯有这样的对决才配称作真正的『恋』色符卡战！」
					PRINTFORMW 「…干嘛用那种打趣的目光看着人家啦…」
				ENDIF
				PRINTFORML
			ENDIF

			IF TALENT:恋慕 && CFLAG:TARGET:溜まってる度 > 500
				PRINTFORMW 「好吧，%CALLNAME:MASTER%！我接受你的挑战%K11_ZE("daze")%！」
				PRINTFORMW 「不过嘛，这次…何不如把规则变得更有趣一点？」
				PRINTFORMW 「在这场对决中落败的一方必须无条件满足胜出者的一个要求，怎么样？」
				PRINTFORMDW 听起来不赖嘛…但你可要努力赢过我哦
				PRINTFORML
			ENDIF
			
			IF ABL:戦闘能力 > ABL:MASTER:戦闘能力
				PRINTFORMDW 在稍微上下打量了你一番后，魔理沙秀眉微蹙
				IF RAND:2
					PRINTFORMW 「喂，喂——我说啊，这样真的没问题么？」
					PRINTFORMW 「如果是正式的弹幕对决，你遇到的家伙可就不一定会像我这样留手了%K11_ZE("daze", 1)%～」
				ELSE
					PRINTFORMW 「并不是刻意挖苦你啦…但以你现在的实力，是没有可能打赢我的%K11_ZE()%～」
					PRINTFORMW 「这点可以相信我，你修炼得还远远不够呐」
				ENDIF
				PRINTFORMDW 你向魔理沙表示了即便如此自己也没有退缩的打算
				IF !RAND:3
					PRINTFORMW 「嘛…姑且给你个忠告吧，无谋之勇可算不上本事哦！」
				ELSEIF RAND:2
					PRINTFORMW 「…好吧，既然话都说到这份儿上了」
					PRINTFORMW 「就让我们认真地一决胜负好啦！」
				ELSE
					PRINTFORMW 「噢噢！很有种呢，我喜欢！」
					PRINTFORMW 「俗话说得好，没点莽撞还算什么勇气嘛！」
				ENDIF
			ELSEIF ABL:MASTER:戦闘能力 >= ABL:戦闘能力
				PRINTFORMW 「嘿嘿～很强呢，你这家伙！看来，这次的对手会稍微有点棘手%K11_ZE()%…」
				IF !(TALENT:MASTER:狐 || TALENT:MASTER:妖狐 || TALENT:MASTER:吸血鬼 || TALENT:MASTER:動物耳)
					PRINTFORMW 「不过也别得意得太早！像是你这样的强者，败在我手上的也已经不知有多少个了噢！」
					PRINTFORMW 「越是站在高处的家伙，摔落的时候就越是狼狈————这可是我在幻想乡中领悟到的真理%K11_ZE("daze")%！」
				ELSE
					PRINTFORMW 「不过也别得意得太早！像是你这样的大妖怪，败在我手上的已经不知有多少个了噢！」
					PRINTFORMW 「就让你好好见识一下吧！自己『退妖专家』的名号，可绝不是浪得虚名%K11_ZE("daze")%！」
				ENDIF
			ENDIF
			PRINTFORML
			PRINTFORML 「那么，要上咯！」
			PRINTFORML 「在重头戏上演前，先让你尝尝我的小型弹幕好了！」
			PRINTFORML %SPLIT_G("『恋符』 \"非定向激光\":『魔符』 \"银河\"")%!
		ENDIF
		CALLF SET_CEVENT("MarisaDanmakuFought", 11)
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;ハンデ
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "ハンデ"
		;ハンデつけなかったら表示されないので無記入でよい
		PRINTFORML 
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;戦闘中TARGET被弾時口上
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "被弾" 
		;別に一律で「やられたー」だけでもいいよ
		IF nSeriousMode == 1
			SELECTCASE 相手残機
				CASE 3
					PRINTFORML 「咕…！？」
					PRINTFORMDL 魔理沙的脸庞上闪过一抹讶色
					PRINTFORML 「意外的顽固嘛，你这家伙」
					PRINTFORML 「但没关系，用不了多久你就没戏唱了%K11_ZE("daze")%！」
				CASE 2
					PRINTFORML 「可恶，少在那里给我得意忘形了！！」
					PRINTFORML 「真正的较量才刚刚开始呢！」
				CASE 1
					PRINTFORML 「哈？！想让我放弃？少瞧不起人了%K11_ZE()%！！」
					PRINTFORML 「败给像你这样的家伙什么的…只有这个，我无论如何也不会接受！」
					PRINTFORML 「绝对！！不会输给你的%K11_ZE("daze")%！！！」
					HANDICAP_FIXED += 300
				CASE 0
					PRINTFORML 「呜哇啊啊啊啊啊啊啊啊！！！」
			ENDSELECT
		ELSE
			SELECTCASE 相手残機
				;残り残機3が表示されるのは【洽覧深識】持ちのみ
				CASE 3
					PRINTFORML 
				CASE 2
					SELECTCASE RAND:4
						CASE 0
							PRINTFORML 「哈啊…刚刚那下让我彻底找回感觉啦！」
						CASE 1
							PRINTFORML 「无论做什么，都必定全力以赴%K11_ZE()%！ 这可是我的原则！」
						CASE 2
							PRINTFORML 「很有干劲嘛，这才像话%K11_ZE()%！」
						CASE 3
							PRINTFORML 「就这么想要欣赏我华丽的符卡攻击吗？」
							PRINTFORML 「那就如你所愿%K11_ZE("daze")%！」
					ENDSELECT
					PRINTFORML %SPLIT_G("『魔符』 \"星尘幻想\":『黑魔』 \"黑洞边缘\":『恋心』 \"二重火花\":『魔空』 \"小行星带\"")%
				CASE 1
					LOCAL = IFRAND("0", ONCE("MarisaDanmaku1Life", 1), "1TO3", 1, "4", !FIRSTTIME("MarisaMima", 1))
					SELECTCASE LOCAL
						CASE 0
							PRINTFORML 「开玩笑的吧…你这家伙居然还没倒下！？」
							PRINTFORML 「看来不动用点真本事是没法击垮你了！！」
							PRINTFORML 「是时候放开手脚开干了%K11_ZE()%！ 尝尝我的最终奥义，绝对火力的巅峰！！！」
							CALLF ONCE("MarisaDanmaku1Life")
							SIF ABL:MASTER:戦闘能力 >= ABL:戦闘能力
								HANDICAP_RAND += 150
						CASE 1
							PRINTFORML 「咕哈…！不错，相当不错嘛，%CALLNAME:MASTER%！」
							PRINTFORML 「但是，自己在幻想乡仅次于博丽巫女的地位是无法被撼动的%K11_ZE("daze")%！！！」
							PRINTFORML 「就像我常说的，如果努力一次不成…那就更拼一点！」
							PRINTFORMDL 魔理沙正在采取强硬的正面突破战术！
							PRINTFORML 「BLAZING STARRRRRR！！！」
							SIF ABL:MASTER:戦闘能力 >= ABL:戦闘能力
								HANDICAP_RAND += 100
						CASE 2
							PRINTFORML 「呃…别逼我对你发难啊！」
						CASE 3
							PRINTFORML 「别小看我%K11_ZE()%！」
						CASE 4
							PRINTFORML 「这个是师傅亲手交给我的信物！」
							PRINTFORML 「能逼迫我用出这件武器来对付的家伙可不多，感到荣幸吧！」
							PRINTFORML 「『天仪』 太阳系仪！！！」
							SIF ABL:MASTER:戦闘能力 >= ABL:戦闘能力
								HANDICAP_RAND += 200
					ENDSELECT
					SIF !GROUPMATCH(LOCAL, 0, 1, 4)
						PRINTFORML %SPLIT_G("『恋风』 \"星光台风\":『光符』 \"地球光\":『光击』 \"射月\"")%！！！
				CASE 0
					PRINTFORML 
			ENDSELECT
		ENDIF
	ENDIF
	;-------------------------------------------------
	;戦闘中
	;TARGETの【残忍酷薄】発動時
	;ARGはTARGETの残機
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "残忍酷薄"
		
	ENDIF
	;-------------------------------------------------
	;戦闘中
	;TARGETの【乾坤一擲】発動時
	;ARGはTARGETの残機
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "乾坤一擲"
		IF nSeriousMode ;反三沙沙
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「『恋符』MASTEEEERRR——————SPAAAAAAAARK！！！」
					PRINTFORMDL 嗡嗡嗡嗡嗡嗡————！！！
					CALL QUAKE(4)
					PRINTFORMDL 一道粗大的，无比耀眼的光束射向了你
					PRINTFORMDL …尽管你能避免被那道可怕的光束直接命中，它边缘的高温或许也会对你造成不小的伤害…
					HANDICAP_FIXED += 350
				CASE 1
					PRINTFORML 「『星符』龙星群！！！」
					PRINTFORMDL 轰隆隆隆隆！！！
					CALL QUAKE(4)
					PRINTFORMDL 一道刺眼的巨大激光从天而降
					PRINTFORMDL …迎头撞上它的话，免不了要遭受重创吧…
					HANDICAP_FIXED += 200
				CASE 2
					PRINTFORML 「『魔炮』FINAL——————SPAAAAAAAAARK！！！」
					PRINTFORMDL 嘭————轰隆隆隆隆隆隆隆隆！！！
					CALL QUAKE(4)
					PRINTFORMDL 一道巨大到难以想象的，闪耀到直令双眼刺痛的可怖光束径直击向了你
					PRINTFORMDL …你只感到死里逃生从未显得如此困难过…
					HANDICAP_FIXED += 500
			ENDSELECT
		ELSE
			IF TALENT:恋慕 && 相手残機 == 1
				PRINTFORMW 「第一步，屏息凝神…」
				PRINTFORMW 「第二步，轻唤对方的名字…」
				PRINTFORMW 「第三步，将它对准所爱之人…」
				PRINTFORMW 「第四步，释放那究极的毁灭魔力！！！」
				PRINTFORMW 『魔炮…』
				PRINTFORMW 「★超究极火花%COND_STR(@" dazeeeeeeeeee", CFLAG:11:K11EN_ZE)%！！！★」
				HANDICAP_FIXED += 300
			ELSEIF ABL:戦闘能力 > ABL:MASTER:戦闘能力
				PRINTFORML 「好吧，也该是不再收手的时候了！ MASTER——SPAAARK…！！只有炉火威力的那种啦…」
				HANDICAP_FIXED -= 100
			ELSE
				PRINTDATAL
					DATAFORM 「该为这场战斗画上休止符了！！」
					DATAFORM 「压倒性的火力…才是魔法使的唯一真理%K11_ZE("daze")%！」
					DATAFORM 「哈哈哈！来一起大闹一场吧！」
					DATAFORM 「哼哼，也到了该用『那个』的时候了！」
					DATAFORM 「嘿嘿～这是送给你的一份大礼！」
					DATAFORM 「老实接下我这招吧！」
					DATAFORM 「你要有大麻烦了%K11_ZE("daze")%，%CALLNAME:MASTER%！」
					DATAFORM 「如果我是你的话，接下来可就要当心了！」
					DATAFORM 「就是现在！！」
				ENDDATA
				PRINTFORML 「『恋符』究极火花！！！」
			ENDIF
			SIF ABL:MASTER:戦闘能力 >= ABL:戦闘能力
				HANDICAP_RAND += 100
		ENDIF
	ENDIF
	;-------------------------------------------------
	;戦闘中
	;TARGETの【怪力乱神】発動時
	;ARGはTARGETの残機
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARGS == "怪力乱神"
		
	ENDIF
	
	IF ARGS == "Pichun"
		;RESULT:8 是你先生的残机
		
		IF nSeriousMode ;反三沙沙
			SELECTCASE RESULT:8
				CASE 3
					IF 相手残機 >= 4 ;沙沙残机为满时
						PRINTFORML 「哦？真有趣，你看起来似乎挺难堪呢，简直就像热锅上的蚂蚁一样～」
						PRINTFORML 「倍感荣幸～我最喜欢欣赏自己的对手费力挣扎的样子了，如果是用别的什么方式我反而提不起兴致呐」
						PRINTFORML 「但很快你就会明白，在真正的主人公面前，一切的抵抗都是徒劳的%K11_ZE("daze")%！」
					ELSEIF 相手残機 >= 2 && 相手残機 <= 3
						PRINTFORMW 「哇喔～刚刚那下肯定很痛吧！」
					ELSE
						PRINTFORMW 「可恶…总算打中你这家伙了！」
					ENDIF
				CASE 2
					IF 相手残機 >= 4
						PRINTFORML 「怎样？感觉如何呀？」
						PRINTFORMDL 然而，你并不打算放弃！无论她耍什么手段，再怎么不按套路出牌…也终究称不上不可战胜的对手！
						PRINTFORML 「是这样吗？可我其实还没有用出全部的实力哦？」
						PRINTFORML 「既然我在用投机取巧的手段对付你，那你就更没理由要输掉了，不是么？」
						PRINTFORMDL 她丝毫不打算留情面的样子！
					ELSE
						PRINTFORMW 「别想逃%K11_ZE("daze")%！！」
					ENDIF
				CASE 1
					IF 相手残機 >= 4
						PRINTFORML 「还没完呢！！」
						PRINTFORMDL 你刚重整完态势，正想喘口气时，魔理沙的魔炮又一次轰了过来！
						IF ABL:MASTER:戦闘能力 >= 3
							PRINTFORMDL 她是对的…再怎么说，过去经历的那些弹幕战，本质上也终归是一种游戏罢了
							PRINTFORMDL 一旦失去了弹幕规则的制约，在魔理沙这位实至名归的异变解决专家面前，你根本讨不到半分便宜！
						ELSE
							PRINTFORMDL 你不禁开始后悔自己不自量力挑衅魔理沙的决定…
						ENDIF
						PRINTFORML 「怎么了？脸色看起来不太好呢，%CALLNAME:MASTER%？」
						PRINTFORML 「像你现在这副不争气的样子，要怎么才能赢我%K11_ZE()%～」
					ELSEIF 相手残機 >= 2 && 相手残機 <= 3
						PRINTFORML 「哼哼，亏你能坚持到现在呢，这点倒是值得稍微夸赞一下～」
						PRINTFORML 「但很可惜，大局已定了%K11_ZE()%！」
						PRINTFORMDL 魔理沙握紧了手中的八卦炉，似乎准备随时将你击落！
					ELSE
						PRINTFORML 「呼唔…哈啊…哈啊…怎么样！？」
						PRINTFORMDL 刚刚发射魔导炮击中了你的魔理沙，正在空中大口喘着粗气，看样子疲惫不堪了
						PRINTFORMDL 可就算如此，她也依旧紧咬着牙关，用充满倔强的眼神望着不远处同样摇摇欲坠的你！
						PRINTFORMDL 半晌的静默过后，你们缠绕着灵力的手臂再度不约而同地朝对方抬起————
						PRINTFORMDL 你相当清楚…接下来便是最终的时刻了…！！
					ENDIF
			ENDSELECT
		ELSE
			SELECTCASE RESULT:8
				CASE 2
					IF RAND:2
						PRINTFORML 「畏怯吧！颤抖吧！你的这份恐惧将被永远地供奉于我，库库库…！」
						IF !(TALENT:MASTER:狐 || TALENT:MASTER:妖狐 || TALENT:MASTER:吸血鬼 || TALENT:MASTER:動物耳)
							PRINTFORML 「嘿嘿～怎么样，魔理沙大人的邪恶妖怪形象？一定把你震慑到了%K11_ZE("daze")%～」
						ELSE
							PRINTFORML 「嘿嘿～怎么样，魔理沙大人的邪恶妖怪形象？完全不比你差劲对吧？」
						ENDIF
					ELSE
						PRINTFORML 「尝尝我的…」
						PRINTFORML 「雾雨魔理沙式————铁★尻★冲★击！！！」
						PRINTFORMDL 哦哦哦哦哦哦哦！！！
						PRINTFORMDL 魔理沙用屁股把你狠狠地撞飞了出去！
						PRINTFORMDL 你只感觉自己的身体仿佛被一记大摆锤击中了
						PRINTFORMDL 这家伙究竟是如何练就出这种本领的啊？！
					ENDIF
				CASE 1
					IF 相手残機 > RESULT:8
						PRINTFORML 「啊呀，这已经算不上是一场势均力敌的较量啦！」
						PRINTFORML 「我突然有点后悔把你当作对手了%K11_ZE()%～」
					ELSE
						PRINTFORML 「哼哼哼…看样子我们两个都只剩下最后一息了」
						PRINTFORML 「那么，接下来就是决胜的时刻…让我们一同见证究竟谁才会笑到最后吧，%CALLNAME:MASTER%！」
					ENDIF
			ENDSELECT
		ENDIF
