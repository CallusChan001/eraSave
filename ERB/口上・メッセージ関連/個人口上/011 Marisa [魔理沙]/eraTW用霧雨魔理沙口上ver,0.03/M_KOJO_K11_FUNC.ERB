@K11_ZE(ARGS, ARG, ARG:1)
#FUNCTIONS
SELECTCASE ARGS
	CASE "daze"
		SIF !CFLAG:11:K11EN_ZE
			RETURNF
		LOCALS = da★ze
CASEELSE
	SIF !CFLAG:11:K11EN_ZE
		RETURNF
	LOCALS = ze
ENDSELECT
SIF ARG:1
	LOCALS '= CAPITALIZE(LOCALS)
IF ARG == 1
	RETURNF LOCALS
ELSEIF ARG == 2
	RETURNF LOCALS + "…"
ELSEIF ARG == 3
	RETURNF LOCALS
ELSEIF ARG == 4
	RETURNF "…" + LOCALS
ELSEIF ARG == 5
	RETURNF "…da…ze"
ELSE
	RETURNF "，" + LOCALS
ENDIF

