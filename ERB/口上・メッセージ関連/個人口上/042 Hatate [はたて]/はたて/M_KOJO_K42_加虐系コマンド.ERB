;100,打屁股
;==================================================
@M_KOJO_MESSAGE_COM_K42_100
CALL M_KOJO_MESSAGE_COM_K42_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K42_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMDL 乓——！
		IF TALENT:恋慕 || TALENT:恋人
			PRINTFORML 「好痛!%MASTERNAME:42%,你在干什么啊～～」
			PRINTDW 
			PRINTFORMDL 屁股真的很漂亮,忍不住…
			PRINTFORML 「打女孩子的屁股,你也太过分了!」
			PRINTDW 
		ELSE
			PRINTFORML 「好痛!你在干什么啊!」
			PRINTDW 
			PRINTFORMDL 屁股真的很漂亮,忍不住…
			PRINTFORML 「不是'忍不住'吧!真是胡闹!」
			PRINTDW 
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMDW 
		RETURN 1
	ELSEIF TALENT:愛欲 || TALENT:炮友
		IF TALENT:TARGET:受虐狂 == 1 || ABL:TARGET:受虐属性 >= 2
			IF TEQUIP:Ｖ性交
					PRINTFORML 「啊♥ 哈♥ 好棒♥ 再多一些♥」
					PRINTFORMDL 在果沉浸其中的性交中,我不由自主地揉捏着她的臀部
					PRINTDW 
					PRINTFORMDL 触感真是太舒服了。她有一个很好的屁股…
					PRINTFORMDL 几乎是不经大脑,我用手掌拍打了一下果的臀部,发出清脆的响声
					PRINTDW 
					PRINTFORML 「哇啊♥」
					PRINTFORML 「啊…不,不是,刚才只是被插入的感觉太舒服了…」
					PRINTDW 
					PRINTFORMDL 虽然果这么解释,但很明显那声音并不是因为抽插带来的
					PRINTFORMDL 看来果是个喜欢被打屁股的变态啊
					PRINTDW 
			ELSEIF TEQUIP:Ａ性交
			ELSE
			ENDIF
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMDL 啪
				PRINTFORML 「好痛。不要那么随便就拍女孩子的屁股好不好」
				PRINTDW 
			CASE 1
				PRINTFORMDL 啪!
				PRINTFORML 「好痛!你在干什么啊!」
				PRINTDW 
				PRINTFORMDL 啪啪!
				PRINTFORML 「好痛!所以说,住手…」
				PRINTDW 
				PRINTFORMDL 啪啪啪!
				PRINTFORML 「啊!?痛!?你再这样我可不会饶你!…」
				PRINTDW 
				PRINTFORMDL 啪啪啪啪!
				PRINTFORML 「够了你这个笨蛋!!!!」
				PRINTDW 
				PRINTFORMDL 吧唧————！
				PRINTFORMDL 果狠狠还了一击
			ENDSELECT
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMDW 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

