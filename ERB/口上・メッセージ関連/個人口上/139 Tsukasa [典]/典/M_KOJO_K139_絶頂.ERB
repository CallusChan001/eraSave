								CALL HPH_PRINT,@"「啊啊HPH　典想要懷孕的卵子HPH　往%CALLNAME:MASTER%大人火熱的子種奔去了～HPH」","L"
							CASE 4
								CALL HPH_PRINT,@"「懷孕HPH　要懷孕了HPH　請原諒我HPH　哦～哦哦～HPH」","L"
							CASE 5
								CALL HPH_PRINT,@"「明明是排卵日的HPH　卻還如此豪不留情的中出HPH　好喜歡啊～HPH」","L"
							CASE 6
								CALL HPH_PRINT,@"（啊啊～HPH　在排卵日被注入新鮮的子種了～HPH）","L"
								CALL HPH_PRINT,@"（讓我感受到我是屬於這个人的HPH　感受到了被愛著HPH）","L"
							CASE 7
								
							ENDSELECT
						ENDIF
					;恋慕・恋人以外
					ELSE
						SELECTCASE RAND:2
						CASE 0
							CALL HPH_PRINT,@"「啊啊嗯HPH　\@ (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行) ? 姐姐 # 典 \@毫無防備的卵子、被子種狠狠的強暴了～HPH」","L"
						CASE 1
							CALL HPH_PRINT,@"「啊啊～HPH　懷孕HPH　要被迫懷孕了～HPH」","L"
						CASE 2
							CALL HPH_PRINT,@"「啊啊嗯HPH　\@ (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行) ? 姐姐 # 典 \@、今天是排卵日哦HPH」","L"
						ENDSELECT
					ENDIF
				ELSEIF CFLAG:生理周期 == 1 || CFLAG:生理周期 == 2
					SELECTCASE RAND:6
					CASE 0
						CALL HPH_PRINT,@"「不要HPH　拔出來HPH　就這樣在全部陰道内HPH」","L"
					CASE 1
						CALL HPH_PRINT,@"「啊…HPH　在體内…HPH」","L"
					CASE 2
						CALL HPH_PRINT,@"「即使是安全期HPH　頂著子宮射精什么的HPH　還是不可以HPH　這樣也是會懷孕的HPH」","L"
					CASE 3
						CALL HPH_PRINT,@"「哈啊HPH　僅僅知道是安全期、就如此豪不留情播種HPH」","L"
					CASE 4
						CALL HPH_PRINT,@"「啊ー啊HPH　僅僅知道是安全期、就如此不負責任地生中出進去HPH」","L"
					CASE 5
						CALL HPH_PRINT,@"「唔嘸HPH　生中出、舒服嗎HPH」","L"
					CASE 6
						CALL HPH_PRINT,@"「嘸嘸HPH　今天可以盡情的生中出進去哦HPH」","L"
					ENDSELECT
				ELSE
					IF TALENT:恋慕 || TALENT:恋人
						SELECTCASE RAND:7
						CASE 0
							CALL HPH_PRINT,@"「好高興HPH　無套的中出HPH」","L"
						CASE 1
							CALL HPH_PRINT,@"「被%CALLNAME:MASTER%大人的肉棒HPH　中出了HPH　好开心HPH」","L"
						CASE 2
							CALL HPH_PRINT,@"「來吧HPH　对著典的孕帶HPH　給予子種的恩惠HPH」","L"
						CASE 3
							CALL HPH_PRINT,@"「好～HPH　會接住的HPH　會將%CALLNAME:MASTER%大人的精子全部都接住的HPH」","L"
						CASE 4
							CALL HPH_PRINT,@"「啊噫HPH　明明不是安全期HPH　卻被内射在里面了HPH」","L"
						CASE 5
							CALL HPH_PRINT,@"「%CALLNAME:MASTER%大人的全部HPH　請給予典吧HPH　哦～哦哦～HPH」","L"
						CASE 6
							IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行)
								CALL HPH_PRINT,@"「射出來吧HPH　將小孩子的精液HPH　多多地灑在姐姐的體内吧HPH」","L"
							ELSE
								CALL HPH_PRINT,@"「好的～HPH　典的子宮HPH　是%CALLNAME:MASTER%大人的專用精子採集袋～HPH」","L"
							ENDIF
						ENDSELECT
					ELSE
						SELECTCASE RAND:5
						CASE 0
							CALL HPH_PRINT,@"「啊嗯HPH　在陰道里射了這麼多出來真的可以嗎HPH」","L"
							CALL HPH_PRINT,@"「小寶寶、可能會懷上的、嗯嗯～HPH」","L"
						CASE 1
							CALL HPH_PRINT,@"「啊嗯HPH　典的子宮HPH　被當作射精袋了～HPH」","L"
						CASE 2
							CALL HPH_PRINT,@"（今天、應該會没事的吧…算了、只要舒服就無所謂了…HPH）","L"
						CASE 3
							CALL HPH_PRINT,@"（嘿嘿、明明没打算讓你内射的、但只要跟你無套做愛就會变成了呢HPH）","L"
						CASE 4
							CALL HPH_PRINT,@"「啊HPH　好熱HPH　射了好多HPH　在\@ (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行) ? 姐姐 # 典 \@的陰道里HPH」","L"
							CALL HPH_PRINT,@"（啊啊～HPH　無套肉棒在體内射精HPH　這个感觉好舒服啊HPH）","L"
						ENDSELECT
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF
RETURN 1

