;100,打屁股
;==================================================
@M_KOJO_机翻更新版_MESSAGE_COM_K139_100
CALL M_KOJO_机翻更新版_MESSAGE_COM_K139_100_1
RETURN RESULT

@M_KOJO_机翻更新版_MESSAGE_COM_K139_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:発情
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行||CFLAG:139:1050 == 1)
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「嗯HPH」","L"
				CALL HPH_PRINT,@"「不行HPH 不要打姐姐的屁股HPH」","L"
				RESETCOLOR
				PRINTL
				PRINTFORMDL 啪！啪！啪！
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「嗯啊HPH 痛HPH 啊HPHHPH」","L"
				CALL HPH_PRINT,@"「好过分…HPH 不要欺负姐姐呀…HPH」","L"
				PRINTL
				RESETCOLOR
				PRINTFORMDL 因为发情，典的身体敏感度被无限放大，连痛感也被强制转换成快感
				PRINTFORMDL 被打屁股的冲击像快感的波浪一样传到了子宫
				PRINTW
			ELSE
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「咿咿唔HPH　咿呀HPH　咿咿HPHHPH」","L"
				RESETCOLOR
				PRINTFORMDL 每次当啪、啪的生硬的声音响起时、典的娇声也会一并响起
				PRINTW
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「明明很痛HPH　明明很痛但是又好舒服HPH　怎么会HPH」","L"
				RESETCOLOR
				PRINTFORMDL 由于发情导致敏感度高到极致的典的身体、连痛感都会被强制替换成快感
				PRINTFORMDL 屁股被拍打的冲击化作快感的浪潮、一直传递到子宫
				PRINTW
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「不要HPH　不要打了HPH　疼HPH　啊咕HPH　啊啊ーHPH　啊啊ーHPH」","L"
				RESETCOLOR
				PRINTW
			ENDIF
		ELSE
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行||CFLAG:139:1050 == 1)
				IF ABL:受虐属性 >= 2
					CALL HPH_PRINT,@"「哦哦哦唔HPH」","W"
					PRINTFORML 「啊、刚才的是…不是的…姐姐、并没有很舒服什么的…」
					PRINTW
					PRINTFORMDL %CALLNAME:MASTER%一言不发的继续挥动着手
					CALL HPH_PRINT,@"「咿咿咿咿HPH」","W"
					CALL HPH_PRINT,@"「不、不行HPH　很痛的、请、请停下、咿咿唔HPH」","L"
					PRINTW
				ELSE
					PRINTFORML 「哦哦哦唔！！？」
					PRINTFORML 「等、等下…打屁股的话…哦哦哦唔！！」
					PRINTFORML 「姐姐、要生气了、哦哦！」
					PRINTFORML 「很、很痛的、别再打了、啊！」
					PRINTW
				ENDIF
			ELSE
				IF ABL:受虐属性 >= 2
					CALL HPH_PRINT,@"「哦哦哦唔HPH」","W"
					PRINTFORML 「刚、刚才是意外、很舒服、什么的哦哦哦哦哦唔！！？」
					PRINTW
					PRINTFORMDL 每在典的屁股上拍打两三下、圆润的屁股上就会浮现出一片红叶
					CALL HPH_PRINT,@"「不要HPH　不能打屁股HPH　说了很痛HPH　哦哦哦哦唔！！」","L"
					PRINTW
				ELSE
					PRINTFORML 「哦哦哦唔！！？」
					IF GROUPMATCH(TALENT:MASTER:体型,2,3)
						PRINTFORML （好痛好痛好痛！！？　真的好痛！！！）
						PRINTW
						PRINTFORMDL 啪唧！
						PRINTFORML 「咿叽咿咿！！？」
						PRINTW
						PRINTFORML 「啊、啊…%CALLNAME:MASTER%阁下、请住手…」
						PRINTFORMDL 典眼角噙着泪花、不含伪装的剧烈颤抖着
						PRINTW
					ELSE
						PRINTFORML 「等、等下…打屁股的话…哦哦哦唔！！」
						PRINTFORML 「痛、请住手、不要、哦哦哦唔！」
						PRINTW
					ENDIF
				ENDIF
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW
		RETURN 1
	;それ以外
	ELSE
		IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行||CFLAG:139:1050 == 1)
			IF ABL:受虐属性 >= 2
				SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「哦齁！HPH 再多！HPH」","L"
					CALL HPH_PRINT,@"「姐姐…好像被打得觉醒了…HPH」","L"
					PRINTW
				CASE 1
					CALL HPH_PRINT,@"「哦齁！HPH」","L"
					PRINTFORML 「喂、喂！不许打姐姐的屁股…不、不可以哦…」
					PRINTL
					PRINTFORMDL 啪！
					CALL HPH_PRINT,@"「哦齁！！！HPH」","L"
					PRINTL
					PRINTFORML 「姐、姐姐的…」
					PRINTL
					PRINTFORMDL 啪！
					CALL HPH_PRINT,@"「屁股！HPH 不许！HPH」","L"
					PRINTL
					PRINTFORMDL 啪！
					CALL HPH_PRINT,@"「玩弄啊！HPH 不要啊！HPH」","L"
					PRINTW
				ENDSELECT
			ELSE
				PRINTFORMDL 啪！
				SELECTCASE RAND:6
				CASE 0
					PRINTFORML 「姐姐，好痛！真的，很痛啦！」
				CASE 1
					PRINTFORML 「现在原谅你！住手，哦齁！」
				CASE 2
					PRINTFORML 「喂、喂！不许把姐姐的屁股当鼓敲！」
				CASE 3
					PRINTFORML 「啊，你给我等着！哦齁！」
				CASE 4
					PRINTFORML 「你、你再不停手我真的要生气了哦！」
				CASE 5
					IF (TALENT:MASTER:応答 == 1 || TALENT:MASTER:態度 == 1)
						PRINTFORML 「你、你这臭小鬼！你给我等着！哦齁！」
					ELSE
						PRINTFORML 「怎、怎么回事啊！？你不是会做这种事的孩子吧！？哦齁！」
					ENDIF
				ENDSELECT
				PRINTW
			ENDIF
		ELSE
			IF ABL:受虐属性 >= 2
				CALL HPH_PRINT,@"「哦齁HPH」","L"
				PRINTFORML 「啊，刚才那个，不是的…不是舒服的意思…」
				PRINTL
				PRINTFORMDL 啪！
				CALL HPH_PRINT,@"「哦齁HPH」","L"
				PRINTFORMDL 明显和痛苦不同的声音混合在惨叫中
				PRINTFORMDL 更重要的是，从秘所流出的爱液量明显增加，这无声地说明了一切
				PRINTW
			ELSE
				PRINTFORMDL 啪！
				PRINTFORML 「哦齁！？」
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「为什么对我做这种事，嗯！？ 痛!」
					PRINTW
				CASE 1
					PRINTFORML 「呃! 我都说痛了…」
					PRINTW
				CASE 2
					PRINTFORML 「嗯，好痛!好痛!!住手!」
					PRINTW
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
	RETURN 1
ENDIF
RETURN 1

