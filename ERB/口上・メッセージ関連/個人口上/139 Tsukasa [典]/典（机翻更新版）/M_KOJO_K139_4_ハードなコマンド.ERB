				PRINTFORMDL 典张开嘴，伸出舌头，做好了接受的准备。
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「请吧……HPH」","L"
				PRINTW
				RESETCOLOR
				PRINTFORMDL 噗溜！
				CALL HPH_PRINT,@"啪啾HPH 啾啵HPH 啾啵HPH 滋啵HPH","L"
				PRINTL
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"「嗯咕HPH 嗯呶HPHHPH 喔咕HPH 嗯啵HPHHPH」","L"
				RESETCOLOR
				PRINTFORMDL 典虽然粗暴地被肉棒顶入口腔，但她的表情却充满了喜悦……
				PRINTL
				SETCOLOR 0xFF69B4
				CALL HPH_PRINT,@"（好高兴啊！HPH 你在姐姐的嘴巴里感到那么舒服呢！HPHHPH）","L"
				CALL HPH_PRINT,@"（没关系的！HPH 专属于你的口腔小穴HPHHPH 尽情侵犯吧HPH）","L"
				CALL HPH_PRINT,@"（在姐姐身上留下你的印记吧HPHHPH）","L"
				RESETCOLOR
				PRINTW
			ELSE
				SELECTCASE RAND:2
				CASE 0
					PRINTFORMDL 啾! 啾! 啾! 啾! 啾!
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"「%TEXTR("嗯啊HPH/呃嗯HPH/呜噢HPH/嗯哼HPH/啊HPH")%%TEXTR("嗯啊HPH/呃嗯HPH/呜噢HPH/嗯哼HPH/啊HPH")%%TEXTR("嗯啊HPH/呃嗯HPH/呜噢HPH/嗯哼HPH/啊HPH")%」","L"
					CALL HPH_PRINT,@"（喉咙被狠狠插入HPH 太痛苦了HPHHPH）","L"
					CALL HPH_PRINT,@"（很痛但发情期的我很享受HPH 每一次插入都高潮HPHHPH）","L"
					PRINTL
					RESETCOLOR
					PRINTFORMDL 典的手向秘处伸去。那里已经完全湿透了
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"「呼ーーーーーHPH 呼ーーーーーHPH 呼ーーーーーーHPHHPH」","L"
					RESETCOLOR
					PRINTFORMDL 典粗暴地用手指搅动阴道。就像在说即使粗暴也没关系，只要能让我更兴奋就好
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"（即使粗暴也没关系HPH 更多疼爱我HPHHPH 用典的身体获得快感HPHHPH)","L"
					RESETCOLOR
					PRINTW
				CASE 1
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"「把典仰面朝天，接下来要对我做什么呢…噫！？HPH」","L"
					RESETCOLOR
					PRINTFORMDL 不理会典的疑问，%CALLNAME:MASTER%将恶意的肉棒插入她的口腔
					PRINTW
					SETCOLOR 0xFF69B4
					CALL HPH_PRINT,@"「呜嗯HPH 嗯呃HPHHPH 呜噢HPH 嗯唔HPHHPH」","L"
					CALL HPH_PRINT,@"（那么激烈HPH 嘴巴子宫HPHHPH 都要被玩坏了HPH）","L"
					PRINTL
					RESETCOLOR
					PRINTFORMDL 典的头被固定住，像按摩棒一样被使用
					PRINTFORMDL 但是，看到心爱的人兴奋地摆动腰部，痛苦比幸福感更胜一筹，似乎是这样
					PRINTW
				ENDSELECT
			ENDIF
		ELSE
			IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行||CFLAG:139:1050 == 1)
				CALL HPH_PRINT,@"「好勒HPH %CALLNAME:MASTER%阁下的欧金金请往这边来HPHHPH」","L"
				PRINTFORMDL 典在嘴前做出一个圈，垂下沾着唾液的舌头，渴望着肉棒的插入。
				PRINTW
				CALL HPH_PRINT,@"啵……黏黏黏……哧溜！HPH","L"
				CALL HPH_PRINT,@"「嗯呵呵HPH 嗯哼，嗯，好像进去了呢……HPHHPH」","L"
				PRINTFORMDL 典用舌头温柔而细致地爱抚着%CALLNAME:MASTER%的阴茎。
				PRINTL
				CALL HPH_PRINT,@"「嗯哼，哦哼～，好像，没关系……HPH」","L"
				PRINTFORMDL 任由典摆布，腰部前后摇摆着。
				PRINTL
				CALL HPH_PRINT,@"啪啾HPH啪啾HPH啪啾HPH啪啾HPH","L"
				CALL HPH_PRINT,@"「%TEXTR("呜噗HPH/哦啵HPH/嗯啵HPH/嗯姆HPH")%%TEXTR("呜噗HPH/哦啵HPH/嗯啵HPH/嗯姆HPH")%%TEXTR("呜噗HPH/哦啵HPH/嗯啵HPH/嗯姆HPH")%」","L"
				PRINTL
				CALL HPH_PRINT,@"（呵呵HPH 腰摇得那么舒服……HPH 那么喜欢我的嘴巴啊……HPH 可爱的孩子HPH）","L"
				PRINTFORMDL 即使口腔被侵犯，典也露出慈母般的笑容，注视着%CALLNAME:MASTER%的行为……
				PRINTW
			ELSE
				CALL HPH_PRINT,@"「好勒HPH 是要口交吗HPH 乐意效劳HPHHPH」","L"
				PRINTFORMDL 典高兴地想要将%CALLNAME:MASTER%的肉棒含入口中。
				PRINTL
				PRINTFORMDL 啪叽！
				PRINTFORML 「哈？」
				PRINTFORMDL 典发出怪叫的下一瞬间，%CALLNAME:MASTER%的肉棒就深深地顶进了典的口腔深处。
				PRINTW
				PRINTFORMDL 噗嗤！噗嗤！噗嗤！噗嗤！噗嗤！噗嗤！
				PRINTFORML 「哦咕！？嗯噗！嗯啵！嗯啵噢！」
				PRINTFORMDL 典眼珠打转，却仍拼命张开嘴，努力不让牙齿碰到阴茎，接受着这凶猛的深喉抽插。
				PRINTL
				CALL HPH_PRINT,@"（%CALLNAME:MASTER%阁下，多么，热情啊！HPH）","L"
				CALL HPH_PRINT,@"（头晕目眩HPH 嘴里充满了欧金金的味道HPH）","L"
				PRINTFORMDL 对典来说，即使是被像物品一样对待的行为，似乎也充满了爱意和愉悦……
				PRINTW
			ENDIF
		ENDIF
		RETURN 1
	;それ以外
	ELSE
		IF (TALENT:MASTER:年齢 == -1 || TALENT:MASTER:幼児／幼児退行||CFLAG:139:1050 == 1)
			SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"啪啾HPH啪啾HPH啪啾HPH啪啾HPH","L"
				CALL HPH_PRINT,@"「%TEXTR("呜噗HPH/哦啵HPH/嗯啵HPH/嗯姆HPH")%%TEXTR("呜噗HPH/哦啵HPH/嗯啵HPH/嗯姆HPH")%%TEXTR("呜噗HPH/哦啵HPH/嗯啵HPHHPH/嗯姆HPH")%」","L"
				PRINTL
				CALL HPH_PRINT,@"（%CALLNAME:MASTER%的……HPH 嘴里%CALLNAME:MASTER%的味道好多HPH）","L"
				CALL HPH_PRINT,@"（嘴里被涂满了，头都晕乎乎的……HPH）","L"
				PRINTW
			CASE 1
				CALL HPH_PRINT,@"「来……用姐姐的这里吧……HPH」","L"
				CALL HPH_PRINT,@"「虽然是嘴巴，但现在叫『口腔小穴』哦……HPH」","L"
				PRINTL
				CALL HPH_PRINT,@"「既然是雌穴，就不能手下留情哦……HPH」","L"
				CALL HPH_PRINT,@"「要用自己的腰，狠狠地侵犯，然后把舒服的%TEXTR("咻咻/噗嗤噗嗤/哔哔")%射到姐姐这里HPH」","L"
				PRINTFORMDL 典垂下舌头，右手做成一个圈，模仿着撸动的样子。
				PRINTW
				CALL HPH_PRINT,@"啾……咕噜噜噜噜～～～～～～HPH","L"
				IF TALENT:MASTER:形状 == 6 || TALENT:MASTER:形状 == 10
					CALL HPH_PRINT,@"「哦啵HPH 哦咕欸HPHHPH」","L"
					PRINTL
					CALL HPH_PRINT,@"（一如既往的鸡巴好大……HPH 稚气未脱的鸡巴呢……HPH）","L"
					CALL HPH_PRINT,@"（被这种东西顶进去，大多数女人都会变成雌兽吧……HPH）","L"
				ELSEIF TALENT:MASTER:形状 == 1 || TALENT:MASTER:形状 == 2
					CALL HPH_PRINT,@"「嗯噗HPH 嗯HPHHPH」","L"
					PRINTL
					CALL HPH_PRINT,@"（呵呵……HPH 小小的鸡巴，拼命想深入到底好可爱……HPH）","L"
					CALL HPH_PRINT,@"（拼命想主张自己是雄性HPH）","L"
				ELSE
					CALL HPH_PRINT,@"「嗯摸HPH 哦啵HPHHPH」","L"
					PRINTL
					CALL HPH_PRINT,@"（呵呵，勃起得有模有样……HPH）","L"
					CALL HPH_PRINT,@"（拼命地想把鸡巴的味道教给我，使劲往里推……HPH）","L"
				ENDIF
				PRINTL
				CALL HPH_PRINT,@"啪啾HPH啪啾HPH啪啾HPH啪啾HPH","L"
				CALL HPH_PRINT,@"「呜噗HPH 哦啵HPH 嗯啵HPH 嗯姆HPH」","L"
				CALL HPH_PRINT,@"（被年幼的孩子强行侵犯嘴巴的感觉……意外地，还不赖呢HPH）","L"
				PRINTW
			CASE 2
				CALL HPH_PRINT,@"「呼～～～～HPH 呼～～～～HPH」","L"
				PRINTFORMDL 典嘴角流着涎水，吸吮着%CALLNAME:MASTER%的龟头……
				PRINTL
				IF TCVAR:MASTER:射精我慢 > 0
					CALL HPH_PRINT,@"（这么拼命地摇着腰，却又忍着不射精，真是惹人怜爱HPH）","L"
					CALL HPH_PRINT,@"（哪怕多一点点，也想继续品尝姐姐舒服的口腔小穴吧HPH）","L"
					IF TCVAR:MASTER:要去了 == 1
						PRINTL
						CALL HPH_PRINT,@"(但是，你快要高潮了吧？HPH 马上就要射了吧？HPH)","L"
						CALL HPH_PRINT,@"(因为，你的前端已经渗出了精液的味道HPH)","L"
						CALL HPH_PRINT,@"(没关系的HPH 就在姐姐的小嘴里面HPH 内射，让喉咙受孕HPHHPH)","L"
					ENDIF
					PRINTW
				ELSE
					IF TCVAR:MASTER:要去了 == 1
						CALL HPH_PRINT,@"（呵呵，明明被折磨的是我，却没有余裕的竟然是%CALLNAME:MASTER%……HPH）","L"
						CALL HPH_PRINT,@"（前端的精液味道越来越浓了HPH 快要射出来了吧HPH 想射出来吧HPH）","L"
						CALL HPH_PRINT,@"（没关系的HPH 不要忍耐HPH 饱满的精液HPH 射进姐姐的身体里吧HPHHPH）","L"
						PRINTW
					ELSE
						IF ABL:舌 >= 2
							CALL HPH_PRINT,@"（呵呵，明明应该是我在进攻，却渐渐失去余裕，我可是知道的哦HPH）","L"
							CALL HPH_PRINT,@"（连改变喉咙的收缩方式，以及舌头摩擦都没注意到……HPH）","L"
							PRINTW
						ELSE
							CALL HPH_PRINT,@"（%CALLNAME:MASTER%这种粗暴的样子，也很新鲜，不坏呢……HPH）","L"
							CALL HPH_PRINT,@"（等会儿“反击”一下也不错呢……HPH）","L"
							PRINTW
						ENDIF
					ENDIF
				ENDIF
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				CALL HPH_PRINT,@"「来HPH %CALLNAME:MASTER%阁下喜欢的口腔小穴哦～HPH」","L"
				CALL HPH_PRINT,@"「典我嫌麻烦，请您自己扭动腰肢，随便高潮吧HPH」","L"
				PRINTL
				PRINTFORMDL 典懒洋洋地伸出舌头，挑衅地晃动着。
				PRINTFORMDL 抓住典那傲慢的头，将肉棒一下子捅到喉咙深处。
				PRINTW
				CALL HPH_PRINT,@"「嗯噗！？嗯～～～～～～！？HPH」","L"
				PRINTFORMDL 典身体微微颤抖着，却用口腔承受着肉棒的暴力。
				PRINTFORMDL 积聚着唾液的口腔温暖湿滑，使得抽插变得轻松。
				PRINTL
				PRINTFORMDL 噗啵！咕噜噜噜噜！噗啵！咕噜噜噜噜！
				CALL HPH_PRINT,@"「哦咕HPH 齁耶HPHHPH 喔HPH 呜噗HPHHPH」","L"
				PRINTL
				PRINTFORMDL 典拍打着%CALLNAME:MASTER%的身体，仿佛在说停下，但嘴巴却吸附着肉棒不肯松开。
				PRINTFORMDL 是无意识地拍打，还是为了煽动这边的施虐心而故意为之……
				IF ABL:舌 >= 2
					PRINTFORMDL %CALLNAME:MASTER%尽情享受了典极致的口技。
				ELSE
					PRINTFORMDL 即使口技不精，%CALLNAME:MASTER%也尽情享受了这场似乎相当愉快的口交。
				ENDIF
				PRINTW
			CASE 1
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%阁下HPH 请和典我的口腔小穴交合吧……HPH」","L"
				PRINTFORMDL 典用手指像拘束具一样勾住嘴巴，央求着肉棒……
				PRINTW
				PRINTFORMDL 如她所愿，像从后入侵犯一般，将典的脸用力拉近肏干。
				PRINTFORMDL 然后一口气贯穿了那饱含唾液蜜汁的媚唇。
				PRINTL
				CALL HPH_PRINT,@"「哦咕HPH 嗯啵HPH 嗯呶HPHHPH」","L"
				PRINTFORMDL 自主摇摆腰肢的口交非常舒服，只是一味地冲向射精。
				CALL HPH_PRINT,@"（这个！HPH 嘴巴被认真交合了！HPH 嘴巴要怀孕了！HPHHPH）","L"
				PRINTW
			CASE 2
				PRINTFORMDL 咕噜噜噜噜～～～～……
				CALL HPH_PRINT,@"「嗯HPH 嗯HPH 嗯HPH 嗯HPH 嗯HPH 嗯HPH」","L"
				CALL HPH_PRINT,@"（来了HPH 来了HPH 来了！HPHHPH）","L"
				PRINTL
				PRINTFORMDL 滋啪！！
				CALL HPH_PRINT,@"「哦咕嘎！？HPHHPH」","L"
				CALL HPH_PRINT,@"（来了！HPH 毫不留情的腰肢摆动！HPH 被当成洞穴了HPH）","L"
				PRINTW
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

