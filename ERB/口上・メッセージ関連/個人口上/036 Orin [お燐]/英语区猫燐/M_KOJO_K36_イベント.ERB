		SIF MARK:反発刻印 || CFLAG:ブチギレ
			RETURN -1
		SIF ARGS == "day" && CFLAG:初期位置 == CFLAG:MASTER:初期位置 ;Orin's morning events can trigger every day like it was before
			RETURN 2
		RETURN 1
	;per event
	CASE DAYEVENT_ORAL
		SIF !HAS_PENIS(MASTER) ;bj only
			RETURN -1
		SIF !TALENT:恋慕
			RETURN -1
		RETURN 0
	CASE DAYEVENT_COWGIRL, NIGHTEVENT_YOBAI
		SIF !HAS_PENIS(MASTER) ;penis only
			RETURN -1
		SIF !TALENT:恋慕
			RETURN -1
		SIF !(ABL:欲望 >= 5)
			RETURN -1
		SIF !(!TALENT:処女 && !IS_DOUTEI(MASTER)) ;must be experienced
			RETURN -1
		RETURN 1
	CASE DAYEVENT_WAKEUP
		SIF !TALENT:恋人
			RETURN -1
		RETURN 1
	CASE NIGHTEVENT_DRINK
		SIF !(ABL:奉仕精神 >= 3 && EXP:奉仕快楽経験 >= 50)
			RETURN -1
		SIF (TALENT:妊娠 || TALENT:育児中)
			RETURN -1
		SIF	CFLAG:2 < 1500 ;allowed without love
			RETURN -1
		RETURN 1
	CASE DAYEVENT_STARE, DAYEVENT_PRANK, NIGHTEVENT_MOONVIEWING ;disabled, no dialogue
		RETURN -1
	CASEELSE
		SIF !TALENT:恋慕
			RETURN -1
ENDSELECT
RETURN 0

