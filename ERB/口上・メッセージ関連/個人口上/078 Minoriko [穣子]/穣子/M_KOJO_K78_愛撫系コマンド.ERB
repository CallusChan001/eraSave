				CASEELSE
					PRINTFORML 「啊、哈啊 那里，好舒服」
				ENDSELECT
			ENDIF
		ENDIF
	ENDIF
	SIF ( CFLAG:1003 && !K78_EXCESSIVE_STATURE_GAP() ) || K78_BASIC_CONDITION_IN_MATING()
