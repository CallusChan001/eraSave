;TCVAR:烂醉(1=通常成功or大失敗)
;==================================================
@M_KOJO_MESSAGE_COM_K78_300
;現在地が浴場で恋慕or愛欲でしらふのときは体毛指定関数を呼び出す
IF BATHROOM() && (TALENT:恋慕 || TALENT:愛欲) && SHIRAHU(TARGET)
	RESETCOLOR
	CALL K78_HAIR_TREATMENT
	;体毛関係のイベントを選んだ場合は通常の会話口上無し
	SIF RESULT == 1
		RETURN 1
ENDIF
CALL M_KOJO_MESSAGE_COM_K78_300_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K78_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF CFLAG:ブチギレ || TFLAG:193 == -3 ;custom code, hate mark 3
		PRINTFORMDW 生气的%CALLNAME:TARGET%把头扭向了另一边。
	ELSEIF TCVAR:烂醉
		IF TALENT:心情 >= 0
			PRINTFORMDL 或许是因为心情很好，%CALLNAME:TARGET%咯咯地笑着。
			PRINTFORMW 「干嘛啦？ 我才没醉呢—— 没问题没问题」
		ELSE
			PRINTFORML 「喂，有没有在听人家说话啊？ 我说你听着啊！」
			PRINTFORMDW 看来穰子的醉法是很难招架的那一种。
		ENDIF
	ELSE
		SELECTCASE TFLAG:193
		CASE 1
			IF ( TALENT:恋慕 || TALENT:愛欲 ) && RAND:3
				SELECTCASE RAND:2
				CASE 1
					CALL K78_HPH_PRINT,@"「能吃到很多好吃的，还能像这样和你在一起…… 我真是个幸福的人啊……HPH」","W"
				CASEELSE
					PRINTFORMDL %CALLNAME:MASTER%盛赞%CALLNAME:TARGET%的美丽。
					CALL K78_HPH_PRINT,@"「真是的，就算你哄我，我也什么都不会给你的啦HPH」","L" 
					SIF CFLAG:既成事實 & 2
						CALL K78_HPH_PRINT,@"「不过，今晚稍微给你点福利也不是不可以哦HPH」","L" 
					WAIT
				ENDSELECT
			ELSE
				PRINTFORML 「啊哈哈♪ %CALLNAME:MASTER%真是太有趣了」
				PRINTFORMDW 看来她非常受用。
			ENDIF
		CASE 0
			;天気の話題
			IF GET_MAPNAME(GET_MAPID(CFLAG:MASTER:現在位置)) != "地底" && !RAND:5
				SELECTCASE TIME:5
				;晴天
				CASE 0
					;１９時以降
					IF TIME:0 >= 1140
						PRINTFORMW 「今晚的星星真美啊」
					;冬
					ELSEIF DAY:2 == 4
						;７～１７時
						IF BETWEENTIME(420,1020)
							PRINTFORMW 「多亏太阳出来了，稍微暖和了一点呐」
						ELSE
							PRINTFORMW 「冬天很干燥，请多多注意哦」
						ENDIF
					ELSE
						PRINTFORMW 「嗯，真是不错的天气。农作物也能茁壮成长就好了」
					ENDIF
				;晴朗
				CASE 1
					;夏
					IF DAY:2 == 2
						;起床～７時
						IF BETWEENTIME(CFLAG:起床時間,420)
							PRINTFORMW 「今天好像会很热的样子，趁凉快的时候把工作处理掉比较好。」
						;７～１９時						
						ELSEIF BETWEENTIME(420,1140)
							PRINTFORMW 「阳光很强呢。请一定要注意不要中暑了哦」
						;１９時以降
						ELSE
							PRINTFORMW 「就算太阳落山了，还是热得让人受不了」
						ENDIF
					;冬
					ELSEIF DAY:2 == 4
						PRINTFORMW 「虽然现在天气很好，但是我有点担心会不会下雪。」
					ELSE
						PRINTFORMW 「万里无云的好天气。真让人舒服呀」
					ENDIF
				;多雲、陰天
				CASE 2,3
					;陰天で１９時以降
					IF TIME:5 == 3 && TIME:0 >= 1140
						PRINTFORMW 「今天是阴天，看不到星星还真是可惜呐…」
					;冬の起床～１７時
					ELSEIF DAY:2 == 4 && BETWEENTIME(CFLAG:起床時間,1020)
						PRINTFORMW 「好冷啊。太阳会不会出来呢」
					;夏の起床～１７時
					ELSEIF DAY:2 == 2 && BETWEENTIME(CFLAG:起床時間,1020)
						PRINTFORMW 「多云的天气。是适合农作的日子呢」
					;陰天
					ELSEIF TIME:5 == 3
						PRINTFORMW 「云很浓啊。也许会下雨吧」
					ELSE
						PRINTFORM 「那朵云，看起来像
						SELECTCASE RAND:4
						CASE 1
							PRINTFORM 竹笋
						CASE 2
							PRINTFORM 茄子
						CASE 3
							PRINTFORM 饭团
						CASEELSE
							PRINTFORM 烤红薯
						ENDSELECT
						PRINTFORMW 一样很美味的样子…」
					ENDIF
				;雨、霧雨、雨雪交加
				CASE 4,6,12
					;夏
					IF DAY:2 == 2
						PRINTFORMW 「闷闷的让人感觉很难受…」
					ELSE
						PRINTFORMW 「天气不是很好呢。也许在家悠闲渡过比较好」
					ENDIF
				;大雨
				CASE 5
					;台風
					IF FLAG:異常気象 == 9
						PRINTFORMW 「大风大雨的……庄稼没问题吧？」
					ELSE
						PRINTFORMW 「好大的雨。还是不要出门的好」
					ENDIF
				;霧
				CASE 7
					PRINTFORMW 「雾好大啊。要出门的话请小心点」
				;雪、細雪、霧雪
				CASE 8,10,11
					PRINTFORMW 「下雪了呢。很冷吧」
				;吹雪、冰雹
				CASE 9,13
					PRINTFORMW 「天气真糟糕呀。今天最好还是呆在家里」
				ENDSELECT
			;季節の話題
			ELSEIF RAND:2
				SELECTCASE DAY:2
				CASE 1
					SELECTCASE RAND:4
					CASE 1
						PRINTFORMW 「说到春天就想到竹笋。竹笋饭♪竹笋饭♪」
					CASE 2
						PRINTFORML 野菜的季节哟。蕨菜、楤木芽、蜂斗菜、野蒜」
						PRINTFORMW 「……有点想喝酒了」
					CASE 3
						PRINTFORMW 「一到春天，是不是就有心跳不已的感觉？」
					CASEELSE
						PRINTFORMW 「该插秧了。这个时期种苗很重要哦」
					ENDSELECT
				CASE 2
					SELECTCASE RAND:4
					CASE 1
						PRINTFORML 「一说到夏天，就想到番茄、茄子、黄瓜、青椒等好吃的蔬菜！」
						PRINTFORMW 「对健康也有好处，所以要好好摄取哦」
					CASE 2
						PRINTFORML 「夏天田里的工作是除草、驱虫、浇水」
						PRINTFORMW 「虽然热得够呛，但比起冷害要好得多。」
					CASE 3
						PRINTFORMW 「好热啊。想吃刨冰什么的」
					CASEELSE
						PRINTFORMW 「夏天要注意台风呐。有台风的话就好好呆在家里吧」
					ENDSELECT
				CASE 3
					SELECTCASE RAND:4
					CASE 1
						PRINTFORML 「已经是秋天了。正如我的名字，是我的季节呢」
						PRINTFORMDW %CALLNAME:TARGET%挺起了胸膛。
					CASE 2
						PRINTFORML 「收获的时期呢。今年也能吃很多初穗的大米」
						PRINTFORMDW %CALLNAME:TARGET%的脸上喜气洋洋的。
					CASE 3
						PRINTFORMW 「你难道不觉得金黄色的稻穗有不亚于红叶之美吗？」
					CASEELSE
						PRINTFORMW 「秋季时令的蘑菇。可以炒，可以炸，可以煮，是非常方便的宝物」
					ENDSELECT
				CASE 4
					SELECTCASE RAND:4
					CASE 1
						PRINTFORMW 「冬天总感觉心情很低落。春天快来吧！」
					CASE 2
						PRINTFORMW 「在寒冷的冬天吃烤白薯。味道又甜又可口♪最喜欢了」
					CASE 3
						PRINTFORMW 「唔，好冷啊。想一直呆在被炉里…」
					CASEELSE
						PRINTFORMW 「冬天果然还是得吃白萝卜。做成美味的关东煮真是太好吃了」
					ENDSELECT
				ENDSELECT
			;其他の話題
			ELSE
				IF ( TALENT:恋慕 || TALENT:愛欲 ) && RAND:2
					SELECTCASE RAND:4
					CASE 1
						PRINTFORML 「呐、你喜欢的食物是什么呢？」
						PRINTFORMDL %CALLNAME:TARGET%唐突的提出了质问。
						PRINTFORMW 「哼，那么就试着培育那个吧」
					CASE 2
						PRINTFORMW 「嗯？ 这个味道？ 呵呵，是丰穰之神特制的香水哦♪」
					CASE 3
						PRINTFORML 「然后啊然后啊，前几天发生了很有趣的事情哦」
						PRINTFORMDW %CALLNAME:TARGET%靠近到身体几乎要贴在一起，兴致勃勃地继续讲着。
					CASEELSE
						PRINTFORMW 「啊，已经这么晚了。和你聊天时间过得真快啊。」
					ENDSELECT
				ELSE
					IF FLAG:扮演 != 77 && !RAND:4
						PRINTFORML 「我跟姐姐说红叶又不能吃，她却说有炸枫叶天妇罗。」
						PRINTFORMW 「那个好吃吗？」
					ELSEIF FLAG:扮演 != 77 && !RAND:3
						PRINTFORML 「那个，不就之前，姐姐做了件很%TEXTR("厉害/糟糕/有趣")%的事」
						PRINTFORMDW 讲关于%CALLNAME:77%的事的%CALLNAME:TARGET%看起来很开心。
					ELSEIF RAND:2
						PRINTFORML 「呐，%CALLNAME:MASTER%，你最喜欢哪个季节？」
						PRINTFORMDW 看来不得不回答是秋天了。
					ELSE
						;農民でなく園芸用品をもっていない
						IF !FLAG:地主 && !ITEM:園芸用品一式
							PRINTFORMW 「摆弄泥土挺好的哦～ 你也来试试家庭菜园怎么样？」
						ELSE
							PRINTFORMW 「田地的情况怎么样？要想丰收，唯有每日的辛勤耕耘。」
						ENDIF
					ENDIF
				ENDIF
			ENDIF
		CASE -1
			PRINTFORML 「嗯唔…」
			PRINTFORMDW 似乎是个%CALLNAME:TARGET%不感兴趣的话题。
		ENDSELECT
	ENDIF
ENDIF
RETURN 1
