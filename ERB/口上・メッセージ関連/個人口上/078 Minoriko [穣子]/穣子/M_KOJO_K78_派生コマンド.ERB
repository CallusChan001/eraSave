;==================================================
;5,手淫口交
;==================================================
@M_KOJO_MESSAGE_SCOM_K78_5
CALL M_KOJO_MESSAGE_SCOM_K78_5_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K78_5_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		PRINTFORMW 
		RETURN 1
	ENDIF
	IF K78_BASIC_CONDITION_IN_MATING()
		$HANDJOB_AND_FELLATIO_LOOP
		IF TALENT:恋慕 || TALENT:愛欲
			IF NOWEX:MASTER:射精 && !TEQUIP:MASTER:避孕套 && EXP:精飲経験 > 0
				SELECTCASE RAND:3
				CASE 1
					;咥えたまま
					IF EXP_UP(15,TARGET) == NOWEX:PLAYER:射精 * 3
						CALL K78_HPH_PRINT,@"「嗯？ 要出来了吗？HPH 可以哟HPH 在我的嘴里尽情的射出来吧HPH」","W"
					ELSE
						GOTO HANDJOB_AND_FELLATIO_LOOP
					ENDIF
				CASE 2
					PRINTFORMDL %CALLNAME:TARGET%察觉到射精即将到来，手上便加大了力道，猛烈地套弄起来。
					CALL K78_HPH_PRINT,@"「快，射出来HPH 好像看到+++用我的手和嘴巴射出好多好多精液的样子HPH」","W"
				CASEELSE
					CALL K78_HPH_PRINT,@"「啊，在抽动HPH 那，就这样射出来吧HPH 啊姆…啾…舔HPH」","W"
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 1
					CALL K78_HPH_PRINT,@"「不用忍着哦HPH 想射的时候就射出来吧HPH」","W"
				CASE 2
					PRINTFORMDL %CALLNAME:TARGET%用舌尖从系带到龟头到处探索刺激着。
					CALL K78_HPH_PRINT,@"「嗯啾、舔、啾啵HPH 这里舒服吗？HPH 还是这边比较好呢？ 嗯姆、舔HPH」","W"
				CASEELSE
					PRINTFORMDL %CALLNAME:TARGET%发出咕啾咕啾的声音，猛烈地套弄着根部，同时含着龟头。
					CALL K78_HPH_PRINT,@"「嗯、嗯咕HPH 呜、咕啵、嗯姆HPH 咕啵、啾噜、啾噜噜HPH」","W"
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE RAND:3
			CASE 1
				IF TALENT:思慕 || TALENT:炮友
					PRINTFORMW 「哈姆…啾…舔…啾啪… 渐渐地会变得很投入呢，这个。」
				ELSE
					PRINTFORMW 「嗯…嗯姆、嗯啾…… 呼，嘴巴和手都开始累了。」
				ENDIF
			CASE 2
				IF ABL:指 + ABL:舌 >= 3
					PRINTFORMW 「舔…啾…啾噜… 怎么样？是不是变得很擅长了？」
				ELSE
					PRINTFORMW 「不、不痛吗？我不太清楚轻重……」
				ENDIF
			CASEELSE
				PRINTFORMW 「快射的时候告诉我哦。突然射出来会吓一跳的。」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
;未記入
;CALLFORM M_KOJO_MESSAGE_COM_K78_{SELECTCOM}_1
RETURN 1

