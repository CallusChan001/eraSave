;"EVENT"系イベント
;※※※※※※※※※※※※※※※※※※※※※※※※※※
;==================================================
;部屋で遭遇
;==================================================
@M_KOJO_EVENT_K78_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	CALL K78_HAIR_GROWTH,78
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて一緒に入る
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF K78_CONSCIOUS_AND_TOLERANT()
			PRINTFORMW 「啊，%CALLNAME:MASTER%已经在里面了呀。呐，我可以进去一起洗么？」
		ENDIF
		SIF SHIRAHU(TARGET)
			TCVAR:78:352 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて出て行った
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが風呂に入ってきて追い出された
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;外出先で遭遇した
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		CALL DIALOGUE_K78_GREET_ONCE_A_DAY
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;他角色と约会中に遭遇した
	;ARG:1に约会的对象
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		IF TCVAR:353 != ARG:1 && K78_CONSCIOUS_AND_TOLERANT()
			;静葉と约会
			IF ARG:1 == 77 && FLAG:扮演 != 77
				IF TALENT:恋人 || ( TALENT:恋慕 && !TALENT:77:恋人 )
					PRINTFORML 「呜～ 姐姐黏得太紧了啦！」
					PRINTFORMDW %CALLNAME:TARGET%显露出对抗心，向%CALLNAME:77%埋怨起来。
				ELSE
					PRINTFORMW 「和姐姐出去？玩得开心哦。」
				ENDIF
			;与刚才不同的对象约会
			ELSEIF TCVAR:353 != 0
				IF TALENT:恋慕 || TALENT:恋人
					PRINTFORML 「又在不停地换女人……」
					PRINTFORMDW %CALLNAME:TARGET%被瞪了一眼。
				ELSE
					PRINTFORML 「为什么约会对象和刚才不一样了啊……」
					PRINTFORMDW %CALLNAME:TARGET%对此感到无奈。
				ENDIF
			ELSE
				IF TALENT:恋慕 || TALENT:恋人
					PRINTFORML 「看起来关系很好呢……」
					PRINTFORMDW %CALLNAME:TARGET%看到有女人作伴的%CALLNAME:MASTER%后，露出了不开心的样子。
				ELSEIF TALENT:思慕
					PRINTFORML 「哼。%CALLNAME:MASTER%人缘真广啊……」
					PRINTFORMDW %CALLNAME:TARGET%冷淡地瞥了一眼%CALLNAME:MASTER%和%CALLNAME:(ARG:1)%。
				ELSE
					PRINTFORM 「啊，%CALLNAME:MASTER%。
					SELECTCASE TIME:0
					CASE IS <600
						PRINTFORMW 早上好」
					CASE 600 TO 1139
						PRINTFORMW 中午好」
					CASE IS >= 1140
						PRINTFORMW 晚上好」
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
		IF SHIRAHU(TARGET)
			TCVAR:78:352 = 1
			TCVAR:78:353 = ARG:1
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1


