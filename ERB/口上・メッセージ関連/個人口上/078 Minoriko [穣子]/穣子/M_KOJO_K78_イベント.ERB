;==================================================
;脱衣口上
;==================================================
@M_KOJO_EVENT_K78_28(ARG,ARG:1)
;口上主はTARGET
;ARGに脱がし方選択
;ARG:1（ARG=3の場合のみあり）地の文の反応
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;全裸にする
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF K78_BASIC_CONDITION_IN_MATING()
				;「下着姿にする」を経由せずに全裸になった場合
				IF !TEQUIP:上衣脱衣完毕
					;腋毛ありかつ全裸になる前から腋が見えている
					IF CFLAG:1007 && K78_ARMPIT_EXPOSURE() 
						IF CFLAG:1007 >= 3
							PRINTFORMDL 刚才就算放下手臂，腋下的浓毛也稍微露出了一些，但脱衣服时抬起腋下，那杂乱茂密的景象便一览无余。
						ELSE
							PRINTFORMDL 腋下\@CFLAG:1007 == 2 ?的毛发 #的剃痕 \@在脱衣前就若隐若现，但脱衣服时抬起手臂，那\@CFLAG:1007 == 2 ?蓬松 #稀疏 \@的毛发便清晰可见。
						ENDIF
						SIF !TEQUIP:眼罩
							PRINTFORML 「真是的，别老盯着腋下看啦。我早就注意到你一直在看了哦。」
						PRINTL
					ENDIF
					PRINTFORMDL %CALLNAME:MASTER%静静地欣赏着%CALLNAME:TARGET%脱衣，享受着肌肤一点点展露出来的景象。
					IF ( TALENT:恋慕 || TALENT:愛欲 || TALENT:思慕 || TALENT:炮友 ) && ABL:露出癖 >= 2 && TALENT:心情 >= 0 && BASE:情緒 >= 900 && BASE:理性 < 500
						PRINTFORMDL %CALLNAME:TARGET%也显得很有兴致，像是要尽情展现自己一样，不紧不慢地脱下了衣服。
						SIF !TEQUIP:眼罩
							CALL K78_HPH_PRINT,@"「瞧你那傻乐的样子HPH %CALLNAME:MASTER%好H～HPH」","L" 
					ELSEIF TALENT:恋慕 || TALENT:愛欲
						PRINTFORMDL 脱下衣服后，甜腻的媚香变得更加浓郁，与暴露出的妖娆身姿交织成的诱惑，令%CALLNAME:MASTER%头晕目眩。
						CALL K78_HPH_PRINT,@"「只有我一个人会害羞HPH 呐，你也脱掉吧HPH」","L"
					ELSEIF !TEQUIP:眼罩
						PRINTFORML 「被你这么盯着看，我会害羞的。」
					ENDIF
				;通过“脱剩内衣”最终全裸的情况
				ELSE
					PRINTFORMDL 脱下内衣，纤毫毕现的%CALLNAME:TARGET%肉体，真的如雕塑刻画的女神般匀称美丽。
					PRINTFORM 「好了，都脱光了
					IF TALENT:恋慕 || TALENT:愛欲
						CALL K78_HPH_PRINT,@"HPH 那么，来做更多好事情吧HPH」","L"
					ELSE
						PRINTFORML 」
					ENDIF
				ENDIF
				;全裸になってはじめて腋が露出した、かつ腋汗ジミあり
				IF !K78_ARMPIT_EXPOSURE() && K78_SWEATY_UNDERARMS()
					PRINTL
					SELECTCASE CFLAG:1007
					CASE 0,1
						PRINTFORMDL 脱下那腋下留下汗渍的衣服后，汗津津、油光发亮的诱人媚腋展露无遗。
					CASE 2,3
						PRINTFORMDL 也许是因为长着毛发，腋下潮湿到衣服都留下了汗渍，刚一脱掉衣服，便开始散发出浓郁的芬香。
					ENDSELECT
					PRINTFORML 「……有点热，所以出汗了。」
					PRINTFORMDL %CALLNAME:TARGET%的脸颊微微泛红。
				;全裸后腋下才首次露出，且有腋毛
				ELSEIF !K78_ARMPIT_EXPOSURE() && CFLAG:1007
					PRINTL
					PRINTFORMDL %CALLNAME:TARGET%全裸后，%K78_ARMPIT_HAIR_MODIFIER()%腋下展露无遗。
					CALL K78_HPH_PRINT,@"「我照你说的那样留得这么长了哦HPH 怎么样？高兴吗？HPH」","L"
				ENDIF
				WAIT
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;变成只穿内衣的样子
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF K78_BASIC_CONDITION_IN_MATING()
				;腋毛ありかつ内衣姿になる前から腋が見えている
				IF CFLAG:1007 && K78_ARMPIT_EXPOSURE()
					IF CFLAG:1007 >= 3
						PRINTFORMDL 就算没抬起手臂时，腋下的毛发也稍微露出了一些，但脱衣服时抬起腋下，那杂乱茂密的景象便一览无余。
					ELSE
						PRINTFORMDL 脱衣前腋下的\@CFLAG:1007 == 2 ?毛发 #剃毛痕迹 \@就若隐若现，但脱衣服时抬起手臂，那\@CFLAG:1007 == 2 ?蓬松 #短硬的毛茬 \@便清晰可见。
					ENDIF
					SIF !TEQUIP:眼罩
						PRINTFORML 「真是的，别老盯着腋下看啦。我早就注意到你一直在看了哦。」
					PRINTL
				ENDIF
				PRINTFORMDL %CALLNAME:TARGET%脱去上衣的身姿，散发着极度的妩媚，同时修长的四肢却保留着少女般的稚气。
				IF TALENT:恋慕 || TALENT:愛欲
					;没穿内裤でなく胖次指定も無
					IF !CFLAG:没穿内裤 && !CFLAG:指定胖次
						CALL K78_HPH_PRINT,@"「今天的内衣怎么样？HPH 你喜欢这种的吗？HPH」","L"
					ELSE
						CALL K78_HPH_PRINT,@"「不脱内衣比较好？我知道了HPH 你喜欢这种的嘛HPH」","L"
					ENDIF
				ELSE
					PRINTFORML 「不喜欢全脱，想看内衣姿态吗？哼，我知道了。」
				ENDIF
				;内衣姿になってはじめて腋が露出した、かつ腋汗ジミあり
				IF !K78_ARMPIT_EXPOSURE() && K78_ARMPIT_EXPOSURE(TARGET,1) && K78_SWEATY_UNDERARMS()
					PRINTL
					SELECTCASE CFLAG:1007
					CASE 0,1
						PRINTFORMDL 脱下那腋下留下汗渍的衣服后，汗津津、油光发亮的诱人媚腋展露无遗。
					CASE 2,3
						PRINTFORMDL 也许是因为长着毛发，腋下潮湿到衣服都留下了汗渍，刚一脱掉衣服，便开始散发出浓郁的芬香。
					ENDSELECT
					PRINTFORML 「……有点热，所以出汗了。」
					PRINTFORMDL %CALLNAME:TARGET%的脸颊微微泛红。
				;下着姿になってはじめて腋が露出した、かつ腋毛あり
				ELSEIF !K78_ARMPIT_EXPOSURE() && K78_ARMPIT_EXPOSURE(TARGET,1) && CFLAG:1007
					PRINTL
					PRINTFORMDL %CALLNAME:TARGET%全裸后，%K78_ARMPIT_HAIR_MODIFIER()%腋下展露无遗。
					CALL K78_HPH_PRINT,@"「我照你说的那样留得这么长了哦HPH 怎么样？高兴吗？HPH」","L"
				ENDIF
				WAIT
			ENDIF
			RETURN 1
