	CFLAG:78:1002 = 0
ENDIF
@KOJO_OPTION_K78_RESULT_2
#LOCALSIZE 1
#LOCALSSIZE 1

gLineCount = LINECOUNT
$UPDATE_LOOP
SIF LINECOUNT - gLineCount > 0
    CLEARLINE LINECOUNT - gLineCount
SELECTCASE CFLAG:78:1002
