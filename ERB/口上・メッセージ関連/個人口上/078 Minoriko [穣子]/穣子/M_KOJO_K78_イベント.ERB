;==================================================
;朝起きたとき（おはよう口上）
;==================================================
@M_KOJO_EVENT_K78_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	SIF SHIRAHU(TARGET)
		TCVAR:78:352 = 1
	CALL K78_HAIR_GROWTH,78
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;非同棲かつ相手が先に起きてる
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室にMASTERが入っている（恋慕時陪睡中就寝時のみ？
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室にTARGETが入ってきてる(恋慕無しでも入ってくる可能性有り
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			IF K78_CONSCIOUS_AND_TOLERANT()
				IF TALENT:恋慕 || TALENT:愛欲
					CALL K78_HPH_PRINT,@"「抱歉，是不是吵醒你了？……不知怎么的，就是想见你HPH」","W"
				ELSEIF TALENT:思慕 || TALENT:炮友
					PRINTFORML 「啊，早上好」
					PRINTFORMDL 为什么%CALLNAME:TARGET%会在这里呢。
					PRINTFORMW 「只，只是顺路过来看看而已哦」
				ELSE
					PRINTFORMW 「醒了么？我来稍微借点东西」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が起きている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF K78_CONSCIOUS_AND_TOLERANT()
			SELECTCASE RAND:3
			CASE 1
				PRINTFORMW 「早上好%CALLNAME:MASTER%。那么，今天就去打理田地吧。」
			CASE 2
				PRINTFORMDL 醒来时，%CALLNAME:TARGET%的脸就在%CALLNAME:MASTER%的眼前。
				CALL K78_HPH_PRINT,@"「嘿嘿♪ 因为你的睡颜太可爱了，所以一直在看哦HPH」","W"
			CASEELSE
				;和TARGET一起醒来
				IF TIME:0 == CFLAG:起床時間
					PRINTFORMW 「早上好。今天要做什么呢？」
				;比TARGET晚起
				ELSE
					PRINTFORMW 「终于醒了？你这个爱睡懒觉的家伙。」
				ENDIF
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同棲かつ相手が寝ている
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝時TARGET起床はこちら（TARGET衰弱or陪睡中TARGET睡魔負け後そのままMASTERが起き続けた上で相手が起床した場合限定
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同棲
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同棲中
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMW 
			RETURN 1
		ENDIF
		PRINTFORMW 
		RETURN 1
	ENDIF
	;其他
	PRINTFORML  
ENDIF
