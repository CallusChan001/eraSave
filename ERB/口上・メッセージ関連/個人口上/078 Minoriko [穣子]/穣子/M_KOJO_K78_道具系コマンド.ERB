	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	
	IF K78_BASIC_CONDITION_IN_MATING()
		;装着
		IF TEQUIP:肛用振動棒
			IF TALENT:恋慕 || TALENT:愛欲
			
			ELSE
			
			ENDIF
		;解除
		ELSE
			IF 充填率(TARGET,2) >= 60 && !RAND:3
				IF CFLAG:1002
					IF ( TALENT:恋慕 || TALENT:愛欲 ) && !TCVAR:351 && !RAND:5
						PRINTFORMDL 额头冒汗的%CALLNAME:TARGET%一直忍耐着便意，但终于到达极限的肛门瞬间张开，伴随着噗嗤噗嗤的剧烈声音，喷射出了混杂着精液的粪便。
						CALL DIALOGUE_K78_DEFECATION_IN_MATING
					ELSEIF !TCVAR:351 && RAND:2
						PRINTFORMDL 也许是因为被大量涌入直肠的精液刺激，%CALLNAME:TARGET%的肚子咕噜咕噜地叫了起来。
						PRINTFORMDL 尽管便意涌上，她似乎仍在拼命忍耐着。
					ELSE
						PRINTFORMDL 被\@ 充填率(TARGET,2) >= 100 ?大量 # \@精液灌满的%CALLNAME:TARGET%从%TEXTR("肛门/屁眼/菊门")%流淌出混杂着肠液、散发着独特气味的%TEXTR("精液/子孙汁/白浊液")%。
					ENDIF
				ELSE
					PRINTFORMDL 从%CALLNAME:TARGET%的肛门中拔出震动棒，大量注入直肠的%TEXTR("精液/子孙汁/白浊液")%缓缓地滴落下来。
				ENDIF
			ELSE
				IF CFLAG:1002 && ( TALENT:恋慕 || TALENT:愛欲 ) && !TCVAR:351 && !RAND:20
					PRINTFORMDL %CALLNAME:TARGET%突然扭捏起来，紧接着伴随着巨大的放屁声，从肛门喷射出了稀稀拉拉的腹泻状粪便。
					CALL DIALOGUE_K78_DEFECATION_IN_MATING
				ELSEIF CFLAG:1002 && !RAND:5
					PRINTFORMDL 也许是受到震动棒的刺激，肠道开始蠕动，%CALLNAME:TARGET%%TEXTR("噗嗤/噗呜/噗噗/噗噗")%地放了个屁。
					CALL DIALOGUE_K78_FARTING_IN_MATING
				ELSE
					PRINTFORMDL %CALLNAME:TARGET%的%TEXTR("肛门/屁眼/屁股眼")%即使在\@ TALENT:MASTER:性別 & 2 ?肉棒 #假阳具 \@被拔出后，仍然%TEXTR("圆张着/一张一合/抽动着")%，暴露着%TEXTR("肛穴嫩肉/湿滑的粘膜")%。
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
RETURN 1

