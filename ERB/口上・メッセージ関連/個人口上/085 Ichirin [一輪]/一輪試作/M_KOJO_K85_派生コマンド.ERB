;==================================================
@M_KOJO_MESSAGE_SCOM_K85_6
CALL M_KOJO_MESSAGE_SCOM_K85_6_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K85_6_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
