	IF LOCAL:1 && TCVAR:101 == PLAYER
		;前回射精した
		IF TCVAR:104
			PRINTFORML 「刚刚❤明明已经射出来过了❤」
			PRINTFORML 「那里❤不行啊❤补药欺副窝❤❤」
			RETURN 1
		ELSE
			PRINTFORMW 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	IF TCVAR:140
		PRINTFORML 「咿呀—❤那里，稀饭❤要补芯了❤❤」
	ELSE
		SELECTCASE RAND:2
		CASE 0
			PRINTFORML 「嗯❤那里，很敏感❤不行❤」
		CASE 1
			PRINTFORML 「尽是些舒服的地方❤要变得奇怪了❤❤」
		ENDSELECT
		RETURN 1
	ENDIF
ENDIF
