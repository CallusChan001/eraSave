RETURN RESULT

@M_KOJO_MESSAGE_COM_K67_85_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		IF MARK:反発刻印 == 3
			;射精(ex:11)
			IF NOWEX:PLAYER:11
				PRINTFORMW 在一动不动的慧音冷冷的注视下，达到极限的%CALLNAME:MASTER%，
				PRINTFORMW 将大量的精液释放在慧音的脚上和鞋上。
				PRINTFORMW 将鞋子套回玉足上，鞋子里发出了咕叽叽的下流濡湿声。
			ELSE
				PRINTFORMW %CALLNAME:MASTER%开始用一动不动的慧音的脚自慰
				PRINTFORMW 虽然连意识都没有，但怀着明确敌意的视线让人感到很舒服
			ENDIF
			RETURN 1
		;射精(ex:11)
		ELSEIF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 一转眼就迎来极限的%CALLNAME:MASTER%，在慧音光滑的脚上释放着精液
				PRINTFORMW 慧音本来就白皙美丽的脚，被白浊点缀得更加美丽了
			CASE 1
				PRINTFORMW 不久后，%CALLNAME:MASTER%射在了慧音的足弓之间
				PRINTFORMW 射精期间，脚心略带僵硬却又绝妙的挤压感，刺激得仿佛腰要断了
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 掰开腿，将肉棒夹在两只脚的足弓之间
				PRINTFORMW 身体似乎相当僵硬，摆体位费了点劲
				PRINTFORMW 可能稍微会弄疼她吧
			CASE 1
				PRINTFORMW 脱掉她的鞋子，将%TINKO(PLAYER)%夹在她的脚趾之间。
				PRINTFORMW 她一本正经的样子，反而显得很协调。
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:恋人
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「嗯，还不许射哦。」
				PRINTFORML 刚到射精前一刻，慧音就开口了
				PRINTFORML 「在老师说『可以』并给出许可之前，要忍住，你能做到吧？」
				PRINTFORMW %CALLNAME:MASTER%咬紧牙关忍耐着，但在愈发激烈的折磨下，数秒内便达到了极限
				PRINTFORML 「哎呀。」
				PRINTFORML 在%CALLNAME:MASTER%忍不住腰肢弹起的一瞬间，慧音灵巧地用脚趾堵住了尿道口
				PRINTFORML 另一只脚依然在玩弄着肉棒
				PRINTFORML %CALLNAME:MASTER%在令人感到恐惧的折磨下发出了悲鸣
				PRINTFORML 「哈啊……为什么就不能听老师的话呢——……？」
				PRINTFORMW 当%CALLNAME:MASTER%拼命请求饶恕时，与那双被嗜虐染浊的眼睛对上了……就这样喷射出前所未有的量！
			CASE 1
				PRINTFORMW 「哎？光用脚就满足了吗？」
				PRINTFORMW 「明明田地都准备好了呢……播种就先搁置吧，呵呵。」
			CASE 2
				PRINTFORML 「看，还能射更多吧？」
				PRINTFORMW 射精的瞬间，压在肉棒上的力道变大了
				PRINTFORML 「怎么了怎么了，渴望怀孕的老师子宫就在你眼前哦？」
				PRINTFORMW 慧音挑逗般分开阴唇展示湿润的穴口嫩肉，同时用巧妙的足技将尿道中的精液一点不剩地榨出来
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「哎？疼吗？」
				PRINTFORMW 「%CALLNAME:MASTER%的演技真差劲啊……没办法，我再用力点好了。」
			CASE 1
				PRINTFORMW 「竟然想被老师用脚踩，真是个相当扭曲的性癖啊。」
				PRINTFORMW 「……不过我也没资格说别人就是了，呵呵。」
				PRINTFORMW 「豁啦豁啦，还能射出更多的吧？」
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:恋慕
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「……看起来很舒服呢，太好了。」
				PRINTFORMW 多亏了指导，慧音的足技也变得相当不错了
				PRINTFORML 她似乎也乐在其中，即使射精了也不肯停下脚
				PRINTFORMW 刚发射的肉棒半是被强迫地，很快就又被弄勃起了
			CASE 1
				PRINTFORML 「啊……」
				PRINTFORMW 「……好温暖……」
				PRINTFORMW 她用仿佛被情热熏染的神情，凝视着沾满白浊的脚
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「嗯……要高潮了，是吗？」
				PRINTFORML 慧音从%CALLNAME:MASTER%的表情中察觉到极限，便将脚从肉棒上移开了
				PRINTFORMW %CALLNAME:MASTER%不自觉地拱起腰渴求嫩足的触感，她也同样地躲开了
				PRINTFORML 当%CALLNAME:MASTER%喘着粗气恳求时，看到那红扑扑的脸上嘴角微微上扬
				PRINTFORMW 也许慧音施虐的天性被勾起来了……
			CASE 1
				PRINTFORML 命令慧音用脚侍奉时张开双腿露出小穴
				PRINTFORMW 看到黏稠的爱液从秘唇滴落下来……
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF MARK:不埒刻印 == 3
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「……总觉得很麻烦啊……啊弄得到处都是了」
				PRINTFORMW 「唉……总比自己被乱来好吧……」
			CASE 1
				PRINTFORMW 「啊……」
				PRINTFORMW 「被踩到射精了……？」
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「听起来容易，做起来难啊」
				PRINTFORMW 「平时哪做过这种事啊……」
			CASE 1
				PRINTFORMW 「你说让我踩……这里，不是很重要的地方吗？」
				PRINTFORMW 「要是出了什么问题我可不管哦」
			ENDSELECT
			RETURN 1
		ENDIF
		RETURN 1
	ELSE
		PRINTFORMW 「用脚？」
		PRINTFORMW 「狠狠的踩上去就行了吗？诶？不对吗？」
		RETURN 1
	ENDIF
ENDIF
RETURN 1
