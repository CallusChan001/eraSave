;==================================================
@M_KOJO_MESSAGE_COM_K67_81
CALL M_KOJO_MESSAGE_COM_K67_81_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K67_81_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		IF MARK:反発刻印 == 3
			;射精(ex:11)
			IF NOWEX:PLAYER:11
				PRINTFORML 临近极限前一刻，将肉棒深深插入慧音的喉咙深处
				PRINTFORMW 直接将精液注入喉咙，然后慢慢拔出，整个口腔充满了白浊
				PRINTFORMW 一脸反感的慧音还在眼神锐利地瞪着这边，精液却不自知地从唇边滴落，着实有些滑稽
			ELSE
				PRINTFORML 将肉棒按入了表情僵硬的慧音口中
				PRINTFORMW 虽然口腔有相当大的压迫感，但作为飞机杯来说，却是恰到好处
			ENDIF
			RETURN 1
		;射精(ex:11)
		ELSEIF NOWEX:PLAYER:11
			PRINTFORMW 对着纹丝不动的慧音口中毫不顾忌地送腰大量射精
			PRINTFORMW 拔出肉棒时，咕噜噜地冒出了被搅出的小小气泡
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 将肉棒按入了时间静止的慧音口中
				PRINTFORMW 摇动腰肢时，她端正的容貌被逐渐蹂躏
			CASE 1
				PRINTFORMW 在目光呆滞地望着别处的慧音口中，开始用小嘴飞机杯自慰
				PRINTFORMW 在寂静中，被搅出的唾液发出咕嘟咕嘟的水声
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:恋人
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「嗯……唔……」
				PRINTFORML 咕噜……咕噜……慧音喉咙中传来猥亵的声音
				PRINTFORMW 就这么保持深喉的姿势运动喉咙，用妖艳的吞精律动继续刺激肉棒
				PRINTFORML 「……呣……呼……啾啵啾噜噜」
				PRINTFORMW 带着陶醉的表情咽下精液后，又立刻继续开始口交了！
			CASE 1
				PRINTFORMW 「唔……呼……」
				PRINTFORMW 「%CALLNAME:MASTER%……这点对老师来说可完全不够哦……？」
				PRINTFORMW 「……啾噗」
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「一如既往地雄伟呢。」
				PRINTFORMW 「为了『下一节课』，可得好好预习一下呢……呵呵。」
			CASE 1
				PRINTFORMW 「光是闻到味道就头晕目眩了呢。」
				PRINTFORMW 「哎嘿嘿，我开动啦……」
			CASE 2
				PRINTFORMW 「听话，要好好地说出要求『老师，请含住我』哟。」
				PRINTFORMW 「现在的话，我会给好孩子❤～满满的奖励哦……？」
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF TALENT:恋慕
		;射精(ex:11)
		IF NOWEX:PLAYER:11
				PRINTFORML 「嗯……唔……」
				PRINTFORML 即使射精，慧音也闭上眼睛更深地含住肉棒，动作也随之停止
				PRINTFORMW 一直保持到射精结束都含着那东西，眼神有些复杂地凝视着这边
				PRINTFORMW 有点不明白她想表达什么，但肉棒被口腔温柔包裹，很快恢复了硬度
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 当把勃起的肉棒凑到慧音脸庞边时，她非常自然地含住开始了侍奉
				PRINTFORMW 感觉慧音变得相当积极了，不是心理作用吧
			CASE 1
				PRINTFORMW 慧音用上挑的眼神观察着这边的表情
				PRINTFORML 虽然舌头的动作还比较单调，但也是非常认真地侍奉肉棒。好像更积极了不是错觉吧？
				PRINTFORMW 感受到慧音想要让自己舒服的心意，肉棒又胀大了一些……
			ENDSELECT
			RETURN 1
		ENDIF
	ELSEIF MARK:不埒刻印 == 3
		;射精(ex:11)
		IF NOWEX:PLAYER:11
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「嗯……唔！」
				PRINTFORMW 「要射就说一声啊……衣服会弄脏的不是吗？」
			CASE 1
				PRINTFORML 「呜哇……」
				PRINTFORMW 「确实，虽然比以前要习惯些了……」
			ENDSELECT
			RETURN 1
		ELSE
			SELECTCASE RAND:2
			CASE 0
				PRINTFORML 「你说我脸红了？」
				PRINTFORMW 「那当然了，这、这种东西被摆在眼前，谁都会脸红的啊。」
			CASE 1
				PRINTFORML 「呜呜……真不想做……一整天都没胃口了。」
				PRINTFORMW 「好了好了，都依你了，别那么一脸可怜巴巴地逼过来呀！」
			ENDSELECT
			RETURN 1
		ENDIF
		RETURN 1
	ELSE
		PRINTFORMW 「哈啊……」
		PRINTFORMW 「做就是了嘛，做就是了…」
		RETURN 1
	ENDIF
ENDIF
RETURN 1

