        CALL K60_DATE_ANGER_PENALTY_FAVOR(ARG)
    ;No penalty if the flag is 2
    CASE 2
        DEBUGPRINTFORML 无惩罚
    ;Hate Marks, anger, favor loss, and yandere points (Not yet implemented)
    CASE 3
        CALL K60_GET_ANGRY()
        CALL K60_DATE_ANGER_PENALTY_FAVOR(ARG)
        IF TALENT:[[帕露西]]:恋人
            LOCAL = 9999
        ELSEIF TALENT:[[帕露西]]:恋慕
            LOCAL = 3
        ELSEIF TALENT:[[帕露西]]:思慕
            LOCAL = 2
        ENDIF
