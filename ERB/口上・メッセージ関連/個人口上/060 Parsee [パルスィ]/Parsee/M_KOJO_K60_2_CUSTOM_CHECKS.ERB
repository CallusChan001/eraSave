;can set custom CAN_COM values here
;ARG is the command number
@M_KOJO_CHECK_K60_CAN_COM(ARGS,ARG,ARG:1)
IF IS_V_INSERT_COM(ARG)
	TRYCALL QOL_COM_ORDER_ELEMENT(TARGET, @"Desperate", 100)
ELSEIF IS_A_INSERT_COM(ARG) || GROUPMATCH(ARG, 45, 46)
	SIF !TALENT:恋人 && EXP:Ａ経験 < 100 && !(IS_MALE(TARGET) && IS_MALE(MASTER) && ABL:親密 >= 20)
		TRYCALL QOL_COM_ORDER_ELEMENT(TARGET, @"Against Degeneracy", -300 + EXP:Ａ経験 * 2 - MARK:反発刻印 * 50)
ENDIF
