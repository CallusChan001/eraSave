@M_KOJO_EGG_MESSAGE_COM_K60_300_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
;General structure borrowed from the EGG Sakuya dialogue
IF LOCAL
    $PARSEE_ALL_CONVOS
    IF TFLAG:193 == -3 ;hate mark 3
        PRINTFORMDW 帕露西冷冷地盯着你
        PRINTDATAW
            DATAFORM 「别跟我说话」
            DATAFORM 「走开」
            DATAFORM 「怎么不去和鬼拼酒量？」
            DATAFORM 「去换个人烦吧」
            DATAFORM 「我很忙…忙什么？那和你有什么关系？」
            DATAFORM 「我在用试着想象力把你杀了，还请不要打扰我」
        ENDDATA
    ELSEIF TFLAG:193 == -1
        [IF DEBUG]
		;fuck you game, I need to debug these unreadable rambling walls of text
        TFLAG:193 = 0
        GOTO PARSEE_ALL_CONVOS
        [ENDIF]
        SELECTCASE K60_ATTITUDE()
		;Loving
            CASE 0
                SELECTCASE RAND:3
                    CASE 0
                        PRINTFORMDW 帕露西温柔地对你微笑
                        PRINTFORML 「要是你没什么话题可以不用勉强自己的」
                        PRINTFORMW 「要不…我们一起做点什么吧？只有我们两个能做的」
                    CASE 1
                        PRINTFORMW 「%K60_MASTER()%，你不用说什么的」
                        PRINTFORMDW 帕露西握住你的手，轻轻捏了一下
                        PRINTFORMW 「只是在一起我就已经满足了」
                    CASE 2
                        PRINTFORML 「你还真是个伶牙俐齿的%PRINT_MALE_CN("家伙", MASTER)%…」
                        PRINTFORMW 「愿意一直和我说到没话说为止吗，我会记住的」
                        PRINTFORMDW 帕露西的耳尖变红了，她转过头不再看你
                ENDSELECT
		;Nice, horny
            CASE 1
		; SELECTCASE RAND:3
		; 	CASE 0
                PRINTFORMW 「你的嘴现在比起尬聊，我觉得用来做别的事更好呢」
		; 	CASE 1

		; 	CASE 2
		; 		PRINTFORML
		; ENDSELECT
		;Grumpy tsundere
            CASE 2
                SELECTCASE RAND:3
                    CASE 0
                        PRINTFORMDW 帕露西对你翻了翻白眼
                        PRINTFORMW 「请不要浪费时间了，如果实在没什么话题，那为什么不保持沉默呢？」
                    CASE 1
                        PRINTFORMW 「你的想法错了，行为错了，现在连说的话也错了」
                    CASE 2
                        PRINTFORMW 「如果你没什么可说的，不必勉强自己跟我说话」
                        PRINTFORMW 「幻想乡里有那么多更愉快的人可以交谈，为什么要浪费时间在我身上？」
                ENDSELECT
		;Fake nice
            CASE 3
                SELECTCASE RAND:3
                    CASE 0
                        PRINTFORMDW 帕露西眨了眨眼
                        PRINTFORMW 「嗯？不好意思，能重复一遍吗？」
                    CASE 1
                        PRINTFORML 「如果你实在没什么可说的，不必勉强自己说话」
                        PRINTFORMW 「不是每个人都有口才上的天分」
                    CASE 2
                        PRINTFORML 「你没话可说了吗？」
                        PRINTFORMW 「没关系」
                        CALL M_KOJO_EGG_COLOR_K60_JEALOUS
                        PRINTFORMW 「我也不是个很会聊天的人」
                        CALL M_KOJO_EGG_COLOR_K60
                        PRINTFORMDW 帕露西皱了皱眉…然后她平时的笑容又回来了
                        PRINTFORMDW 你是眼花了吗？
                ENDSELECT
        ENDSELECT
	;Drunk
    ELSEIF TCVAR:泥酔
        CALL M_KOJO_EGG_MESSAGE_COM_K60_300_DRUNK
	;Date
    ELSEIF CHK_DATENOW(CFLAG:TARGET:约会中)
        CALL M_KOJO_EGG_MESSAGE_COM_K60_300_DATE
	;in bed
    ELSEIF CFLAG:TARGET:添い寝中
        CALL M_KOJO_EGG_MESSAGE_COM_K60_300_SLEEP
	; ;in bath
    ELSEIF BATHROOM()
        CALL M_KOJO_EGG_MESSAGE_COM_K60_300_BATH
	;other
    ELSE
        CALL M_KOJO_EGG_MESSAGE_COM_K60_300_OTHER
    ENDIF

ENDIF
RETURN 1

