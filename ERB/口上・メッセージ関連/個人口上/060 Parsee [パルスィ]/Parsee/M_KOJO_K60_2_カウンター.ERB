;Get closer ("CHARACTER gets a little closer to you…")
;距離が近い
;==================================================
@M_KOJO_EGG_MESSAGE_COUNTER_K60_1
CALL M_KOJO_EGG_MESSAGE_COUNTER_K60_1_1
RETURN RESULT
@M_KOJO_EGG_MESSAGE_COUNTER_K60_1_1
SELECTCASE K60_ATTITUDE()
    ;Loving
    CASE 0
        SELECTCASE RAND:3
            CASE 0
                PRINTFORML 「太近了吗？抱歉」
                PRINTFORMDL 帕露西从你身边挪开
            CASE 1
                PRINTFORMDL 帕露西伸手揽住了你的腰。
                PRINTFORMDL 起初动作缓慢而僵硬，但%HE_SHE_CN(TARGET)%随后变得更加大胆和熟练，手臂紧紧地缠绕着你，好像不想让你逃离
                PRINTFORMDL 你向%HE_SHE_CN(TARGET)%露出了微笑，轻抚着%HE_SHE_CN(TARGET)%的手
            CASE 2
                PRINTFORMDL 帕露西缓缓走到你面前，牵起了你的手
        ENDSELECT
    ;Nice, horny
    CASE 1
        SELECTCASE RAND:3
            CASE 0
                PRINTFORMDL 帕露西朝你走来，“偶然”间不小心抓了抓你的屁股
            CASE 1
                PRINTFORML （我都不敢想…从%K60_MASTER()%那看来，\@ BASE:TARGET:勃起 >= 1000 ? 我 # 我的乳头\@硬的得有多明显啊？）          
            CASE 2
                PRINTFORML 「这个距离…只要稍稍晃动，鼻尖就会互相触碰在一起啊」
                PRINTFORMDL 帕露西眯眼笑了
                PRINTFORML 「不太巧的是，我好像有点喝多了，你看…平衡感都差成这样了呢…」
                PRINTFORML 「所以如果我“不小心”中做出点什么事。还请希望您不要生我的气哦」
        ENDSELECT
    ;Grumpy tsundere
    CASE 2
        SELECTCASE RAND:3
            CASE 0
                PRINTFORML 「总得有人叮着你」
                PRINTFORML 「要是让你自己一个人乱跑，大概会试图去和幻想乡里所有能喘气的玩意上床吧」
            CASE 1
                CALL M_KOJO_EGG_COLOR_K60_JEALOUS
                PRINTFORML 「离你太近了？怎么？不行吗？我就难看到了这个程度吗？如果我是%K60_NAME_WITH_HONORIFICS([[勇儀]])%你肯定就不会…」
                PRINTFORMDL 帕露西不停地向%HE_SHE_CN(TARGET)%自己发牢骚
                CALL M_KOJO_EGG_COLOR_K60
            CASE 2
                PRINTFORMDL 帕露西趁你不注意的时候，悄悄地往你这边挪了一步，尽管她没有正眼看着你，但她肯定在用余光观察你。接着又是一步，你感觉帕露西的嘴角好像在微微上扬，然后再是一步…
                PRINTFORMDL 你故意清了清嗓子，%HE_SHE_CN(TARGET)%原地吓了一跳，肩膀都在剧烈抖动，然后低着头快速地用小碎步退回到%HE_SHE_CN(TARGET)%原来的位置
        ENDSELECT
    ;Fake nice
    CASE 3
        $FAKE_NICE_COUNTER_1
        SELECTCASE RAND:3
            CASE 0
                IF SHOW_TOP(MASTER,1) == ""
                    GOTO FAKE_NICE_COUNTER_1
                ELSE
                    CALL M_KOJO_EGG_COLOR_K60_JEALOUS
                    PRINTFORML 「这件%SHOW_TOP(MASTER,1)%穿在你身上还真是漂亮啊。要是我也有钱买得起这种漂亮的衣服就好了，帕露帕露…」
                    CALL M_KOJO_EGG_COLOR_K60
                ENDIF
            CASE 1
                CALL M_KOJO_EGG_COLOR_K60_JEALOUS
                PRINTFORML 「你今天漂亮到我第一眼都差点没认出来。帕露帕露…服装方面的见解，能给我分享一些吗？」
                CALL M_KOJO_EGG_COLOR_K60
            CASE 2
                PRINTFORML 「换了个发型吗？」
        ENDSELECT
ENDSELECT
RETURN 1

