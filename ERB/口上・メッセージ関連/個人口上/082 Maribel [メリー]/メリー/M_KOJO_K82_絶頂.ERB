				CALL HPH_PRINT,@"「啊啊HPH嗯HPH」","W"
				RETURN 1
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「嗯HPH好幸福……HPH」","W"
				RETURN 1
			ELSE
				CALL HPH_PRINT,@"「再多HPH啊啊HPH」","W"
				RETURN 1
			ENDIF
		ELSEIF NOWEX:MASTER:射精 > 1 ;big load
			IF !RAND:3
				PRINTFORMW 「呼啊！ 小穴里，进来了！ 嗯呜啊！！」
			ELSEIF RAND:2
				PRINTFORMW 「哈啊…里面，满了…」
				PRINTFORMDW 梅莉幸福地笑着，抚摸着自己的小腹
			ELSE
				PRINTFORMW 「嗯啊啊！ 不行，肚子要破了！！」
			ENDIF
		ELSE
			IF RAND:3 == 0
				CALL HPH_PRINT,@"「嗯HPH多射点哦HPH」","W"
				RETURN 1
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「啊哈HPH再多射点HPH」","W"
				RETURN 1
			ELSE
				CALL HPH_PRINT,@"「多射点哦HPH」","W"
				RETURN 1
			ENDIF
		ENDIF
	ELSEIF 妊娠可否(TARGET) && CFLAG:生解禁 ;fertile, consent
		IF NOWEX:MASTER:射精 > 1 ;big load
			IF !RAND:3
				PRINTFORMW 「嗯嗯！ 呜啊啊啊！ 出来了，哈啊啊！！」
			ELSEIF RAND:2
				PRINTFORMW 「哈啊！ 哈啊啊！！」
				PRINTFORMDW 梅莉因射精感敲击小穴而颤抖着
			ELSE
				PRINTFORMW 「啊啊……小穴里……射了好多……」
				PRINTFORMW 「被射了这么多…」
			ENDIF
		ELSE
			IF !RAND:3
				PRINTFORMW 「哈啊啊…射了这么多，还不够吗…？」
			ELSEIF RAND:2
				PRINTFORMW 「哈啊…肚子里，好热…」
			ELSE
				PRINTFORMW 「啊啊…射在里面了…」
			ENDIF
		ENDIF
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「啊啊…不要，不要啊…这样……」
			CASE 1
				PRINTFORMW 「啊啊……啊……求你住手……不要射在里面……」
			CASE 2
				PRINTFORMW 「咦……难道，骗人的，住手，住手，不要射在里面，讨厌，不要啊！」
			CASE 3
				PRINTFORMW 「讨厌，啊呜，拔出去！ 不要射！」
		ENDSELECT
	ENDIF
ENDIF
RETURN 1

