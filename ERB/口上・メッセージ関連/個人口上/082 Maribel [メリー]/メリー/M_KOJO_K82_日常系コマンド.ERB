		RETURN 1
	ENDIF
	
	IF TFLAG:194 > 0
		CALL GIFTDATA_MAIN(GET_GIFTDATA(TCVAR:今日の贈り物, "本体"))
		strGift = %RESULTS%
		nPrice = RESULT:1
		IF STRFIND(RESULTS:1, "抱住") >= 0
			strReaction = 抱く
		ELSEIF STRFIND(RESULTS:1, "觀賞") >= 0
			strReaction = 眺める
		ELSEIF STRFIND(RESULTS:1, "握住") >= 0
			SIF STRFIND(RESULTS:1, "装饰") >= 0
				strReaction = 身につける
			strReaction = 握る
		ELSE
			strReaction = 身につける
		ENDIF
	ENDIF
	
	SELECTCASE TFLAG:194
	;ひやかし
	CASE -2
		;(GET_MAPID(CFLAG:MASTER:現在位置) == 5 && GIFTSHOP_NAME(5) == "香霖堂") ;kourindou check
		IF RAND:3 == 0
			PRINTFORMW 「被骂了呢，玩闹得太久了」
