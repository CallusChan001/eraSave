	CASE -1
		IF RAND:3 == 0
			PRINTFORMW 「光是看着也很有趣呢」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「光看不买对店家不好哦」
		ELSE
			PRINTFORMW 「有找到什么有趣的东西吗？」
		ENDIF
	;400点未満（BAD）
	CASE 1
		IF RAND:3 == 0
			PRINTFORMW 「那、那样啊……谢谢你啊……」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「这不是送给女孩子的东西啦……」
		ELSE
			PRINTFORMW 「总觉得有点恶心呢……」
		ENDIF
		PRINTFORMDW 梅莉看起来不太高兴……
	;400点～699点（NOMAL）
	CASE 2
		IF RAND:4 == 0
			PRINTFORMW 「收到礼物很开心，但我适合吗？」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「哎呀？你要买给我吗？谢谢你啊」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「呵呵，我有点喜欢这个呢」
		ELSE
			PRINTFORMW 「谢谢，我会好好珍惜的」
		ENDIF
		PRINTFORMDW 梅莉露出了略带羞涩的笑容。
	;700点以上（GOOD）
	CASE 3
		IF strReaction == "身につける" ;clothes
			IF RAND:2
				PRINTFORMW 「好可爱的衣服呢。谢谢你」
			ELSE
				PRINTFORMW 「衣服？ 我来到这里时只穿着随身衣物，所以很高兴。谢谢你」
			ENDIF
		ELSEIF nPrice >= 30000
			IF RAND:2
				PRINTFORML 「谢谢。那么作为回礼……」
				PRINTFORMDW 梅莉说着，亲吻了%CALLNAME:MASTER%的脸颊
			ELSE
				PRINTFORMW 「谢谢，我会好好珍惜的」
				PRINTFORMW 「%CALLNAME:MASTER%你比我想象中更有挑选这种东西的品味呢」
				PRINTFORMW 「『比想象中』的意思是，一开始并不是这么认为的吗」
			ENDIF
		ELSE
			IF RAND:4 == 0
				PRINTFORMW 「真漂亮呢。真的可以收下吗？」
				PRINTFORMW 「谢谢，我会好好珍惜的」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「竟然知道我想要什么，真厉害呢」
			ELSEIF RAND:2 == 0
				PRINTFORMW 「谢谢，我会好好珍惜的……！」
			ELSE
