;==================================================
;4,乱牡丹
;==================================================
@M_KOJO_MESSAGE_SCOM_K82_4
CALL M_KOJO_MESSAGE_SCOM_K82_4_1
RETURN 1
@M_KOJO_MESSAGE_SCOM_K82_4_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(TFLAG:50 + 500)
		IF TCVAR:破瓜 == 1
			IF TALENT:恋慕
				PRINTFORML 「确实第一次很重要，我也想好好地经历……」
				PRINTFORMW 「虽然能看得很清楚，但这……」
				PRINTFORMDW 梅莉看着摆在自己面前的大镜子
				PRINTFORMW 「嘛……既然你特意为我着想了，那也只好这样了呢」
				PRINTFORMDW 她放弃似地说道，然后慢慢地坐了下去
				PRINTFORMW 「嗯！ 呜……嗯嗯嗯！」
				PRINTFORMW 「虽然知道会痛，但这比想象中还要痛，不，是根本无法想象的程度……」
				PRINTFORMDW 睁开因疼痛而闭着的眼睛，镜子里映出的是被%CALLNAME:MASTER%抱着，小穴口流着破瓜之血，幸福地笑着的自己
			ELSE
				PRINTFORMW 「嗯嗯！ 呜、啊啊……！」
				PRINTFORMW （竟然要献给不喜欢的人……这个噩梦什么时候才能结束呢……？）
			ENDIF
		ELSE
			IF TALENT:恋慕
				PRINTFORMW 「这下，全都能看到了呢……」
				PRINTFORMDW 梅莉看着自己暴露无遗的秘处，脸涨得通红
				PRINTFORMW 「我、我知道了啦，快点结束吧？ 我要羞死了……」
			ELSE
				PRINTFORMW 「你可真有奇怪的癖好呢……」
				PRINTFORMW （被这个人救了，果然是错了吗……）
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		RETURN 1
	ELSE
		IF ABL:露出癖 >= 3 && ABL:Ｖ感覚 >= 3
			IF !RAND:3
				PRINTFORMW 「映出来了……我和，%CALLNAME:MASTER%连接在一起的地方……！」
				PRINTFORMW 「啊嗯，啊啊，镜子里的我，也真是，好淫荡啊……！」
			ELSEIF RAND:2
				PRINTFORMW 「啊啊啊！ 全部，都看到了…！」
				PRINTFORMDW 梅莉出神地凝视着镜中自己淫荡的姿态
			ELSE
				PRINTFORML 「咿呀，嗯嗯！ 嗯啊啊！」
				PRINTFORMDW 梅莉捂着脸，压抑着声音，不去看镜子
			ENDIF
		ELSE
			SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 「映出来了……我和，%CALLNAME:MASTER%连接在一起的地方……！」
					PRINTFORMW 「啊啊，%CALLNAME:MASTER%，好害羞，明明好害羞，我却、我却……！」
				CASE 1
					PRINTFORMW 「哈啊啊！ %CALLNAME:MASTER%的全部都进去了，看得清清楚楚呢…」
				CASE 2
					PRINTFORML 「呜呜……无论做多少次，该害羞的还是会害羞啊…」
					PRINTFORMDW 梅莉被镜子里的自己煽动起了羞耻心
				CASE 3
					PRINTFORMW 「这种玩法我不太习惯呢…」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
