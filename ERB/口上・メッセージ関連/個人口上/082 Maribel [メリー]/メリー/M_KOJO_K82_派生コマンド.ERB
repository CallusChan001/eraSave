			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「嗯嗯HPHHPHHPH」","W"
				CALL HPH_PRINT,@"「已经习惯了吧HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「啊HPH舔HPH」","W"
			ENDIF
