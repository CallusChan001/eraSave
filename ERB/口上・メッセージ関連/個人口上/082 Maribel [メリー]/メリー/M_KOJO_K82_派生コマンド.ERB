					CALL CHARA_TEXT([[蓮子]], @"『啊啊，梅莉也，%CALLNAME:MASTER%也，一起高潮了……！ 这个，好厉害，太厉害了！』")
					PRINTFORMW 「莲子，那样，我，嗯，啊，不、不行了……！」
			ENDSELECT
		ENDIF
		RETURN 1
	ELSE
	ENDIF
ENDIF
