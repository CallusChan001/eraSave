		CALL HPH_PRINT,@"「不行……我没体力了……HPH」","W"
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
	;満足して解放
		CALL HPH_PRINT,@"「嗯、满足了……HPH」","W"
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
	;満足したところを被逆推た
		CALL HPH_PRINT,@"「不行……现在全身都变得迟钝了……HPH」","W"
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
	;壊れるまでしてあげる
		CALL HPH_PRINT,@"「不行啊、我会坏掉的、那样也没关系吗HPH」","W"
		RETURN 1
	ENDIF
ENDIF
RETURN 1

