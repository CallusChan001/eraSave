			RETURN -1
		RETURN 1
	CASE DAYEVENT_PAIZURI, DAYEVENT_KISS, DAYEVENT_COSLEEPING, NIGHTEVENT_KISS, NIGHTEVENT_DRINK, NIGHTEVENT_SLEEPOVER
		RETURN 0 ;normal condition
	CASEELSE ;disabled, no dialogue
		RETURN -1
ENDSELECT
