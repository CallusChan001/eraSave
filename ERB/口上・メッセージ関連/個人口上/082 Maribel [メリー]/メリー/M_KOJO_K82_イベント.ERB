@M_KOJO_CHECK_K82_DAYEVENT_COND(ARGS, ARG, ARG:1)
SELECTCASE ARG
	CASE DAYEVENT_GENERAL ;initial, if the function is allowed to run at all, 0 is disallowed, 1 is allowed
		SIF !TALENT:恋慕
			RETURN -1
		SIF MARK:反発刻印 || CFLAG:ブチギレ
			RETURN -1
		RETURN 1
	;per event
	;RETURN -1 - means disallowed, 0 - fall back to original condition, 1 - allowed
	CASE DAYEVENT_ORAL
		SIF !HAS_PENIS(MASTER) ;bj only
			RETURN -1
		RETURN 0 ;og condition otherwise
	CASE DAYEVENT_COWGIRL, NIGHTEVENT_YOBAI
		SIF !HAS_PENIS(MASTER) ;penis only
			RETURN -1
		SIF !(ABL:欲望 >= 5)
