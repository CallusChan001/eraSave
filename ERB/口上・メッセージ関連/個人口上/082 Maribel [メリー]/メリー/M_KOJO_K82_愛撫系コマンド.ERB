			ELSE
				PRINTFORMW 「虽然很舒服……但只有我一个人这样也不太好……」
				PRINTFORMW 「也想要%CALLNAME:MASTER%的呢……」
			ENDIF
		ELSEIF RAND:5 == 0 && !IS_FALLEN()
			PRINTFORMW 「啊、啊呜，那、那样做的话，我……！」
		ELSEIF RAND:4 == 0 && !IS_FALLEN()
			PRINTFORMW 「嗯，呼……嗯呜！」
			PRINTFORMDW 用指腹轻轻抚摸，梅莉的身体猛地一颤
		ELSEIF RAND:3 == 0
			CALL HPH_PRINT,@"「不许碰小穴HPH」","W"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯HPH等等HPH不行HPH」","W"
		ELSE
			CALL HPH_PRINT,@"「嗯HPH」","W"
			CALL HPH_PRINT,@"「手指HPH好粗HPH」","W"
		ENDIF
	ENDIF
ENDIF
