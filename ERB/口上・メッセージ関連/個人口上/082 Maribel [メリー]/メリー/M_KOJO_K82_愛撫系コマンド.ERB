	ELSE
		IF !FLAG:70 && IS_VIRGIN(TARGET) && CFLAG:2 >= 1500 && FIRSTTIME("MarisaFingerPop")
			PRINTFORML
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「嗯，啊啊……咦？ 莲子，怎么了？」
				CALL CHARA_TEXT([[蓮子]], "『怎么样，梅莉，就这样，到最深处……』", "w")
