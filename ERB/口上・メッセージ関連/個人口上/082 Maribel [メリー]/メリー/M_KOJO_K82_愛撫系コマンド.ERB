;==================================================
;20,接吻
;==================================================
@M_KOJO_MESSAGE_COM_K82_20
CALL M_KOJO_MESSAGE_COM_K82_20_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K82_20_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF EXP:キス経験 == 0
			PRINTFORMW 「嗯……！」
			CALL HPH_PRINT,@"「……呵呵，初吻哦，谢谢你HPH」","W"
		ELSE
			PRINTFORMW 「嗯……接吻的时候可能最幸福了……」
			PRINTFORMW 「感觉真的是被爱着……」
			CALL HPH_PRINT,@"「……气氛是要营造的呢，谢谢你HPH」","W"
		ENDIF
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF RAND:6 == 0
			PRINTFORMW 「嗯……啾……」
			PRINTFORMW 「%CALLNAME:MASTER%，好高兴……最喜欢你了……」
		ELSEIF RAND:5 == 0
			PRINTFORML 「啾，嗯嗯…啾噜，噗哈…喂，再多亲亲好不好？」
			PRINTFORMDW 梅莉主动凑上嘴唇，舌头缠了上来
			PRINTFORMDW %CALLNAME:MASTER%回应后，她便缠着要更多更多的吻
		ELSEIF RAND:4 == 0
			PRINTFORMW 「嗯，嗯嗯…啾噜，哈呼…光是接吻就感觉舒服起来了…」
			PRINTFORMDW 就像梅莉说的那样，她的秘处饥渴地抽动着
		ELSEIF RAND:3 == 0
			CALL HPH_PRINT,@"「嗯HPH再多一点……HPH」","W"
			CALL HPH_PRINT,@"「想和你紧密相连……HPH」","W"
		ELSEIF RAND:2 == 0
			PRINTFORMW 「……可以吗？光是接吻」
			PRINTFORMDW 梅莉一边说着，一边又夺走了%CALLNAME:MASTER%的嘴唇。
		ELSE
			CALL HPH_PRINT,@"「嗯嗯嗯HPH」","W"
			PRINTFORMDW 梅莉被夺去嘴唇，任由%CALLNAME:MASTER%摆布。
		ENDIF
	;それ以外
	ELSE
		IF RAND:4 == 0
			PRINTFORMW 「果然气氛很重要呢……嗯，啾……」
		ELSEIF RAND:3 == 0
			CALL HPH_PRINT,@"「嗯嗯……HPH」","W"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯嗯HPH喜欢HPH」","W"
		ELSE
			CALL HPH_PRINT,@"「嗯嗯HPH」","W"
			PRINTFORMDW 梅莉被亲吻后，更加用力地将嘴唇压了上来。
		ENDIF
	ENDIF
ENDIF
RETURN 1


;==================================================
;21,什麼也不做
;==================================================
@M_KOJO_MESSAGE_COM_K82_21
CALL M_KOJO_MESSAGE_COM_K82_21_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K82_21_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯哼，我喜欢这种焦躁play」
	ENDIF
	;基本セット
	;时奸中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;それ以外
	ELSE
		IF IS_HERE([[蓮子]]) && ONCE("MerryRenkoDoNothing")
			PRINTFORMW 「你刚才一直盯着我的脸看，怎么了吗？」
			CALL CHARA_TEXT([[蓮子]], "『哎……不，没什么啦』", "w")
			PRINTFORMW 「是吗……」
		ELSEIF TALENT:恋慕
			IF RAND:2
				PRINTFORMW 「呼……果然还是累了呢」
				PRINTFORMDW 梅莉说着，躺在了%CALLNAME:MASTER%的旁边
			ELSE
				PRINTFORMW 「要休息吗？ 那我想要个臂枕」
				PRINTFORMDW 梅莉像撒娇的猫一样凑了过来
			ENDIF
		ELSEIF PALAM:欲情 >= 3
			PRINTFORMW 「不要再吊胃口了……%CALLNAME:MASTER%……」
		ELSEIF RAND:5 == 0
			PRINTFORMW 「这、这样什么都不做，果然还是会感到害羞呢」
			PRINTFORMDW 梅莉拉过床单，遮住了自己的裸体
			PRINTFORMDW 真可惜
		ELSEIF RAND:4 == 0
			PRINTFORMW 「我会忍耐，会忍耐的……但是……」
		ELSEIF RAND:3 == 0
			CALL HPH_PRINT,@"「身体痒得受不了……HPH」","W"
		ELSEIF RAND:2 == 0
			CALL HPH_PRINT,@"「嗯嗯HPH快点啦HPH」","W"
		ELSE
			PRINTFORMW 「接下来要做什么？」
		ENDIF
	ENDIF
ENDIF
RETURN 1

