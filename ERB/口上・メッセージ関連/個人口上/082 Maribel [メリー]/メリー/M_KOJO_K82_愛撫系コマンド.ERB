					PRINTFORMW （莲子，对不起……）
				ENDIF
				CALL COLORMESSAGE(@"处女丧失 (%CALLNAME:TARGET%)", 0xF7ABA6, 2)
				CALL SET_HISTORY_LOST_V(TARGET, IS_HERE([[蓮子]]) ? [[蓮子]] # MASTER, "指")
