RETURN RESULT

@M_KOJO_MESSAGE_COM_K82_186_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF !FLAG:70 && LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF IS_HERE([[蓮子]])
			PRINTFORMW 「要在镜子前做吗？ 又想到些奇怪的点子呢……」
			PRINTFORMW 「莲子……梦和现实是相同的，但是……镜子的世界和现实的世界是相同的吗？」
			CALL CHARA_TEXT([[蓮子]], "『梅莉，我之前说过吧。梦会变成现实，要把梦的世界变成现实哦』", "w")
			CALL CHARA_TEXT([[蓮子]], "『即使镜子的世界是相同的，哪怕那是一个不同的世界』", "w")
			CALL CHARA_TEXT([[蓮子]], "『只要像是向那个世界的两个人炫耀一样做就好了哦』", "w")
		ELSE
			PRINTFORMW 「镜子啊……在这里做这种事……光是想象就够厉害了呢……」
			PRINTFORMW 「梦和现实是相同的……镜子的世界和现实的世界是相同的吗？」
			PRINTFORMW 「明明只是反过来而已，却像是映照出另一个世界……」
		ENDIF
		RETURN 1
	ENDIF
	;基本套装
	;开始
	IF TEQUIP:羞恥プレイ
		;恋慕
		IF TALENT:恋慕
			IF IS_HERE([[蓮子]])
				CALL CHARA_TEXT([[蓮子]], "『镜子里的我们也很害羞……而且看起来很开心呢』", "w")
				CALL CHARA_TEXT([[蓮子]], "『看，梅莉，我们来向镜子里的我们炫耀吧』", "w")
			ELSE
				IF ABL:露出癖 > 3
					PRINTFORML 「又要在镜子前做吗？ 一如既往地变态呢」
					PRINTFORMDW 梅莉这样说着，她的眼眸却似乎带着一丝妩媚
				ELSE
					PRINTFORMW 「%CALLNAME:MASTER%，又要在我镜子前做吗？」
					PRINTFORMW 「果然，总觉得很不可思议呢……」
				ENDIF
			ENDIF
			RETURN 1
		;屈服3
		ELSEIF MARK:不埒刻印 == 3
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「现实和镜子……哪个是真，哪个是假」
				CALL CHARA_TEXT([[蓮子]], "『……梅莉？』", "w")
				PRINTFORMW 「现在的我不知道……」
			ELSE
				PRINTFORMW 「呜呜…又这样让我做这么羞耻的事…」
				PRINTFORMW 「如果镜子里的我和现实的我能互换的话，真想互换啊……」
				PRINTFORMW 「可能两者都是一样的吧……」
			ENDIF
			RETURN 1
		;其他情况
		ELSE
			IF IS_HERE([[蓮子]])
				IF ABL:露出癖 >= 3
					PRINTFORMW 「明明应该很害羞的……总觉得很不可思议……」
					CALL CHARA_TEXT([[蓮子]], "『明明只是相反而已，却像是另一个世界呢……』", "w")
					CALL CHARA_TEXT([[蓮子]], "『来，梅莉，我们开始吧』", "w")
				ELSEIF ABL:露出癖 >= 1
					CALL CHARA_TEXT([[蓮子]], "『在镜子前做也很有趣哦。映照出来的自己就像不是自己一样呢……喂梅莉，你怎么了？』", "w")
					PRINTFORMW 「……不，没什么。…………」
				ELSE
