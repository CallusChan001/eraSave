;==================================================
;44,バイブ
;==================================================
@M_KOJO_MESSAGE_COM_K82_44
CALL M_KOJO_MESSAGE_COM_K82_44_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K82_44_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF !FLAG:70 && LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TCVAR:破瓜 == 1
			IF TALENT:恋慕
				IF IS_HERE([[蓮子]])
					IF ABL:レズ中毒 >= 3 && ABL:従順 >= 5 && MARK:反発刻印 == 0
						CALL CHARA_TEXT([[蓮子]], "『呐，梅莉……这个……是那个……』", "w")
						PRINTFORMW 「这个是……这里竟然连这种东西都有……。……莲子？」
						CALL CHARA_TEXT([[蓮子]], "『……梅莉，我有个请求。你的第一次，我可以得到吗？』", "w")
						PRINTFORMW 「哎……。……、……嗯，如果是莲子的话，可以。来吧……」
						PRINTFORMW （%CALLNAME:MASTER%，对不起，但是，我……）
					ELSE
						PRINTFORMW 「这个是……等一下，莲子，难道」
						CALL CHARA_TEXT([[蓮子]], "『没错，梅莉，你的第一次，我收下了』", "w")
						PRINTFORMW 「你、你在开玩笑吧莲子，我，我的第一次，是想给%CALLNAME:MASTER%的……」
						CALL CHARA_TEXT([[蓮子]], @"『我得到%CALLNAME:MASTER%的允许了……来吧，我要开始了』", "w")
						PRINTFORMW 「怎么会，骗、骗人的……%CALLNAME:MASTER%，为什么……」
					ENDIF
					CALL COLORMESSAGE(@"处女丧失 (%CALLNAME:TARGET%)", 0xF7ABA6, 2)
					CALL SET_HISTORY_LOST_V(TARGET, [[蓮子]], "震动棒")
				ELSE
					IF ABL:従順 >= 5
						PRINTFORMW 「哎，那个难道是……！」
						PRINTFORMW 「……不，没关系，如果%CALLNAME:MASTER%希望如此的话」
						PRINTFORMDW 震动棒插入梅莉体内的前一刻，梅莉流下了一滴泪水……
					ELSE
						PRINTFORMW 「哎，那个难道是……！」
						PRINTFORMW 「%CALLNAME:MASTER%，我还没有做过这种事，第一次，是想给%CALLNAME:MASTER%的啊……！」
					ENDIF
					PRINTFORMW 「嗯嗯！ 嗯咕…又痛，又不直接给我，真是糟糕透顶…」
					PRINTFORMW 「这个责任你可要好好负起来哦」
				ENDIF
			ELSE
				IF MARK:反発刻印
					PRINTFORMW 「……那个是……」
					PRINTFORMW 「你要用那个夺走我的第一次吗……好吧，反正我已经……」
					PRINTFORMW 「……事到如今……你也不是会犹豫的人吧，你……。要做就快点……」
					PRINTFORMW 「…………」
				ELSE
					PRINTFORMW 「哎，那个难道是……！」
					PRINTFORMW 「求你，住手，住手！！ 我，还没有……！」
					PRINTFORMW 「好痛！ 好痛好痛！ 拔出来，拔出来啊！」
					PRINTFORMDW 梅莉因震动棒插入秘唇的异物感和破瓜的疼痛而呜咽哭泣
				ENDIF
			ENDIF			
			RETURN 1
		ELSE
			IF TALENT:恋慕
				PRINTFORMW 「竟然连这种东西都有……」
				PRINTFORMW 「总觉得形状有点眼熟呢…呜呜，不要太弄痛我哦？」
			ELSE
				PRINTFORMW 「那、那么大的东西，怎么可能塞得进去啊…我、我知道了啦，不要露出那么可怕的表情啦」
			ENDIF 
			RETURN 1
		ENDIF
	ENDIF
	;基本セット
	;装着
	IF TEQUIP:バイブ
		;時姦中
		IF FLAG:70
			;PRINTFORMDW 
			RETURN 1
		;恋慕
		ELSEIF TALENT:恋慕
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「……又是这个？」
				CALL CHARA_TEXT([[蓮子]], "『不讨厌吧？ 梅莉戴着这个的时候看起来很舒服啊』", "w")
			ELSE
				IF !RAND:3
					PRINTFORMW 「又是这个？ 可以的话，我想用你的……」
				ELSEIF RAND:2
					PRINTFORMW 「嗯嗯！ 呼啊啊！」
					PRINTFORMDW 梅莉的秘处将震动棒吞噬到深处，身体随着震动而颤抖
				ELSE
					PRINTFORMW 「又是那个？ 其实我想让你来帮我……」
					PRINTFORMW 「我知道了啦，知道了就别露出那么遗憾的表情啦」
				ENDIF
			ENDIF
		;其他情况
		ELSE
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「比起这个我……只想和莲子单纯地相拥……」
				CALL CHARA_TEXT([[蓮子]], "『说得真让人高兴呢，不过这次是这个哦』", "w")
				CALL CHARA_TEXT([[蓮子]], "『既然有这么好的东西，就得有效利用起来』", "w")
			ELSEIF RAND:2
				PRINTFORMW 「又拿出这种东西…如果我说无法满足你的话怎么办？」
			ELSE
				PRINTFORMW 「那个，让我感觉很奇怪，所以不太喜欢呢…」
			ENDIF
		ENDIF
	;解除
	ELSE
		;时奸中
		IF FLAG:70
			;PRINTFORMDW 
			RETURN 1
		ELSEIF TALENT:恋慕
			IF IS_HERE([[蓮子]])
				CALL CHARA_TEXT([[蓮子]], "『梅莉，我要拔出来了哦？』", "w")
				PRINTFORMW 「呜……嗯！ 啊……哈啊……」
			ELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「呜……嗯！ 哈啊……」
						PRINTFORMW 「舒服是舒服，但这种事果然还是……」
					CASE 1
						PRINTFORMW 「下一次，用你的……」
					CASE 2
						PRINTFORMW 「嗯，哈啊…结束了吗？ 那么，下一次…呢？」
				ENDSELECT
			ENDIF
		ELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「嗯呜…哈啊…结束了吗…？」
				CASE 1
					PRINTFORMW 「呜呜…股间火辣辣的…」
			ENDSELECT
		ENDIF
	ENDIF
ENDIF
RETURN 1

