;==================================================
;100,スパンキング
;==================================================
@M_KOJO_MESSAGE_COM_K82_100
CALL M_KOJO_MESSAGE_COM_K82_100_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K82_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF !FLAG:70 && LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「痛、等等，我做了什么坏事吗，啊呜！」
		PRINTFORMW 「求你，疼痛什么的……不、住手！ 如果有什么不对的地方我会注意的，再这样下去……！」
		RETURN 1
	ENDIF
	;基本套装
	;时奸中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF ABL:マゾっ気 >= 3
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「哈啊、嗯嗯！ 莲、莲子，我，好奇怪，明明被打了，却有快感了……！」
				PRINTFORMW 「莲子，我，为什么……！」
			ELSEIF RAND:2
				PRINTFORMW 「痛、啊啊……%CALLNAME:MASTER%，真是的……我……」
			ELSE
				PRINTFORMW 「啊啊，我，变、变得奇怪了……明明被打了，却感觉到了……！」
				PRINTFORMW 「%CALLNAME:MASTER%，我，啊啊，为什么，为什么会这样……！」
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORMW 「对不起，对不起……！ 我讨厌疼痛……」
			ELSE
				PRINTFORMW 「痛 我讨厌疼痛……」
			ENDIF
		ENDIF
		RETURN 1
	;屈服3
	;ELSEIF MARK:不埒刻印 == 3
	;	RETURN 1
	;それ以外
	ELSE
		IF ABL:マゾっ気 >= 3
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「好痛……莲子……」
				CALL CHARA_TEXT([[蓮子]], "『嘴上这么说……身体是不是有点，发痒了？』", "w")
				PRINTFORMW 「……！ 才、才不是那样，没有啦……」
			ELSE
				PRINTFORMW 「好痛、啊啊……嗯，讨厌，不要啊……」
				PRINTFORMW （为什么……明明被打了，明明很痛，身体却在发痒……？）
			ENDIF
		ELSE
			PRINTFORMW 「哎，你做什么……好痛，好痛！！」
			PRINTFORMW 「你干什么，竟然想用暴力让我顺从……！」
			PRINTFORMW 「痛、不要打我，求你，不要啊……！」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;104,アイマスク
;==================================================
@M_KOJO_MESSAGE_COM_K82_104
CALL M_KOJO_MESSAGE_COM_K82_104_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K82_104_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF !FLAG:70 && LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋慕
			PRINTFORMW 「一片漆黑……。喂……%CALLNAME:MASTER%，请待在我身边哦……」
		ELSE
			PRINTFORMW 「让我看不见……你打算做什么？」
		ENDIF		
		RETURN 1
	ENDIF
	;基本套装
	;装着
	IF TEQUIP:アイマスク
		IF TALENT:恋慕
			IF IS_HERE([[蓮子]])
				IF ABL:マゾっ気 >= 3
					PRINTFORMW 「在看不见的情况下被做什么……什么的……」
					CALL CHARA_TEXT([[蓮子]], "『会兴奋吗？ 在看不见的情况下被做什么呢』", "w")
					PRINTFORMW 「才、才没有……那种事，啦……」
				ELSE
					PRINTFORMW 「什么都看不见，一想到什么时候会被做什么……」
					CALL CHARA_TEXT([[蓮子]], "『会兴奋吗？』", "w")
					PRINTFORMW 「我……不太明白……。但是，如果是莲子的话……」
				ENDIF
			ELSE
				PRINTFORMW 「什么都看不见，一想到会被你做什么……啊啊……」
			ENDIF
		;屈服3
		;ELSEIF MARK:不埒刻印 == 3
		;	RETURN 1
		;それ以外
		ELSE
			IF IS_HERE([[蓮子]])
				IF ABL:マゾっ気 >= 3
					PRINTFORMW 「在看不见的情况下被做什么……什么的」
					CALL CHARA_TEXT([[蓮子]], "『在看不见的情况下被做什么，会感到兴奋吗？』", "w")
					PRINTFORMW 「才、才没有……那种事啦。只是有点，害怕而已……」
				ELSE
					CALL CHARA_TEXT([[蓮子]], "『黑漆漆的很可怕之类的，没关系吗？』", "w")
					CALL CHARA_TEXT([[蓮子]], "『但是，在看不见的情况下被做什么，我想也会很兴奋哦？』", "w")
					PRINTFORMW 「……我，不太明白。也许最讨厌的是看不到莲子的脸吧……」
				ENDIF
			ELSE
				PRINTFORMW 「一片漆黑……呜呜……你打算做什么啊……」
				PRINTFORMW （反而，黑暗更让我平静呢……）
				PRINTFORMW （虽然不知道会被做什么很害怕，但可以不用看到脸）
			ENDIF
		ENDIF
	;解除
	ELSE
		;恋慕
		IF TALENT:恋慕
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「啊啊，终于结束了呢……。莲子，可以抱抱我吗？ 还有点，害怕……」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%、好害怕……」
				PRINTFORMW 「拜托了，下次，看着你的脸……」
			ENDIF
		;屈服3
		;ELSEIF MARK:不埒刻印 == 3
		;	RETURN 1
		;それ以外
		ELSE
			PRINTFORMW 「要摘掉……是吗？」
		ENDIF 
	ENDIF
ENDIF
RETURN 1

;==================================================
;105,縄
;==================================================
@M_KOJO_MESSAGE_COM_K82_105
CALL M_KOJO_MESSAGE_COM_K82_105_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K82_105_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF !FLAG:70 && LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋慕
			PRINTFORMW 「%CALLNAME:MASTER%，绳子要用来做什么……哎，绑我？ 有点太热狂了吧？」
			PRINTFORMW 「不要绑得太紧哦……」
		ELSE
			PRINTFORMW 「什么……哎，绑我……绑我吗？」
			PRINTFORMW 「开、开玩笑的……不、不要啊，好痛，好痛啊，不要绑那么紧！」
		ENDIF 
		RETURN 1
	ENDIF
	;基本套装
	;装着
	IF TEQUIP:縄
		IF ABL:マゾっ気 >= 3
			IF IS_HERE([[蓮子]])
				CALL CHARA_TEXT([[蓮子]], "『湿了……梅莉，难道说……』", "w")
				PRINTFORMW 「呜……嗯……我、我……被绑着，却感觉到了……！」
				PRINTFORMW 「求你%CALLNAME:MASTER%，再多折磨我，让我变得奇怪吧！ 我忍不住了！」
			ELSE
				PRINTFORMW 「被绑得紧紧的，动弹不得……不知道会被做什么，很害怕」
				PRINTFORMW 「明明疼痛却很舒服……求你%CALLNAME:MASTER%，再多让我变得奇怪吧！」
			ENDIF
		ELSEIF TALENT:恋慕
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「唔……」
				CALL CHARA_TEXT([[蓮子]], "『痛吗？ 但要忍耐哦。不然就不能算是调教了』", "w")
				PRINTFORMW 「没、没关系……我会忍耐的……」
				PRINTFORMW （莲子竟然有这种癖好吗……）
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%，再放松一点……」
				PRINTFORMW 「呜呜……而且这样我也无法侍奉你了……」
			ENDIF
		;屈服3
		ELSEIF MARK:不埒刻印 == 3
			PRINTFORMW 「唔，呜呜……%CALLNAME:MASTER%，好痛……」
		;其他情况
		ELSE
			IF ABL:マゾっ気 >= 3
				PRINTFORMW 「如果要绑的话，至少不要绑得太紧……我讨厌疼痛……」
			ELSE
				PRINTFORMW 「呜……！ 不要绑那么紧……我不会逃的」
			ENDIF
		ENDIF
	;解除
	ELSE
		;恋慕
		IF TALENT:恋慕
			IF IS_HERE([[蓮子]])
				PRINTFORMW 「莲子，结束了吗？」
				CALL CHARA_TEXT([[蓮子]], "『哎？ 你还想继续吗？』", "w")
				PRINTFORMW 「动不了什么的，有点……」
			ELSEIF ABL:マゾっ気 >= 3
				PRINTFORMW 「就这样动弹不得被%CALLNAME:MASTER%单方面地对待也不错」
				PRINTFORMW 「但是，我也想侍奉你啊……」
			ELSE
				PRINTFORMW 「啊，好痛……果然，我还是不喜欢疼痛……」
			ENDIF
		;其他情况
		ELSE
			PRINTFORMW 「唔，呜呜……全是勒痕……好过分……」
		ENDIF
	ENDIF
ENDIF
RETURN 1

