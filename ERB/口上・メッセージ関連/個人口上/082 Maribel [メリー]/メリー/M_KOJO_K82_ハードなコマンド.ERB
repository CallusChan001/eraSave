;==================================================
;140,イラマチオ custom code expanded
;==================================================
@M_KOJO_MESSAGE_COM_K82_140
CALL M_KOJO_MESSAGE_COM_K82_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K82_140_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF !FLAG:70 && LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF ABL:奉仕精神 >= 3
			PRINTFORMW 「嗯，嗯噗，嗯嗯ー！！」
			PRINTFORMW 「噗哈，哈啊，哈啊，要是想激烈地做，事先告诉我一声就好了」
			PRINTFORMW 「嗯，我会努力的，你可以继续了哦」
		ELSE
			PRINTFORMW 「嗯咕，嗯嗯，呜咕咕，……嗯嗯ー！」
			PRINTFORMW 「咳，咳，那种，强行……嗯噗！」
			PRINTFORMW 「呜，呜呃呃呃……不、不、住手，嗯咕！」
		ENDIF 
		RETURN 1
	ENDIF
	;基本套装
	IF FLAG:70
	ELSEIF TALENT:恋慕
		IF ABL:奉仕精神 >= 3
			PRINTFORMW 「又要激烈地做吗？ 好啊，尽情地侵犯我的嘴吧」
			PRINTFORMW 「嗯咕，嗯，滋噗，……噗哈，再多，多做点……」
		ELSE
			PRINTFORMW 「又要激烈地做吗？ 好啊，虽然很痛苦，但我会忍耐的」
			PRINTFORMW 「嗯咕，嗯，滋噗，……噗哈，哈啊，我会努力的，再多来点……」
		ENDIF
		RETURN 1
	;其他情况
	ELSE
		IF !RAND:3
			PRINTFORMW （好痛苦……但就这样窒息……是不可能的吧……）
			PRINTFORMW （只要忍耐着奉侍就好了……）
		ELSEIF RAND:2
			PRINTFORMW 「嗯，咕，滋噗，嗯嗯，呜咕……」
			PRINTFORMW 「噗哈，哈啊，哈啊，好痛苦，啊……嗯咕咕！」
		ELSE
			PRINTFORMW 「嗯咕，嗯嗯，嗯ー！！ 咳，咳、好痛苦，哈啊，哈啊……」
			PRINTFORMW 「拜、托、了，不、不要……嗯噗呜！ 嗯，嗯咕ー！」
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

