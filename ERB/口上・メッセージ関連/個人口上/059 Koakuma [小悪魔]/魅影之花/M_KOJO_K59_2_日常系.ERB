	;并不是都需要写，部分内容可以合并在一起
	IF FLAG:70 
	ELSEIF TFLAG:194 
		;非初次，拒绝
		SELECTCASE TFLAG:194
			CASE 1 		;有路人

			CASE 2		;不想在户外做

			CASE 3 		;不想多人一起做

			CASE 4 		;还没有做的心情（即使有合意）

			CASE 100	;有合意，但是因为口上特殊判定而强制失败

		ENDSELECT 
	ELSEIF TCVAR:181
		;初次拥抱，允许或失败都有
		SELECTCASE TCVAR:181
			CASE 1		;普通地获得合意

			CASE 2		;还没有KISS过就获得合意

			CASE 3		;媚药或者发情期时获得合意…

			CASE 4		;使用奇迹之石后获得合意…

			CASE 5		;被逼迫要求做且无法拒绝(拥有胆怯素质才有可能触发)

			CASE 6		;被拒绝了，但是并不是不高兴

			CASE 7		;被普通地拒绝

			CASE 8		;被强硬地拒绝

			CASE 9		;即便禁止插入，也允许了推倒

			CASE 98		;口上内特殊判定加持时失敗

			CASE 99		;口上内特殊判定加持时成功

		ENDSELECT
	ELSE 
		;非初次 成功

	ENDIF 
ENDIF
RETURN 1
