			PRINTFORMW 「今天玩的很开心呢，下次还要来找我约会哦」
			CALL ASK_YN("好的","是的")
			IF !RESULT
			ELSE
			ENDIF
			PRINTFORMW 「我很期待哦」
		ELSEIF TALENT:思慕
			PRINTFORMW 「今天就这样结束了吗，下次记得还来约我哦」
			CALL ASK_YN("好的","是的")
			IF !RESULT
			ELSE
			ENDIF
			PRINTFORMW 「哼哼、不错的回答呢、我等着哦」
		ELSE
			PRINTFORMW 「今天玩得还蛮开心的，下次再约吧」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印消去イベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML 「…看在你这么奋力的道歉上我原谅你」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印2>1レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「…别以为我会给你第二次机会」
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;反発刻印3>2レベルダウンイベント
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORML 「想要得到我的原谅？可以啊」
		PRINTFORMDL 八千慧把袜子跟鞋子脱掉后把脚踩在%CALLNAME:MASTER%头上
		PRINTFORML 「用你的舌头把我的脚舔一遍我就原谅你」
		PRINTFORMDL 看来她超级生气的…也只能照做了…
		RETURN 1
	ENDIF
	PRINTFORML
ENDIF
RETURN 1

