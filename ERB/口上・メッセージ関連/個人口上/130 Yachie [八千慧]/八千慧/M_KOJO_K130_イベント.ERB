					PRINTFORMW 「并不是什么可爱的内衣……」
				ELSE
					PRINTFORMW 「别一直看着啊」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;半脱
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			;時間停止時
			IF FLAG:70
			;通常
			ELSE
				IF RAND:3 == 0
					PRINTFORMW 「这样子会更兴奋吗？」
				ELSEIF RAND:2 == 0
					PRINTFORMW 「不想把衣服给弄脏了」
				ELSE
					PRINTFORMW 「这样子也还不错」
				ENDIF
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;胖次ちょうだい
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE ARG:1
				;-------------------------------------------------
				CASE 0
				;交換です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「要是把%CALLNAME:MASTER%的内裤给我的话就行」
						CALL ASK_YN("好的","是的")
						IF !RESULT
						ELSE
						ENDIF
						PRINTFORMW 「那就交换吧」
						PRINTFORMDW 不知道是不是错觉感觉八千慧更兴奋…
					ELSEIF RAND:2 == 0
						PRINTFORMW 「……那就来交换吧」
						CALL ASK_YN("好的","是的")
						IF !RESULT
						ELSE
						ENDIF
					ELSE
						PRINTFORMW 「我不想只是给啊，来交换吧」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 1
				;OK（嬉々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「这样就满意了吗？绝对还不止的对吧？」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「来，拿好」
					ELSE
						PRINTFORMW 「这可是我给你的，要好好感谢我哦」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 2
				;OK（渋々
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「嘛，倒也无所谓」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「啊……反正无所谓」
					ELSE
						PRINTFORMW 「真是的，想拿来干什么啊」
					ENDIF
					RETURN 1
				ENDIF
				;-------------------------------------------------
				CASE 3
				;嫌です
				;記入チェック（=0, 非表示、1, 表示）
				LOCAL:1 = 1
				;-------------------------------------------------
				IF LOCAL:1
					IF RAND:3 == 0
						PRINTFORMW 「不要」
					ELSEIF RAND:2 == 0
						PRINTFORMW 「不行」
					ELSE
						PRINTFORMW 「真是奇怪的兴趣」
					ENDIF
					RETURN 1
				ENDIF
				RETURN 1
			ENDSELECT
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

