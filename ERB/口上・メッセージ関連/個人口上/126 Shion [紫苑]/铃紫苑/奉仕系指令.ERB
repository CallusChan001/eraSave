;81,口交
;==================================================

@M_KOJO_铃紫苑_MESSAGE_MESSAGECHECK_COM_K126_81
RETURN 0

@M_KOJO_铃紫苑_MESSAGE_COM_K126_81
CALL M_KOJO_铃紫苑_MESSAGE_COM_K126_81_1
RETURN RESULT

@M_KOJO_铃紫苑_MESSAGE_COM_K126_81_1
;-------------------------------------------------
;启用（= 0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初次使用
	;启用
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW
		RETURN 1
	ENDIF
	;基本差分模板
	;時姦中
	IF FLAG:70
		PRINTFORMW
		RETURN 1
	;恋慕
	ELSEIF TALENT:恋慕
		IF NOWEX:MASTER:11
			SELECTCASE RAND:2
				CASE 0
				PRINTFORMDL 紫苑全心全意的侍奉，很快就让你来到了顶峰。
				CALL HPH_PRINT,@"「啾唔～HPH射出来，我会全部接住的。」","W"
				CASE 1
				PRINTFORML 「药粗来了嘛？可以哦，涉出来吧。」
				PRINTFORMDW 察觉到你即将到达顶峰，紫苑在保持口中的吸吮的同时，还增加了一只手撸动肉棒根部，另一种手轻轻揉捏阴囊的刺激。在这无微不至的照顾下你很快便发射了。
			ENDSELECT
		ELSE
			SELECTCASE RAND:4
				CASE 0
				PRINTFORMDL 紫苑逐渐把肉棒含的更深，慢慢吞吐，即便在口腔里，舌头也在不停地舔弄龟头。
				PRINTFORMW 「姆…哈姆，诶嘿嘿，%CALLNAME:MASTER%舒服的表情好可爱…」
				CASE 1
				PRINTFORMDW 看着紫苑的头埋在你的胯间一上一下地耸动，尽心尽力地侍奉着你，快感和幸福感填满了你的脑海。
				CASE 2
				PRINTFORMDL 紫苑偶尔会松开口中的肉棒，低下头。将你的子袋地含进暖滑湿润的口腔舔弄，一边用沾满了晶莹唾液的手不住地刺激龟头。
				PRINTFORMW 「呼呼……%CALLNAME:MASTER%稀饭这个素吧。」
				CASE 3
				PRINTFORMDW 少女的口腔毫无保留地包裹着你的肉棒，紫苑那柔软的唇瓣圈住性器，下流地来回动作
			ENDSELECT
		ENDIF
		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW
		RETURN 1
	;除此以外
	ELSE
		PRINTFORMW
		RETURN 1
	ENDIF
ENDIF
RETURN 1

