;==================================================
;80,手淫
;==================================================
@M_KOJO_MESSAGE_COM_K126_80
CALL M_KOJO_MESSAGE_COM_K126_80_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K126_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		IF TALENT:恋人 || TALENT:恋慕
			PRINTFORMW 「用手……做吗？」
			PRINTFORMW 「不要抱太大的期待」
			PRINTFORMDW 紫苑手势非常麻利、将手放在%CALLNAME:MASTER%的肉棒上
			PRINTFORMW 「嗯、战战兢兢的……」
		ENDIF
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMW
		RETURN 1
	;恋人
	ELSEIF TALENT:恋人 || TALENT:恋慕
		IF RAND:3 == 0
			PRINTFORMW 「这里、就这样……」
			CALL HPH_PRINT,@"「很舒服吧？呵呵、我知道了…HPH」","W"
		ELSEIF RAND:2 == 0
			PRINTFORMW 「这样被抚摸的话、会变得很奇怪哦」
			CALL HPH_PRINT,@"「稍微去除燃气吧HPH」","W"
		ELSE
			PRINTFORMW 「又硬又热……」
			CALL HPH_PRINT,@"「非常想要呢……HPH」","W"
		ENDIF
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORMW
		RETURN 1
	;それ以外
	ELSE
		PRINTFORMW
		RETURN 1
	ENDIF
ENDIF
RETURN 1

