		CALL ASK_M("盯着看",CFLAG:68:1001>=10,"瞟一眼",1,"不看",1,)
			SELECTCASE RESULT
				CASE 0
					PRINTFORML 「别盯着看……我，我会不好意思的……」
					PRINTFORMDL 幽香掩耳盗铃式地扭过红透了的脸
					CALL yuka_AA("浴室1")
				CASE 1
