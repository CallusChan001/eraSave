@M_KOJO_ORTHODOX_MESSAGE_COM_K49_300
;コマンド共通メッセージ(TRAIN_MESSAGE) 
;削除すると「～话をした。」とかが消えます
IF CFLAG:1496 == 2 && !RAND:2
	CALL PET49()
ELSEIF CFLAG:1496 == 1 && !RAND:2
	CALL ONRYO49()
ENDIF
SIF 陷落状态() < 3 && !TCVAR:302 && (BASE:酒気 < (MAXBASE:酒気 / 10) * 4)
	CALL TRAIN_MESSAGE
;口上メイン呼出
CALL M_KOJO_ORTHODOX_MESSAGE_COM_K49_300_1
RETURN RESULT

@M_KOJO_ORTHODOX_MESSAGE_COM_K49_300_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
		CALL D_LINE
		CALL DRUNKER49()
	ELSEIF TCVAR:302
		CALL D_LINE
		IF 陷落状态() >= 4
			IF 陷落状态() == 5
				PRINTFORML 「啊，听着%CALLNAME:MASTER%的声音，感觉真不错、偶尔来随便聊聊天吧？」
				PRINTFORML 「『很无聊吗』？　只要在%CALLNAME:MASTER%身边我都觉得很开心」
				PRINTFORML 「『真是可爱啊』……、因为%CALLNAME:MASTER%实在太棒了嘛」
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%、不用勉强自己开口也可以哦」
				PRINTFORML 「哼哼、这样试图寻找话题的样子、真是非常有趣呢」
				PRINTFORML 「而且、能见到你的心，我也很开心哦」
			ENDIF
		ELSEIF 陷落状态() == 3
			PRINTFORML 「我很喜欢%CALLNAME:MASTER%的声音、还有你的心声」
			PRINTFORML 「所以，毋须强行跟我说话也可以的哦」
		ELSEIF 陷落状态() == 2
			PRINTFORML 「不打算开口的话、毋须强行跟我说话也可以的」
		ELSEIF 陷落状态() == 1
			PRINTFORML 「明明没有话想说、却强行让自己开口吗？」
		ELSE
			PRINTFORML 「没有话要说的话，还请你闭嘴」
		ENDIF
		PRINTL
	ELSEIF 陷落状态() >= 4
		IF 陷落状态() == 5
			CALL D_LINE
			PRINTFORM 觉一边听着%CALLNAME:MASTER%的话语、
			IF CFLAG:TARGET:陪睡中
				PRINTFORML 一边带着神魂倾倒的表情、紧紧地抱着%CALLNAME:MASTER%
			ELSEIF CHK_DATENOW(CFLAG:TARGET:约会中)
				PRINTFORML 一边与%CALLNAME:MASTER%十指相扣着、面露灿烂如鲜花般的笑容
			ELSE
				PRINTFORML 一边满脸笑容地、紧紧地抱着%CALLNAME:MASTER%
			ENDIF
			PRINTL
			SELECTCASE RAND:2
				CASE 1
					PRINTFORML 「然后到底怎样了？」
					PRINTFORMDL 觉两眼放光地催促着%CALLNAME:MASTER%继续刚才的话题
					PRINTFORML 「『不会厌倦吗』？　不会哦ー、因为%CALLNAME:MASTER%话语的声音和心里的”声音”是不一样的」
					PRINTFORMDL 觉脸上带着爽朗的笑容
					PRINTFORML 「%CALLNAME:MASTER%真是非常棒呢」
					PRINTFORMDL 如此直接而充满好意的夸奖、令%CALLNAME:MASTER%感到有点害羞
				CASE 0
					PRINTFORML 「%CALLNAME:MASTER%真是知道不少新奇的事呢」
					PRINTFORMDL 紧贴着%CALLNAME:MASTER%的觉，看起来像是感到不可思议一样
					PRINTFORML 「『你早就知道了吧』？　即是我早就拥有这些知识、但那与从有经验的人口中说出来是两回事」
					PRINTFORMDL 觉脸上带着欢喜的笑容
					PRINTFORML 「而且、能够听到我最喜欢的%CALLNAME:MASTER%的声音，我也很开心」
					PRINTFORMDL 见到觉那样的笑容后、决定不再继续追问下去了
			ENDSELECT
		ELSE
			CALL D_LINE
			PRINTFORM 觉一边听着%CALLNAME:MASTER%的话语、
			IF CFLAG:TARGET:陪睡中
				PRINTFORML 一边带着神魂倾倒的表情、紧紧地抱着%CALLNAME:MASTER%
			ELSEIF CHK_DATENOW(CFLAG:TARGET:约会中)
				PRINTFORML 一边与%CALLNAME:MASTER%十指相扣着、面露神魂荡漾的笑容
			ELSE
				PRINTFORML 一边满脸笑容地。紧紧地抱着%CALLNAME:MASTER%的胳膊
			ENDIF
			PRINTL
			SELECTCASE RAND:3
				CASE 2
					PRINTFORML 「%CALLNAME:MASTER%的声音、真的很棒呢」
					PRINTFORMDL 看到现在的觉毫无初次见面的冷淡痕迹后、我自然的笑出来了
					PRINTFORMW 「……因为那时、我还不信任任何、除了家人以外的人」
					PRINTFORMDL 现在背叛的话或许会很有趣呀、心里这样想着的时候、觉羞涩地笑了
					PRINTFORML 「时停所得不到的、被人侍奉带来的悦乐，都一并舍弃掉，也无所谓吗？」
					PRINTFORMDW 那么现在就好想被觉侍奉啊、%CALLNAME:MASTER%故意这样想着…目光闪烁的觉，只是笑着看着我
				CASE 1
					PRINTFORML 「%CALLNAME:MASTER%真的很喜欢说话呢」
					PRINTFORMDL 觉笑着说道，%CALLNAME:MASTER%自然地发出了苦笑
					PRINTFORMW 「『这可不一定』？　哼哼、我可是知道的哦、%CALLNAME:MASTER%在说慌了哦」
					PRINTFORMDL 要是我说慌就被雷劈好了、%CALLNAME:MASTER%这样回答了
					PRINTFORML 「%CALLNAME:MASTER%心里所想的和心声不一样。任何慌言都骗不过我的哦」
					PRINTFORMDW 说完、微红的脸上带着害羞的神情、觉一言不发地抱住了%CALLNAME:MASTER%
				CASE 0
					PRINTFORML 「%CALLNAME:MASTER%和我说话时、真的很开心呢」
					PRINTFORMDL 没有理由会感到开心吧、这样想着时候，觉笑得更开心了
					PRINTFORMW 「哼哼、那为什么还要跟我说话呢？」
					PRINTFORMDL 纯粹只是一时兴起而已、%CALLNAME:MASTER%在内心这样回答道
					PRINTFORML 「哼哼哼、你说是就是啦～」
					PRINTFORMDW 觉愉快地说着、%CALLNAME:MASTER%不知为何也觉得开心起来
			ENDSELECT
		ENDIF
	ELSEIF 陷落状态() == 3
		CALL D_LINE
		IF CFLAG:TARGET:陪睡中
			PRINTFORML 在能够感受到对方体温的距离间、与欢笑着的觉说着话
		ELSEIF CHK_DATENOW(CFLAG:TARGET:约会中)
			PRINTFORML 与欢笑地依偎着%CALLNAME:MASTER%的觉交谈着
		ELSE
			PRINTFORML 与看起来很开心地笑着的觉交谈着
		ENDIF
		PRINTL
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「……哼哼、%CALLNAME:MASTER%的声音听起来真是美妙啊」
				PRINTFORMDL 我说的话到底有什么好呢、在这样问之前觉已经做出了回答
				PRINTFORML 「我想，是因为所思和所言的一致吧、感觉很畅快哦」
				PRINTFORMDW 觉这样说然后笑着、%CALLNAME:MASTER%也就没有再追问下去了
			CASE 1
				PRINTFORML 「%CALLNAME:MASTER%这样说的话那应该就是这样吧」
				PRINTFORMDL 看见觉愉快地笑着后、%CALLNAME:MASTER%在心里问道，实际上觉又是怎么想的呢…?
				PRINTFORML 「哼哼、我所想的就包含了你刚才说的一部分、就是这样想的哦」
				PRINTFORMDW 发现自己被戏弄后、%CALLNAME:MASTER%只能回以苦笑
			CASE 0
				PRINTFORML 「原来如此、真是充满%CALLNAME:MASTER%风格的话呢」
				PRINTFORMDL 觉钦佩地点头、%CALLNAME:MASTER%内心想道，那么简单就被我骗了吗…?
				PRINTFORML 「……虽然恋爱是盲目的、但我的”眼”还没有变得盲目哦、看穿慌言这种程度还是能做到的」
				PRINTFORMDW 觉窃笑着说道、%CALLNAME:MASTER%不禁叹了一口气
		ENDSELECT
	ELSEIF 陷落状态() == 2
		PRINTFORML 「……给我滚开，混蛋」
		PRINTFORMDL 一开口、觉就这样说到、%CALLNAME:MASTER%只能收声，嘿嘿地笑着
		PRINTFORML 「『家人真的那么重要吗』？　……看来你这个下劣卑贱污秽之辈根本不懂义理啊」
		PRINTFORMDW 眼神冷淡，压抑着愤怒而露出咬牙切齿的表情的觉、让%CALLNAME:MASTER%不禁笑了一下
	ELSEIF 陷落状态() == 1
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「原来如此、也有这种事呀」
				PRINTFORMDW 看到觉温柔的表情、%CALLNAME:MASTER%的脸上也自然地浮现笑容了
			CASE 1
				PRINTFORML 「嘛、就算是%CALLNAME:MASTER%这样的人也曾失败过呢」
				PRINTFORMDW 看到觉很开心的样子、%CALLNAME:MASTER%报以苦笑
			CASE 0
				PRINTFORML 「这可真是、值得去见识一下呢」
				PRINTFORMDW 这样说着的觉、嘴角露出了罕见的笑意
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
			CASE 2
				PRINTFORML 「……没有必要说出来吧？」
				PRINTFORMDW 只是一时兴起而已、内心这样想着的时候、觉冷淡地看了过来
			CASE 1
				PRINTFORML 「嗯、已经听到了哦。我的”眼”早就已经看见了」
				PRINTFORMDW 觉这样说到、不过即使如此，还是能感觉到觉并没有无视%CALLNAME:MASTER%
			CASE 0
				PRINTFORML 「真是无聊的话题、只有这样吗？」
				PRINTFORMDW 周围蔓延着完全无法继续话题的气氛
		ENDSELECT
	ENDIF
	PRINTFORMW 
ENDIF
RETURN 1
;==================================================
;301,お茶を淹れる
;==================================================
@M_KOJO_ORTHODOX_MESSAGE_COM_K49_301
IF CFLAG:1496 == 2 && !RAND:2
	CALL PET49()
ELSEIF CFLAG:1496 == 1 && !RAND:2
	CALL ONRYO49()
ENDIF
CALL M_KOJO_ORTHODOX_MESSAGE_COM_K49_301_1
RETURN RESULT

@M_KOJO_ORTHODOX_MESSAGE_COM_K49_301_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF (BASE:酒気 > (MAXBASE:酒気 / 10) * 4)
		CALL DRUNKER49()
	ELSEIF 陷落状态() >= 4
		IF GETBIT (CFLAG:1490,3)
			IF TALENT:恋慕
				IF 陷落状态() == 5
					PRINTFORML 「%CALLNAME:MASTER%、今日没有下药吗？　真是可惜啊～」
					PRINTFORMDL 听到觉希望下药后，感到有些意外的%CALLNAME:MASTER%，对觉苦笑着
					PRINTFORMW 「因为、%CALLNAME:MASTER%下药后就会和我Ｈ了」
					PRINTFORMDL 觉满脸笑容低声嘟嚷着
					PRINTFORML 「虽然没有下药的茶也很好喝就是了」
					PRINTFORMDW 这样说完，觉便开始喝茶了
				ELSE
					PRINTFORML 「%CALLNAME:MASTER%、茶里没有下药吗？」
					PRINTFORMDL 你想要我下药吗？　%CALLNAME:MASTER%这样问后、觉苦笑起来
					PRINTFORMW 「如果可以的话我敬谢不敏、但如果%CALLNAME:MASTER%想这样做的话我没所谓哦～」
					PRINTFORMDL 现在不是说这种话题的时候吧…
					PRINTFORML 「哼哼、如此任性的%CALLNAME:MASTER%，我也不讨厌哦」
					PRINTFORMDW 觉低声嘟嚷着，然后开始喝茶了
				ENDIF
			;恋慕も持たずに恋人とな？
			ELSE
				PRINTFORML 「太好了、没有下药呢」
				PRINTFORMDL 下了药你就不会喝了呀、%CALLNAME:MASTER%这样想着同时、觉微笑着说道
				PRINTFORMW 「当然。就算我们是恋人、也有可以做的事和不能做的事哦」
				PRINTFORML 「非常亲密的事姑且不论、这种会被人发现的事还是希望能不要做比较好」
				PRINTFORMDL %CALLNAME:MASTER%没有回答、而是悄悄地将自己秘藏的茶点拿了出来
				PRINTFORMDW 觉开心地收下了
			ENDIF
		ELSE
			IF 陷落状态() == 5
				PRINTFORML 「嗯哼哼、%CALLNAME:MASTER%的茶、好好喝好喜欢～」
				PRINTFORMDL 觉满面笑容地说着、%CALLNAME:MASTER%感受不到任何恶意
				PRINTFORML 「我明明是真的觉得开心、你到底在想什么呀？」
				PRINTFORMDL 我无法读觉的心、所以我就只能靠观察，才能知道你是在说真心话还是在奉承我了、%CALLNAME:MASTER%这样回答道
				PRINTFORML 「我讨厌这样的%CALLNAME:MASTER%哦～」
				PRINTFORMDL %CALLNAME:MASTER%苦笑地抚摸着嘟着嘴的觉的头
			ELSE
				PRINTFORML 「%CALLNAME:MASTER%、谢谢你。我很开心」
				PRINTFORMDL 觉收下茶和茶点、愉快地看向我
				PRINTFORMW 「『纯粹只是一时兴起』？　哼哼、就算只是一时兴起我也很开心」
				PRINTFORMDL 觉窃笑着拿起了茶和茶点
				PRINTFORML 「你泡的茶和那群孩子泡的茶味道不同、能喝到你泡的茶我很开心哦」
				PRINTFORMDW 听到觉这样说后、我就不打算再说什么话了
			ENDIF
		ENDIF
	ELSEIF 陷落状态() == 3 || TALENT:184 || TALENT:185
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「啊啦、今天没有泡药进去吗？」
			PRINTFORMDL 觉窃笑地嘟嚷着
			PRINTFORML 「『现在就算了吧』？　哼哼、哼哼嗯、%CALLNAME:MASTER%真是任性呢」
			PRINTFORMDW 不知为何很开心的觉、窃笑着接过茶喝了起来
		ELSE
			PRINTFORML 「真好喝呢。是%CALLNAME:MASTER%帮我泡的，对吧」
			PRINTFORMDL 谁来泡茶也都一样吧、就在%CALLNAME:MASTER%这样嘟嚷着的时候、觉脸上的笑容变成了苦笑
			PRINTFORML 「不、就算是家里的孩子们、阿燐的话很擅长泡茶、阿空就……」
			PRINTFORMDW 那两人的话的确很可能是这样呢、%CALLNAME:MASTER%心里这样想着时候、觉苦笑地点了点头
		ENDIF
	ELSEIF 陷落状态() == 2
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「『我没有下药』……、这不是理所当然吗」
			PRINTFORMDL 觉冷淡地瞪着这边
			PRINTFORML 「『这次没有』？　真是可惜、那是没用的哦」
			PRINTFORMDW 觉深深地叹了一口气、表情有些呆然
		ELSE
			PRINTFORML 「你不会觉得斟茶递水这种程度的事就能笼络我吧」
			PRINTFORMDL 觉一边不快地嘟嚷着、一边伸出手接过了茶和茶点
			PRINTFORML 「……什么啊？」
			PRINTFORMDW 察觉到接过茶的觉带有的一丝快乐后、%CALLNAME:MASTER%嘿嘿地奸笑着
		ENDIF
	ELSEIF 陷落状态() == 1
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「太好了、今日的茶没有下药啊」
			PRINTFORMDW 觉微笑着含糊地说道
		ELSEIF TFLAG:193 == 1
			PRINTFORML 「%CALLNAME:MASTER%很擅长泡茶呢」
			PRINTFORMDW 觉开心地笑着、%CALLNAME:MASTER%也以笑容作为回应了
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「……那个、虽然谢谢你、但这个有点」
			PRINTFORMDW 见到觉困惑的神情后、低头尝了尝茶的味道的%CALLNAME:MASTER%，因感到难喝而有点僵住了
		ELSE
			PRINTFORML 「哼哼、谢谢你」
			PRINTFORMDW 觉微笑地接过茶后开始喝了起来
		ENDIF
	ELSE
		IF GETBIT (CFLAG:1490,3)
			PRINTFORML 「以后、也请你不要再下药了」
			PRINTFORMDW 觉一边接过茶一边不愉快地低声嘟嚷
		ELSEIF TFLAG:193 == 1
			PRINTFORML 「谢谢你……、很好喝」
			PRINTFORMDW 觉少见地稍微有点开心地微笑着
		ELSEIF TFLAG:193 == -1
			PRINTFORML 「……谢了」
			PRINTFORMDW 觉喝了一口茶的瞬间、露出了难以形容的表情
		ELSE
			PRINTFORML 「谢谢你」
			PRINTFORMDW 表情毫无变化的觉、接过茶并开始喝了起来
		ENDIF
	ENDIF
	CLEARBIT CFLAG:1490,3
ENDIF
RETURN 1
