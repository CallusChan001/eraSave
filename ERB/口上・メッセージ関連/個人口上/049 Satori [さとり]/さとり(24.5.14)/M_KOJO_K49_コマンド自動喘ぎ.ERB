;-------------------------------------------
;陷落状态の指定
;ARGを入力しなければ、TARGET入力
;-------------------------------------------
@陷落状态(ARG)
#FUNCTION
SIF !ARG
	ARG = TARGET
IF TALENT:ARG:恋人
	;原版觉的撒娇模式、少女长咲夜恋人模式、其他剧本版本和妹红剧本
	IF CFLAG:清い交際 && ((ARG == 49 && GETBIT(CFLAG:ARG:1490,6) && CFLAG:ARG:口上セレクタ == 3) || (ARG == 49 && CFLAG:ARG:口上セレクタ != 3) || (ARG == 15 && CFLAG:ARG:口上セレクタ == 1 && !CFLAG:ARG:1151) || (ARG == 15 && CFLAG:ARG:口上セレクタ != 1) || (ARG != 49 && ARG != 15))
		RETURNF 5
	;通常恋人
	ELSE
		RETURNF 4
	ENDIF
ELSEIF ARG == 49 && MARK:ARG:反発刻印 >= 3
	RETURNF 2
;恋慕orセフレor愛人or咲夜時姦バレ＋思慕
ELSEIF TALENT:ARG:恋慕 || TALENT:ARG:愛欲 || TALENT:ARG:炮友
	RETURNF 3
;妹紅合意orさとり家族時姦
ELSEIF (ARG == 49 && GETBIT (CFLAG:ARG:1490,0)) || (ARG == 61 && GETBIT (CFLAG:ARG:1 ,1))
	RETURNF 2
;通常思慕or妹紅以外合意or咲夜時姦バレorさとり思慕＆時姦なし
ELSEIF ((GETBIT (CFLAG:ARG:1 ,1) && ARG != 61) || (ARG == 15 && CFLAG:ARG:口上セレクタ == 1 && GETBIT(CFLAG:ARG:1151,0)) || TALENT:ARG:思慕) && ((!GETBIT (CFLAG:ARG:1490,1) && ARG == 49) || ARG != 49)
	RETURNF 1
ENDIF
