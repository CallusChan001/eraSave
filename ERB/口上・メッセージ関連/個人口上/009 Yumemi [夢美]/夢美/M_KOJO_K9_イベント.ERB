				;時間停止時
				IF FLAG:70
				;通常
				ELSE
					IF TALENT:恋慕 || TALENT:愛欲 || TALENT:炮友
						CALL HPH_PRINT,@"「……我也受不了……要不我们快点？ HPH」","W"
					ELSE
						PRINTFORML 「被人这么盯着看，好尴尬……」
					ENDIF
				ENDIF
				RETURN 1
			ENDIF
		;-------------------------------------------------
		;下着姿にする
		;-------------------------------------------------
		CASE 1
			;記入チェック（=0, 非表示、1, 表示）
			LOCAL:1 = 1

			IF LOCAL:1
				;時間停止時
				IF FLAG:70
				;通常
				ELSE
					PRINTFORML 「内裤可以穿着吗？」
				ENDIF
				RETURN 1
			ENDIF
		;-------------------------------------------------
		;半脱ぎ
		;-------------------------------------------------
		CASE 2
			;記入チェック（=0, 非表示、1, 表示）
			LOCAL:1 = 1

			IF LOCAL:1
				;時間停止時
				IF FLAG:70
				;通常
				ELSE
					PRINTFORML 「我不觉得有什么意义。……」
					PRINTFORML 「总觉得比全部脱掉更感到背德感……更让人感到额外兴奋」
				ENDIF
				RETURN 1
			ENDIF
		;-------------------------------------------------
		;パンツちょうだい
		;-------------------------------------------------
		CASE 3
			;記入チェック（=0, 非表示、1, 表示）
			LOCAL:1 = 1

			IF LOCAL:1
				SELECTCASE ARG:1
					;-------------------------------------------------
					;交換です
					;-------------------------------------------------
					CASE 0
						;記入チェック（=0, 非表示、1, 表示）
						LOCAL:1 = 1

						IF LOCAL:1
							PRINTFORML 「倒也无所谓、不过……」
							PRINTFORML 「我也能得到%CALLNAME:MASTER%的吗？」
							RETURN 1
						ENDIF
					;-------------------------------------------------
					;OK（嬉々
					;-------------------------------------------------
					CASE 1
						;記入チェック（=0, 非表示、1, 表示）
						LOCAL:1 = 1

						IF LOCAL:1
							CALL HPH_PRINT,@"「诶？想要我的内裤？　真是的、%CALLNAME:MASTER%想要的话那就没办法……HPH」","W"
							IF ABL:露出癖 > 5
