		;順位は2～4位
		;全角30文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		順位 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:2
				CASE 0
					RESULTS = 「为什么会这样……我做错什么了吗？」
				CASE 1
					RESULTS = 「至少、在我还有理性的时候……下次我不会输的！」
			ENDSELECT
	;		RESULTS = 「好不甘心啊～！」
		ENDIF
	CASE "チーム戦：勝ち"
		;-------------------------------------------------
		;順位は1位（やられていても）
		;全角32文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		順位 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:2
				CASE 0
					RESULTS = 「有我在、那是当然的」
				CASE 1
					RESULTS = 「我的胜利是确定的。祝你们好运……」
			ENDSELECT
	;		RESULTS = 「多亏了我！」
		ENDIF
	CASE "チーム戦：負け"
		;-------------------------------------------------
		;順位は2位
		;全角32文字まで（カギカッコ含む）
		;記入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		順位 = ARG
		;-------------------------------------------------
		IF LOCAL:1
			SELECTCASE RAND:2
				CASE 0
					RESULTS = 「想回家……想重新考虑一下作战计划。」
				CASE 1
					RESULTS = 「为什么会是这样的结果……我做错什么了吗？」
			ENDSELECT
	;		RESULTS = 「我们好弱啊！！」
		ENDIF
ENDSELECT
SIF RESULTS != ""
