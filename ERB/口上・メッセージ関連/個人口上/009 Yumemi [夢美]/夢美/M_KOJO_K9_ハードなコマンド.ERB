;==================================================
;144,放尿
;==================================================
@M_KOJO_MESSAGE_COM_K9_144
CALL M_KOJO_MESSAGE_COM_K9_144_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K9_144_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORML 「咦！？小、小小小小小小小……便……想、想、看！？」
		PRINTFORML 「等、等等、到底在想什么啊……」
		PRINTFORMDW 露骨的表现出失望之后、梦美开始动摇了
		PRINTFORML 「不、不用那么失望吧……因为、那、不是排泄行为吗？这、这不是给人看的东西吧？」
		PRINTFORMDW 真想看啊。好想看啊。有谁能给我看看吗
		PRINTFORML 「啊啊、真是的、我知道了啦！给你看就是了！所以不要那么难过失落啊！」
		PRINTFORML 「真是的、真是变态……」
		;洋式トイレかそうでないかでMSG分岐するよ
		; WC(14, 40, 236, 316, 322, 406, 418, 824, 906, 917)
		;洋式そうなところ(40,316,322,406,418,824,906,917)
		IF CFLAG:MASTER:現在位置 == 40 || CFLAG:MASTER:現在位置 == 316 || CFLAG:MASTER:現在位置 == 322 || CFLAG:MASTER:現在位置 == 406 || CFLAG:MASTER:現在位置 == 418 || CFLAG:MASTER:現在位置 == 824 || CFLAG:MASTER:現在位置 == 906 || CFLAG:MASTER:現在位置 == 917
			PRINTFORMDW 梦美准备好排尿后，便坐到马桶上。
			PRINTFORMDW %CALLNAME:MASTER%正对着她，梦美的股间清晰地映入眼帘。
		ELSE
			PRINTFORMDW 梦美蹲下身，开始准备排尿……
			PRINTFORMDW 从后面看，可以看到菊穴在微微蠕动，从前面看，梦美的股间映入%CALLNAME:MASTER%的眼帘……
		ENDIF
		RETURN 1
	ENDIF
	;基本部分
	;恋慕
	IF TALENT:恋慕
		PRINTFORML 「那、那么，我要尿了……\/\/\/\/」
		PRINTFORMDW 梦美红着脸开始排尿。
		PRINTFORML 「嗯嗯……啊%CALLNAME:MASTER%快看，尿出来了，出来了啊……\/\/\/\/」
		PRINTFORMDW 红着脸的梦美尿道口，尿液“哗”地一声喷涌而出。
		CALL HPH_PRINT,@"「呼啊啊……HPH」","W"

		;如果是厕所的话，尿液会碰到水发出哗啦哗啦的声音。什么？旱厕？没有那种东西（
		IF IN_TOILET(CFLAG:MASTER:現在位置)
			PRINTFORMDW 尿液落在马桶中的水面上，发出「噗噜噜噜噜噜噜……」的声音。
			PRINTFORMDW 随着排尿的进行，冲力减弱，同时发出的声音也变成了「哗啦啦啦啦……啪嗒，啪嗒……」。
		ELSE
			PRINTFORMDW 尿液落地，形成水坑，发出“噼啪噼啪”的声音。
		ENDIF

		PRINTFORML 「讨厌……别那么仔细地看啦……\/\/\/\/」
		PRINTFORMDW 梦美害羞地说道，但%CALLNAME:MASTER%却毫不在意，继续凝视着梦美的排尿姿态。
		CALL HPH_PRINT,@"不久排尿结束后，梦美「哈啊……HPH」地吐出一口热气，身体微微颤抖。","W"
		PRINTFORMDW 仔细一看，阴毛上也溅到了一些尿液，显得有些湿润。
		CALL HPH_PRINT,@"「嗯嗯……哈啊……HPH　尿完了哦……满意吗……？」","W"

		RETURN 1
	;屈服3
	ELSEIF MARK:不埒刻印 == 3
		PRINTFORML 「那、那么……我，要尿了……\/\/\/\/」
		PRINTFORML 「嗯嗯……嗯……」
		PRINTFORMDW 梦美的尿道口，尿液“哗”地一声喷涌而出。

		;如果是厕所的话，尿液会碰到水发出哗啦哗啦的声音
		IF IN_TOILET(CFLAG:MASTER:現在位置)
			PRINTFORMDW 尿液落在马桶中的水面上，发出「噗噜噜噜噜噜噜……」的声音。
			PRINTFORMDW 随着排尿的进行，冲力减弱，同时发出的声音也变成了「哗啦啦啦啦……啪嗒，啪嗒……」。
		ELSE
			PRINTFORMDW 尿液落地，形成水坑，发出“噼啪噼啪”的声音。
		ENDIF

		PRINTFORML 「讨厌……被看到了，尿尿的时候被人看到了啊……\/\/\/\/」
		PRINTFORMDW 不久排尿结束后，梦美身体微微颤抖。
		PRINTFORMDW 仔细一看，阴毛上也溅到了一些尿液，显得有些湿润。
		PRINTFORML 「嗯……呼……\/\/\/\/」

		RETURN 1
	;除此之外
	ELSE
		PRINTFORML 「……嗯」
		PRINTFORMDW 梦美的尿道口，尿液“哗”地一声喷涌而出。

		;如果是厕所的话，尿液会碰到水发出哗啦哗啦的声音
		IF IN_TOILET(CFLAG:MASTER:現在位置)
			PRINTFORMDW 尿液落在马桶中的水面上，发出「噗噜噜噜噜噜噜……」的声音。
			PRINTFORMDW 随着排尿的进行，冲力减弱，同时发出的声音也变成了「哗啦啦啦啦……啪嗒，啪嗒……」。
		ELSE
			PRINTFORMDW 尿液落地，形成水坑，发出“噼啪噼啪”的声音。
		ENDIF

		PRINTFORMW 「别那么直勾勾地看啦……」
		PRINTFORMDW 不久排尿结束后，梦美身体微微颤抖。
		PRINTFORMDW 仔细一看，阴毛上也溅到了一些尿液，显得有些湿润。
		PRINTFORML 「嗯嗯……呼。」

		RETURN 1
	ENDIF

	;这个好色之徒！
	IF ABL:露出癖 > 5
		CALL HPH_PRINT,@"「呐%CALLNAME:MASTER%……被人看着尿尿，好像要变成习惯了呢……HPH」","W"
		PRINTFORMDW %CALLNAME:MASTER%说「变态」，梦美露出满不在乎的表情，身体微微颤抖。
		CALL HPH_PRINT,@"「讨厌啦不要说……HPH」","W"
	ENDIF

ENDIF
RETURN 1

