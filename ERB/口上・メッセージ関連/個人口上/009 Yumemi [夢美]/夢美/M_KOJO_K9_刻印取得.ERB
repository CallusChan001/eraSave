@M_KOJO_MESSAGE_MARKCNG_K9
;このままだと地の文が表示されません
;地の文を表示したい方は、下記のコメントアウトを削除して下さい
;苦痛刻印
IF TFLAG:22 == 1
	PRINTFORMW 
ELSEIF TFLAG:22 == 2
	PRINTFORMW 
ELSEIF TFLAG:22 == 3
	PRINTFORMW 
ENDIF

;時姦刻印
IF TFLAG:時姦刻印取得 == 1
	PRINTFORML （诶，什么？什么啊，这个……？）
ELSEIF TFLAG:時姦刻印取得 == 2
	CALL HPH_PRINT,@"（讨厌，这到底是什么感觉……身体……好像在渴望着什么……HPH）","W"
ELSEIF TFLAG:時姦刻印取得 == 3
	CALL HPH_PRINT,@"（搞什么啊，为什么突然间，会变得这么舒服啊啊啊HPHHPHHPH）","W"
ENDIF

;快感刻印
IF TFLAG:23 == 1
	CALL HPH_PRINT,@"「嗯嗯嗯嗯～～～HPH」","W"
	PRINTFORML （呀、讨厌，刚才的……超级舒服的……）
ELSEIF TFLAG:23 == 2
	CALL HPH_PRINT,@"「……发啊……HPH」","W"
	CALL HPH_PRINT,@"（脑子里……全都是舒服……会变成，傻瓜的……HPH）","W"
ELSEIF TFLAG:23 == 3
	CALL HPH_PRINT,@"「啊——……HPH　哈——HPH　哈HPH」","W"
	CALL HPH_PRINT,@"（做爱竟然这么舒服……HPH　身体知道了全身都舒服的感觉的话……HPH）","W"
	CALL HPH_PRINT,@"（除了变得最喜欢做爱，还能怎么样呢……HPHHPHHPH）","W"
ENDIF

;不埒刻印
;快楽による刻印
;LV1
IF TFLAG:24 == 11
	PRINTFORML （什么啊，刚才的……舒服，吗？）
	PRINTFORML 「呐，再多做一点……好吗？」
;LV2
ELSEIF TFLAG:24 == 12
	PRINTFORML （我很清楚……这种行为是舒服的……）
	PRINTFORML 「想更……舒服……和%CALLNAME:MASTER%一起。」
;LV3
ELSEIF TFLAG:24 == 13
	PRINTFORML （……够了，没关系吧，这种舒服的事……不用忍耐了，对吧。）
	PRINTFORML 「%CALLNAME:MASTER%……呐，我们再多做爱吧……？HPH」

;屈服、恭顺造成的刻印
;LV1
ELSEIF TFLAG:24 == 31
	PRINTFORML （如果是这个人……稍微做这种事，也许可以……？）
	PRINTFORML 「啊，不，没什么哦？嗯。」
;LV2
ELSEIF TFLAG:24 == 32
	PRINTFORML （总觉得我……好像在想和这个人做更多这种事……）
	PRINTFORML 「问我怎么了？没什么，我没生气，没事的。」
;LV3
ELSEIF TFLAG:24 == 33
	PRINTFORML （嗯，如果是这个人，没关系，对吧……再多，做各种……事。）
	PRINTFORML 「问我怎么了？嗯，没什么，%CALLNAME:MASTER%♪」
ENDIF

