@M_KOJO_假酒自制_MESSAGE_COM_K66_80_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		SELECTCASE TALENT:MASTER:250
		CASE 6,10
			PRINTFORML 「哦呀～❤」
			PRINTFORMDW 如此硕大的男根使幽幽子愣了一下
			SELECTCASE TALENT:MASTER:100
			CASE -5 TO -1
				PRINTFORML 「瘦小的身躯里居然成长着野兽呢～❤」
				PRINTFORML 「营养全被下面吸收了吗～❤」
			CASEELSE
				PRINTFORML 「居然有这么大吗～」
			ENDSELECT
			PRINTFORML 「很想试一试呢～❤」
			PRINTFORMDW 话虽如此说着，幽幽子慢慢侍奉的手在微微颤抖
		CASE 5
			PRINTFORML 「哦～如此奇怪呢～」
			PRINTFORMDL 幽幽子伸出的手迟疑了一会
			PRINTFORMDW 思考过后还是用手侍奉了上去
			PRINTFORMW 「对幽灵会有什么影响呢～」
		CASE 1,2
			PRINTFORML 「❤～」
			SELECTCASE TALENT:MASTER:100
			CASE -5 TO -1
				PRINTFORML 「果然还是在发育中啊～❤」
				PRINTFORML 「要好好吃饭长的大大的哦～❤」
				PRINTFORMDW 幽幽子戏谑地侍奉着
			CASEELSE
				PRINTFORML 「唉～真的是这样吗～」
				PRINTFORMDW 幽幽子对这个尺寸有点失落
					IF TALENT:23:思慕 || TALENT:23:恋慕 || TALENT:23:恋人
						PRINTFORMW 「要不要让妖梦给你做点补品呢～」
					ELSE
						PRINTFORMW 「嗯姆～」
					ENDIF
			ENDSELECT
		CASEELSE
			PRINTFORML 「❤～」
			PRINTFORMDW 健壮的阴茎展露在幽幽子的面前
			PRINTFORML 「很期待你的表现哦～❤」
			PRINTFORMDW 幽幽子冰凉的手温柔缓慢地侍奉着眼前的阴茎
		ENDSELECT
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 用幽幽子固定的手反复对着阴茎侍奉着
		RETURN 1
	ELSE
		IF PALAM:潤滑 >= 5000
			PRINTFORMDL 手上充满润滑液的幽幽子搓着手发出了气泡声
		ENDIF
		IF TALENT:恋慕 || TALENT:恋人
			IF ABL:指 >= 3
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「喜欢这个样子嘛～❤」
					PRINTFORMDW 幽幽子混合着先走汁用手指不停刺激着龟头
				CASE 1
					PRINTFORML 「硬硬的哦～❤」
					PRINTFORMDW 幽幽子用小拇指和无名指勾动着阴囊，温柔地撸动着柱身
				CASE 2
					PRINTFORML 「手凉凉的嘛～再摸一会就暖和了哦～❤」
					PRINTFORMDW 幽幽子用手侍奉的幅度又大了一点
				ENDSELECT
			ELSE
				SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「啊啦❤～一动一动的～❤」
					PRINTFORMDW 幽幽子的手上下撸动着%CALLNAME:MASTER%的阴茎
				CASE 1
					PRINTFORML 「怎么样～有没有点感觉呢❤」
					PRINTFORML 「整个已经变得滑滑的了哟～❤」
					PRINTFORMDW 幽幽子的手上沾满着先走汁涂满了整个阴茎
				CASE 2
					PRINTFORML 「很有意思哟～❤」
					PRINTFORML 「一涨一涨的感觉❤」
					PRINTFORMDW 肉棒在幽幽子的手里轻轻抖动着
				ENDSELECT
			ENDIF
			IF NOWEX:MASTER:射精
				IF TALENT:恋慕 || TALENT:恋人
					IF ABL:精液中毒 >= 5
						PRINTFORMDL 幽幽子妩媚地舔了舔黏在手上的精液
					ENDIF
					SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「啊啦～要是就这样射出来了的话就太可惜了～❤」
					CASE 1
						PRINTFORMW 「想要在哪里标记呢～%CALLNAME:MASTER%❤」
					CASE 2
						PRINTFORMW 「忍不住了嘛～尽情射出来吧～❤」
					ENDSELECT
				ELSE
					PRINTFORMW 「哦～射出来很多呢❤」
				ENDIF
			ENDIF
		ELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORML 「请在坚持一会～❤」
					PRINTFORMW 「还没到主菜哦～❤」
				CASE 1
					PRINTFORML 「怎么样～有没有点感觉呢❤」
					PRINTFORML 「整个已经变得滑滑的了哟～❤」
					PRINTFORMDW 幽幽子的手上沾着先走汁涂满了整个阴茎
				CASE 2
					PRINTFORML 「很有意思哟～❤」
					PRINTFORML 「一涨一涨的感觉❤」
					PRINTFORMDW 肉棒在幽幽子的手里轻轻抖动着
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

