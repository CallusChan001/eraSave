;==================================================
;相手から约会のお诱い
;==================================================
@M_KOJO_EVENT_K26_27(ARG,ARG:1)
;台词主人公为LOCAL
;ARG:1は目的地
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	SELECTCASE ARG
		;-------------------------------------------------
		CASE 0
		;诱い文句
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「呐，%CALLNAME:MASTER%，要不要去约会？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「%CALLNAME:MASTER%，要不要一起去玩？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%，现在有空吗？」
			ELSE
				PRINTFORMW 「%CALLNAME:MASTER%，要不要陪我一起去夜游？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 1
		;接受了
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「希望今晚是个愉快的夜晚，%CALLNAME:MASTER%」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「呵呵，希望不会被误认为是发生了什么异变」
			ELSEIF TALENT:思慕
				PRINTFORMW 「要对灵梦保密哦？不然会被退治的」
			ELSE
				PRINTFORMW 「来吧，让我们在幻视之夜中徘徊吧」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 2
		;断った
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「呵呵，那么下次有机会再一起吧」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「哎呀，真可惜呢」
			ELSEIF TALENT:思慕
				PRINTFORMW 「哎呀呀，要不要让蓝教教你搭讪的方法呢」
			ELSE
				PRINTFORMW 「真遗憾，被拒绝了呢」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 3
		;被邀请到房间
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「难得的机会，要不要来我的房间呢？」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「或许能看到有趣的东西，要不要来呢？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「不想试试找找我家吗？」
			ELSE
				PRINTFORMW 「要不要稍微喝杯茶呢？」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 4
		;接受了邀请到房间
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「今天一整天，都能和%CALLNAME:MASTER%一起度过了呢♪」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「呵呵，就算后悔也没用了哦？」
			ELSEIF TALENT:思慕
				PRINTFORMW 「%CALLNAME:MASTER%，真是个天不怕地不怕的人呢」
			ELSE
				PRINTFORMW 「那么，就通过这个隙间吧——」
			ENDIF
			RETURN 1
		ENDIF
		;-------------------------------------------------
		CASE 5
		;拒绝了邀请到房间
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 1
		;-------------------------------------------------
		IF LOCAL:1
			IF TALENT:恋人
				PRINTFORMW 「真遗憾，不过随时都可以来哦」
			ELSEIF TALENT:恋慕
				PRINTFORMW 「哎呀？真可惜呢」
			ELSEIF TALENT:思慕
				PRINTFORMW 「明明是难得的机会呢」
			ELSE
				PRINTFORMW 「嘛，真是明智的选择」
			ENDIF
			RETURN 1
		ENDIF
	ENDSELECT
ENDIF
RETURN 1

