;==================================================
;150,双乳间抽插
;==================================================
@M_KOJO_MESSAGE_COM_K26_150
CALL M_KOJO_MESSAGE_COM_K26_150_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K26_150_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分支
	;-------------------------------------------------
	;初次
	;记入检查
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「哎呀？那么用力地顶着肉棒，难道我的乳房就那么有魅力吗♥」
		RETURN 1
	ENDIF
	;基本设置
	;时奸中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;胸3以上
	ELSEIF ABL:胸 > 2
		IF RAND:3 == 0
			PRINTFORMW 「总觉得被肉棒侵犯乳房，会变得兴奋呢♥」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯♥不要啊，乳房会沾上精液味的♥」
		ELSE
			PRINTFORMW 「%CALLNAME:MASTER%比起我，更喜欢我的欧派呢♥」
		ENDIF
	;其他
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯……竟然把肉棒蹭在我的胸上，真有眼光呢」
		ELSEIF RAND:2 == 0
			PRINTFORMW 「再不快点射精，我的乳房就无法解放了」
		ELSE
			PRINTFORMW 「我的乳房也值得夸耀吧？♥」
		ENDIF
	ENDIF
ENDIF
RETURN 1

