;==================================================
;140,深喉插入
;==================================================
@M_KOJO_MESSAGE_COM_K26_140
CALL M_KOJO_MESSAGE_COM_K26_140_1
RETURN RESULT

@M_KOJO_MESSAGE_COM_K26_140_1
;-------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分支
	;-------------------------------------------------
	;初次
	;记入检查
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && FIRSTTIME(SELECTCOM)
		PRINTFORMW 「嗯！嗯嗯～～～！」
		PRINTFORMW 「不要啊，强行什么的……♥」
		RETURN 1
	ENDIF
	;基本设置
	;恋慕
	IF TALENT:恋慕
		PRINTFORMDW 
		RETURN 1
	;射出前
	ELSEIF BASE:PLAYER:2 > 8000
		IF RAND:3 == 0
			PRINTFORMW 「嗯嗯♥」
			PRINTFORMDW 紫感受到%CALLNAME:MASTER%的射精，用喉咙巧妙地催促着。
			PRINTFORMW （快点……♥快点……♥）
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯♥嗯嗯♥」
			PRINTFORMDW 紫感受到%CALLNAME:MASTER%的射精，用喉咙巧妙地催促着。
		ELSE
			PRINTFORMW 「嗯嗯♥嗯～嗯♥」
			PRINTFORMDW 紫感受到%CALLNAME:MASTER%的射精，用喉咙巧妙地催促着。
		ENDIF
	;其他
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「嗯～！」
			PRINTFORMDW 紫被%CALLNAME:MASTER%插入肉棒而呻吟着。
		ELSEIF RAND:2 == 0
			PRINTFORMW 「嗯嗯……嗯嗯！！！」
			PRINTFORMDW 紫被%CALLNAME:MASTER%插入肉棒而呻吟着。
		ELSE
			PRINTFORMW 「～～～！！！」
			PRINTFORMDW 紫被%CALLNAME:MASTER%插入肉棒而呻吟着。
		ENDIF
	ENDIF
ENDIF
RETURN 1

