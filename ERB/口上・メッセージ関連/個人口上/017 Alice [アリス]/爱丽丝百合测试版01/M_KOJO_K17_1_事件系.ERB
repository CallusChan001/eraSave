		RESULTS = %TEXTR("近了！很近了哦！/快到了哦！/再一点点！")%
	CASE "遠い"
		RESULTS = %TEXTR("还很远哦/还早着呢～")%
	CASE "かなり遠い"
		RESULTS = %TEXTR("还远着呢～/还远远的呢～")%
	CASE "前"
		RESULTS = %TEXTR("在前面哦/就这样，就这样/那边那边")%
	CASE "少し前"
		RESULTS = %TEXTR("就一点点前面！/稍微前面一点/再一步！")%
	CASE "後ろ"
		RESULTS = %TEXTR("面向后面了！/反了，反了！/不是那边哦～")%
	CASE "少し後ろ"
		RESULTS = %TEXTR("就在后面哦！/稍微后面一点/一步后面！")%
	CASE "右"
		RESULTS = %TEXTR("在右边哦/右边，右边/再往右")%
	CASE "少し右"
		RESULTS = %TEXTR("就一点点右边/还是右边吧")%
	CASE "左"
		RESULTS = %TEXTR("在左边哦/左边，左边/再往左")%
	CASE "少し左"
		RESULTS = %TEXTR("就一点点左边/还是左边吧")%
	CASE "結果"
		;-------------------------------------------------
		;结束后的一句话
		;没有字数限制，也没有通用台词
		;TFLAG:193,
		;   -1=失敗
		;    0=成功
		;    1=大成功
