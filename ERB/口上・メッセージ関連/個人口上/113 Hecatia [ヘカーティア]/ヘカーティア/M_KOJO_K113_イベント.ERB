@M_KOJO_総合スレ７４９_EVENT_K113_1(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;正月、バレンタイン、ハロウィン、クリスマスの場合
		IF (DAY:2 == 1 && DAY:3 == 1 && DAY:0 != 1) || (DAY:2 == 4 && DAY:3 == 14 && CFLAG:2 >= 1000) || (DAY:2 == 3 && DAY:3 == 30) || (DAY:2 == 4 && DAY:3 == 25)
			CALL SPECIALDAY_EVENT_K113_総合スレ７４９
		ELSEIF CFLAG:318 == 1
			IF !TCVAR:113:今日会ったか
				PRINTFORML 「……啊，%CALLNAME:MASTER%。你在这儿啊？」
				PRINTFORMDW 赫卡提亚的心情似乎不太好……
			ELSE
				PRINTFORML 「刚才就一直在晃来晃去……你在做什么呢？」
				PRINTFORMDW 赫卡提亚的心情似乎还没有好转……
			ENDIF
		ELSE
			IF !TCVAR:113:今日会ったか
				SELECTCASE TIME:2
					;早上
					CASE 1,2
						PRINTFORML 「早上好～！」
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「希望今天也会发生有趣的事情♪」
							CASE 1
								PRINTFORMW 「清新的早晨啊，%CALLNAME:MASTER%♪」
						ENDSELECT
					;中午
					CASE 3,4
						PRINTFORML 「下午好～！」
						SELECTCASE RAND:2
							CASE 0
								PRINTFORMW 「“谐音不好听”？这样吗？」
							CASE 1
								PRINTFORMW 「%CALLNAME:MASTER%，你的事情已经办完了吗？」
						ENDSELECT
					;晚上
					CASE 5,6,7
						SELECTCASE RAND:2
							CASE 0
								PRINTFORML 「晚上好～！」
								PRINTFORMW 「我们的时间从现在开始，对吧，%CALLNAME:MASTER%♪」
							CASE 1
								PRINTFORML 「你到底去了哪里呢？%CALLNAME:MASTER%」
								PRINTFORMW 「作为惩罚，今晚我要你陪我直到天亮♪」
						ENDSELECT
				ENDSELECT
			ELSE
