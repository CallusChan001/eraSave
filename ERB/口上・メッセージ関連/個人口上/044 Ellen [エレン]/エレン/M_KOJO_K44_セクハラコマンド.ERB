@M_KOJO_MESSAGE_COM_K44_312_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	IF FLAG:70
	ELSEIF CFLAG:睡眠
	ELSEIF !CFLAG:ブチギレ && TFLAG:193 != 99 && !MARK:反発刻印
		;ありがとうおしゃぶり霊夢の人！そしてごめんなさい！
		IF EXP:キス経験 == 1
			;ファーストキッス群
			IF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「哎嘿嘿嘿//　恋人之间的亲吻竟然是这么幸福的事呢♥」
				PRINTFORMW 「接下来由我来♥ 啾♥」
				EXP:キス経験 += 4
				EXP:MASTER:キス経験 += 4
				EXP:愛情経験 += 10
			ELSEIF TALENT:恋人 || TALENT:恋慕
				PRINTFORMW 「哎嘿嘿//　这次轮到我了哦♥ 嘿！♥」
				EXP:キス経験 += 1
				EXP:MASTER:キス経験 += 1
			ELSEIF TALENT:思慕
				PRINTFORMW 「诶？诶！？诶诶诶！？」
				PRINTFORMW （没想到%CALLNAME:MASTER%竟然会亲亲亲亲亲吻过来//）
				PRINTFORMW （%CALLNAME:MASTER%主动亲吻过来，难道说难道说难道说难道说喜欢我吗！？//）
				PRINTFORMW （虽虽虽虽说喜欢%CALLNAME:MASTER%可是那是作为朋友的喜欢//）
				PRINTFORMW （我以为%CALLNAME:MASTER%也这么想的但是但是但是）
				PRINTFORMW （啊——呜——//）
				CFLAG:1014 = 1
				TALENT:羞恥心 = 1
			ELSE
				PRINTFORML 「……我的初吻～！！」
				PRINTFORMDW 艾伦一边哭一边生气着…
				BASE:怒り += 500
				CFLAG:好感度 -= 300
				;マイナスにはならない
				SIF CFLAG:好感度 < 0
					CFLAG:好感度 = 0
			ENDIF
		ELSE
			IF TALENT:恋人 && TALENT:恋慕 && GET_TARGETNUM() == 1
				SELECTCASE RAND:3
				CASE 1
					PRINTFORMW 「接下来由我来♥ 啾～♥」
				CASE 2
					PRINTFORMW 「呐，再来一次嘛♥」
				CASEELSE
					SELECTCASE CFLAG:溜まってる度
					CASE 0 TO 499
						PRINTFORMW 「啾♥　啾♥」
					CASE 500 TO 799
						PRINTFORMW 「啾……嗯//」
					CASE IS > 800
						PRINTFORMDW 艾伦涨红着脸热情地索求着%CALLNAME:MASTER%的唇……
					ENDSELECT
				ENDSELECT
			ELSEIF TALENT:恋慕 && (OUTROOF(CFLAG:TARGET:現在位置)) || (OPENPLACE(CFLAG:TARGET:現在位置))
				PRINTFORMW 「啾…噗哈//　有点害羞呢//」
			ELSEIF TALENT:恋人 && TALENT:恋慕
				PRINTFORMW 「啾…噗哈。　啊哈哈哈♥ 最最最喜欢%CALLNAME:MASTER%了♥」
			ELSEIF TALENT:恋慕 || CFLAG:1014 == 1 || TALENT:恋人
				PRINTFORMW 「啾…噗哈。　啊哈哈哈//」
			ENDIF
		ENDIF
	ENDIF

ENDIF
RETURN 1
