;-------------------------------------------------
;奴隷の絶頂時の口上
;NOWEX:Ｃ絶頂～4 CVABM絶頂
;NOWEX:二重絶頂 二重絶頂
;NOWEX:三重絶頂 三重絶頂
;NOWEX:四重絶頂 四重絶頂
;NOWEX:五重絶頂 五重絶頂
;NOWEX:噴乳 噴乳
;NOWEX:射精 射精
;NOWEX:放尿 放尿
;随意組み合わせて使ってください
;-------------------------------------------------
@M_KOJO_MESSAGE_PALAMCNG_B_K44
;-------------------------------------------------
;噴乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:噴乳
	;恋人かつ恋慕なら異常経験上がらない
	SIF TALENT:恋人 && TALENT:恋慕 && FIRSTTIME(1051)
	EXP:異常経験 -= 1
ENDIF
;-------------------------------------------------
;射精
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF

;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && !FLAG:70 && !CFLAG:睡眠
;絶頂後処理
IF NOWEX:0 || NOWEX:1 || NOWEX:2 || NOWEX:3
	;イったフラグ
	TCVAR:387 += 1
ENDIF
SIF NOWEX:Ｃ絶頂
TCVAR:382 += 1
SIF NOWEX:Ｖ絶頂
TCVAR:383 += 1
SIF NOWEX:Ａ絶頂
TCVAR:384 += 1
SIF NOWEX:Ｂ絶頂
TCVAR:385 += 1
SIF NOWEX:Ｍ絶頂
TCVAR:388 += 1
	;-------------------------------------------------
	;五重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:五重絶頂
		PRINTFORMW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;四重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:四重絶頂
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		[SKIPSTART]
		SELECTCASE NOWEX:四重絶頂
			;Ｃ＆Ｖ＆Ａ＆Ｂ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ａ＆Ｍ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ＆Ｍ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ＆Ｂ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;-------------------------------------------------
	;三重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:三重絶頂
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		[SKIPSTART]
		SELECTCASE NOWEX:三重絶頂
			;Ｃ＆Ｖ＆Ａ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｖ＆Ｍ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｖ＆Ｍ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｖ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	;三重絶頂以上
	IF NOWEX:三重絶頂 || NOWEX:四重絶頂 || NOWEX:五重絶頂
		;絶頂加算
		TCVAR:381 += 1
		TCVAR:387 += 2
	ENDIF
	;-------------------------------------------------
	;二重絶頂
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:二重絶頂
		;部位を指定せず簡易的に書く場合、[SKIPSTART]&[SKIPEND]のコメントアウトを外して、[SKIPSTART]とこのコメントの間に書いてください
		[SKIPSTART]
		SELECTCASE NOWEX:二重絶頂
			;Ｃ＆Ｖ絶頂
			CASE 1
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ａ絶頂
			CASE 2
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ａ絶頂
			CASE 3
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｂ絶頂
			CASE 4
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｂ絶頂
			CASE 5
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｂ絶頂
			CASE 6
				PRINTFORMW 
				RETURN 1
			;Ａ＆Ｍ絶頂
			CASE 7
				PRINTFORMW 
				RETURN 1
			;Ｖ＆Ｍ絶頂
			CASE 8
				PRINTFORMW 
				RETURN 1
			;Ｃ＆Ｍ絶頂
			CASE 9
				PRINTFORMW 
				RETURN 1
			;Ｂ＆Ｍ絶頂
			CASE 10
				PRINTFORMW 
				RETURN 1
		ENDSELECT
		[SKIPEND]
	ENDIF
	IF NOWEX:Ｃ絶頂
		;超強絶頂Ｃ
		IF NOWEX:Ｃ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｃ
		ELSEIF NOWEX:Ｃ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ絶頂
		;超強絶頂Ｖ
		IF NOWEX:Ｖ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｖ
		ELSEIF NOWEX:Ｖ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ａ絶頂
		;超強絶頂Ａ
		IF NOWEX:Ａ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ａ
		ELSEIF NOWEX:Ａ絶頂 == 2
			PRINTFORMW 
		;絶頂Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ絶頂
		;強絶頂Ｂ
		IF NOWEX:Ｂ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｂ
		ELSEIF NOWEX:Ｂ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｂ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｍ絶頂
		;超強絶頂Ｍ
		IF NOWEX:Ｍ絶頂 == 3
			PRINTFORMW 
		;強絶頂Ｍ
		ELSEIF NOWEX:Ｍ絶頂 == 2
			PRINTFORMW 
		;絶頂Ｍ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF

;監視用
;御香用
IF TCVAR:373 == 1
	;你の勃起度と精力を最大にしつつ互相用気力を少しずつ回復という升仕様。艾伦限定かつ比較的レアだから大丈夫…よね？
	PALAM:欲情 += (ABL:親密 * 1000)
	PALAM:潤滑 += (ABL:欲望 * 500)
	BASE:MASTER:勃起 = MAXBASE:MASTER:勃起
	BASE:MASTER:精力 = MAXBASE:MASTER:精力
	BASE:気力 += 50
	BASE:MASTER:気力 += 50
ENDIF

;魔法の披露が出来るかどうか
IF CFLAG:1008 > 40 && TIME:5 <= 3
	CFLAG:1011 = 1
ELSE
	CFLAG:1011 = 0
ENDIF

;月に願いを
SIF CFLAG:1013 == 1 && DAY:3 > 1 && DAY:3 < 31
CFLAG:1013 = 0

;連打防止解除
SIF TCVAR:362 == 1
TCVAR:362 = 0

;健全時
IF CFLAG:1891 == 0; || TALENT:妊娠
	SIF ABL:欲望 > 10
	ABL:欲望 = 10
	SIF CFLAG:积攒度 > 300
	CFLAG:积攒度 = 300
ENDIF

;兒童の描写をオフにしているならここで吃飯のTCVARを消す
IF !FLAG:兒童描写 && GROUPMATCH(SELECTCOM, 414, 415)
	TCVAR:360 = 0
	TCVAR:361 = 0
ENDIF


