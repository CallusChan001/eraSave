		PRINTFORMW 看起来几近于与%CALLNAME:TARGET%\@ GET_TARGETNUM() > 1 ? 们 # \@相见恨晚…
		TFLAG:193 = 2
	ENDSELECT
ELSEIF TALENT:思慕 || TALENT:恋慕
	SELECTCASE RAND:8
	CASE 0
		PRINTFORML 说着话
		PRINTFORMW 完全聊不到一块去…
		TFLAG:193 = -2
	CASE 1,2
		PRINTFORML 说着话
		PRINTFORMW 嘴刚一停下、尴尬的气氛就充斥着四周…
		TFLAG:193 = -1		
	CASE 3 TO 5
		PRINTFORMW 说着话
		TFLAG:193 = 0
	CASE 6 TO 8
		PRINTFORML 说着话
		PRINTFORMW 渡过了快乐的时光…
		TFLAG:193 = 1
	CASE 9
		PRINTFORML 说着话
		PRINTFORMW 看起来几近于与%CALLNAME:TARGET%\@ GET_TARGETNUM() > 1 ? 们 # \@相见恨晚…
		TFLAG:193 = 2
	ENDSELECT
