;育児イベント入浴
;-------------------------------------------------
@M_KOJO_STELLARIS_EVENT_K98_CHILD_RAISING_BATH(ARGS,ARG,ARG:1)
IF ARGS == "幼児期"
	IF CFLAG:98:1221 == 1
		PRINTFORMDW 哗啦啦啦————
	ELSE
		IF CFLAG:98:4 >= 130000 && CFLAG:98:1453 == 1
			PRINTFORMW 「放水里泡一泡就行了」
		ELSE
			PRINTFORMW 「你看，只要这样……铛铛！干干净净！」
		ENDIF
	ENDIF
ELSEIF ARGS == "幼少期"
	IF CFLAG:98:1221 == 1
		IF CFLAG:98:4 >= 130000 && CFLAG:98:1453 == 1
			PRINTFORMW 「Reisen，去叫两只兔子来」
		ELSE
			PRINTFORMW 「……不要乱动」
		ENDIF
	ELSE
		IF CFLAG:98:4 >= 130000 && CFLAG:98:1453 == 1
			PRINTFORMW 「二号，去叫两只兔子来」
		ELSE
			PRINTFORMW 「来妈妈这边洗头发吧」
		ENDIF
	ENDIF
ELSE
	IF CFLAG:98:1221 == 1
		IF CFLAG:98:4 >= 130000 && CFLAG:98:1453 == 1
			PRINTFORMW 「自己去洗」
		ELSE
			PRINTFORMW 「又跑出去弄得这么脏……好好洗干净」
		ENDIF
	ELSE
		IF CFLAG:98:4 >= 130000 && CFLAG:98:1453 == 1
			PRINTFORMW 「还不会自己洗吗？」
		ELSE
			PRINTFORMW 「果然家人就要一起泡澡呢，爸爸你在躲什么啊，来吧，到我旁边来」
		ENDIF
	ENDIF
