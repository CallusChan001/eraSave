		PRINTFORMW 「互相舔舐什么的简直是小丑剧场啊」
		RETURN 1
	ENDIF
	;基本セット
	;時姦中
	IF FLAG:70
		PRINTFORMDW 
		RETURN 1
	;出す直前
	ELSEIF BASE:PLAYER:2 > 8000
