			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「嗯……嗯嗯HPH」","W"
			ELSE
				CALL HPH_PRINT,@"「嗯嗯……啊HPH」","W"
			ENDIF
		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			CALL HPH_PRINT,@"「嗯……嗯……HPH」","W"
		ELSEIF RAND:2 == 0
