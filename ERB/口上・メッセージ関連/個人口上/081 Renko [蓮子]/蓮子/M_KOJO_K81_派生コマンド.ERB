				PRINTFORMDW 莲子感觉到%CALLNAME:MASTER%将要射精用舌头巧妙地催促着。
			ELSE
				CALL HPH_PRINT,@"「嗯啊HPH嗯～啊HPH」","W"
				PRINTFORMDW 莲子感觉到%CALLNAME:MASTER%将要射精用舌头巧妙地催促着。
				CALL HPH_PRINT,@"「……我喜欢这个味道HPH」","W"
			ENDIF
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「嗯？」
				PRINTFORMDW 莲子感觉到%CALLNAME:MASTER%将要射精用舌头巧妙地催促着。
				CALL HPH_PRINT,@"「快点射出来就行了HPH」","W"
			ELSEIF RAND:2 == 0
				CALL HPH_PRINT,@"「嗯嗯……一点一点出来了……HPH」","W"
				PRINTFORMDW 莲子感觉到%CALLNAME:MASTER%将要射精用舌头巧妙地催促着。
			ELSE
				CALL HPH_PRINT,@"「嗯嗯HPH」","W"
				PRINTFORMDW 莲子感觉到%CALLNAME:MASTER%将要射精用舌头巧妙地催促着。
			ENDIF
		ENDIF
