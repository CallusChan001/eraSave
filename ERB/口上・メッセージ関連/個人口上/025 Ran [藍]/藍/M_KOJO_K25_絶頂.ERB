;-------------------------------------------------
IF LOCAL
	IF FLAG:70
		PRINTFORML
		RETURN 1
	ELSE
		SELECTCASE RAND:6
			CASE 0
				CALL HPH_PRINT,@"「精液HPH好喜欢HPH……还想要HPH更多HPH……」","L"
			CASE 1
				CALL HPH_PRINT,@"「肉棒插在里面射精了HPH小穴要被填满了HPH……」","L"
			CASE 2
				CALL HPH_PRINT,@"「HPH最喜欢了，%CALLNAME:MASTER%HPH」","L"
			CASE 3
				CALL HPH_PRINT,@"「全都射在里面了吗？HPH真棒HPH」","L"
			CASE 4
				CALL HPH_PRINT,@"「肚子里暖暖的，好舒服HPH」","L"
			CASE 5
				CALL HPH_PRINT,@"「HPH」","L"
		ENDSELECT
		
		IF TALENT:妊娠
			IF 充填率(TARGET,1) > 100
				SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「哈……哦哦HPH……子宫……好胀HPH」","L"
					PRINTFORMDW 被注入大量精液后，蓝的腹部膨胀得像是又怀上了好几胎那样，大得惊人……
				CASE 1
					PRINTFORML 「肚子都明显膨胀起来了，到底在里面射了多少……」
					PRINTFORMW 「子宫里的宝宝，要被精液浸透了……」
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「对孕妇内射了呢HPH%CALLNAME:MASTER%感觉如何？」","W"
				CASE 1
					CALL HPH_PRINT,@"「在孕妇的小穴里射精了呢HPH%CALLNAME:MASTER%一定很舒服吧HPH」","W"
				ENDSELECT
			ENDIF
		ELSEIF  TCVAR:口服避孕薬
			IF 充填率(TARGET,1) > 100
				PRINTFORML 「在里面射了这么多，肚子都膨胀起来了……」
				PRINTFORMDW (要是没吃药的话，估计已经受精了吧……)
			ELSE
				PRINTFORML 「已经吃过药了，不用担心会怀孕哦」
				PRINTFORMW 「所以，多多地射在里面吧❤~」
			ENDIF
		ELSEIF CFLAG:生理周期 == 7
		;危险日
			IF 充填率(TARGET,1) > 100
				SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「精液HPH好多HPH子宫里已经是满满的了HPH」","L"
					CALL HPH_PRINT,@"「要受精了HPH要怀上%CALLNAME:MASTER%的孩子了HPH我要当妈妈了HPH……」","W"
				CASE 1
					CALL HPH_PRINT,@"「又射了HPH子宫已经被精液填满了HPH我的卵子已经无处可逃了呜呜HPH~要怀上%CALLNAME:MASTER%的孩子了HPH」","W"
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
				CASE 0
					CALL HPH_PRINT,@"「对着危险日的小穴内射了呢HPH」","L"
					CALL HPH_PRINT,@"「%CALLNAME:MASTER%，准备好当爸爸了吗？HPH」","W"
				CASE 1
					CALL HPH_PRINT,@"「今天是我的危险日呢HPH%CALLNAME:MASTER%能明白我的意思吧HPH~」","W"
				ENDSELECT
			ENDIF
		ELSEIF CFLAG:生理周期 == 1 || CFLAG:生理周期 == 2
		;安全日
			IF 充填率(TARGET,1) > 100
				PRINTFORML 「肚子都膨胀起来了，到底在里面射了多少……」
				PRINTFORMW 「即使是在安全日，被射入这么多的话，也可能会怀孕的啊……」
			ELSE
				PRINTFORML 「今天是安全日，不用担心会怀孕哦」
				PRINTFORMW 「所以，多多地射在里面吧❤~」
			ENDIF
		ELSE
			IF 充填率(TARGET,1) > 100
				PRINTFORML 「肚子都膨胀起来了，到底在里面射了多少……」
				PRINTFORMW 「这样下去，真的会怀孕的啊❤……」
			ELSE
				PRINTFORML 「虽然不是危险日，但只要努力的话，还是有受精的机会的❤」
				PRINTFORMW 「所以，多多地射在里面吧❤~」
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1

