;==================================================
;房间で遭遇
;==================================================
@M_KOJO_EVENT_K25_1(ARG,ARG:1)
;台词主人公为TARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;MASTERが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	;四季ごとに朝昼晩
	IF LOCAL:1 && ARG == 1
		PRINTL
		;一日一回
		SIF TCVAR:25:350 == 1
			RETURN 1
		IF TALENT:恋慕
			;DAY:2で季节を选择
			SELECTCASE DAY:2
			;春
			CASE 1
				;TIME:2で朝昼晩を选择
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORML 「%CALLNAME:MASTER%样。早上好」
					PRINTFORML 「讨厌死板的问候？」
					PRINTFORMDW 被尾巴搔弄着，似乎玩得很开心
				;昼
				CASE 3,4
					PRINTFORML 「呵呵、%CALLNAME:MASTER%」
					PRINTFORMW 「在这季节。选这时间出门最适合呢」
				;夜
				CASE 5,6,7
					PRINTFORML 「晚上好、%CALLNAME:MASTER%」
					PRINTFORMW 「就算是春分、晚上还是会冷。注意身体别着凉了」
				ENDSELECT
			;夏
			CASE 2
				SELECTCASE TIME:2
				;朝
				CASE 1,2
					PRINTFORML 「早上好，%CALLNAME:MASTER%」
					PRINTFORML 「现在是早晚也很热的季节了。昨晚睡得好吗？」
					PRINTFORMDW 蓝总是保持着凉爽的姿态
				;中午
				CASE 3,4
					PRINTFORML 「你好。今天也很热呢，%CALLNAME:MASTER%」
					PRINTFORML 「哎呀，出了好多汗。我帮你擦擦」
					PRINTFORMDW 蓝帮我擦去了额头上的汗水
				;晚上
				CASE 5,6,7
					PRINTFORML 「是%CALLNAME:MASTER%啊。没有被晒干真是太好了」
					PRINTFORML 「天黑了，也变得凉快了。要卿卿我我的话现在正是时候哦」
					PRINTFORMDW 蓝似乎觉得这是夏天为数不多的好机会，凑了过来
				ENDSELECT
			;秋天
			CASE 3
				SELECTCASE TIME:2
				;早上
				CASE 1,2
					PRINTFORML 「您身体好吗，%CALLNAME:MASTER%」
					PRINTFORMW 「身体没有不舒服吧。偶尔去竹林里的医生那里看看也好」
				;中午
				CASE 3,4
					PRINTFORML 「嗯，风中飘来银杏的香味」
					PRINTFORML 「说实话我已经闻腻了。救救我」
					PRINTFORMDW 蓝把脸凑到我的脖子上，嗅了嗅鼻子
				;晚上
				CASE 5,6,7
					PRINTFORML 「呀，%CALLNAME:MASTER%」
					PRINTFORML 「你看，那边有蟋蟀在叫」
					PRINTFORMDW 两人一起静静地聆听着这风雅的音色
				ENDSELECT
			;冬天
			CASE 4
				SELECTCASE TIME:2
				;早上
				CASE 1,2
					PRINTFORML 「早上好，%CALLNAME:MASTER%……嗯？」
					PRINTFORML 「真是突然啊。我倒是无所谓」
					PRINTFORMDW 我把冰冷的手埋进了蓝蓬松的尾巴里
				;中午
				CASE 3,4
					PRINTFORML 「冷吗，%CALLNAME:MASTER%？」
					PRINTFORML 「那，再靠近我一点。我帮你暖到心底」
					PRINTFORMDW 蓝的尾巴轻轻地缠绕在我的脖子上
				;晚上
				CASE 5,6,7
					PRINTFORML 「呜，变得好冷啊」
					PRINTFORML 「即使有毛皮，冷的时候还是冷啊」
					PRINTFORMW 「这种时候还是人体的温暖最棒了。你觉得呢？」
				ENDSELECT
			ENDSELECT
		ELSEIF TALENT:思慕
			PRINTFORML 「是%CALLNAME:MASTER%啊。能见到你真开心」
			PRINTFORMDW 蓝笑着亲切地向我打招呼
		ELSE
			PRINTFORML 「呀」
			PRINTFORMDW 蓝注意到这边，举起了手
		ENDIF
		TCVAR:25:350 = 1
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが入ってきた
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTL
		IF TALENT:恋慕
			PRINTFORML 「原来你在这里啊，%CALLNAME:MASTER%」
			PRINTFORML 「嗯。我是来见你的」
			PRINTFORMDW 蓝以开朗的语气靠近过来
		ELSEIF TALENT:思慕
			PRINTFORML 「哦哦，%CALLNAME:MASTER%」
			PRINTFORMDW 一发现这边，蓝的表情立刻变得明媚起来
		ELSE
			PRINTFORML 「哎呀，这不是上次的」
			PRINTFORMDW 蓝漫不经心地向这边打了个招呼
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET进入澡堂，一起进入
	;记入检查（=0, 不显示、1, 显示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTL
		IF TALENT:恋慕
			PRINTFORML 「哎呀，是%CALLNAME:MASTER%啊」
			PRINTFORMDL 门哗啦一声打开，蓝拿着一条手巾走了进来
			PRINTFORMDL 脸上带着愉快的笑容。完全没有要遮掩随着每一步都颤颤巍巍摇晃的乳房的样子
			PRINTFORML 「难得的机会，就让我一起洗吧。你不会拒绝吧？」
			PRINTFORMDW %CALLNAME:MASTER%毫不犹豫地点了点头
		ELSE
			PRINTFORML 「哎呀，已经有客人了」
			PRINTFORMDL 打开门进入浴场的是蓝
			PRINTFORMDL 既没有用毛巾遮掩丰满的裸体，也没有遮掩那摇晃着的沉甸甸的乳房，就这么朝这边走来
			PRINTFORML 「还有空位，我可以打扰一下，在你旁边洗吗？」
			PRINTFORMDW 当然可以，%CALLNAME:MASTER%欢迎蓝的到来
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGETが澡堂に入ってきて出て行った
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTL
			PRINTFORMDL 打开门，有人进入了浴场
			PRINTFORMDL 那是胸前放着一条小手巾的蓝
			PRINTFORMDL 完全不够遮掩起伏很大的身体，乳房的顶端和阴部都暴露在外……
			PRINTFORML 「在洗澡吗。打扰了」
			PRINTFORMDW 蓝没有表现出丝毫的动摇，转过身关上了门
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET进入澡堂，被赶了出来
	;记入检查（=0, 不显示、1, 显示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;在外出地点遭遇
	;记入检查（=0, 不显示、1, 显示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMDL 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;和其他角色约会时遭遇
	;ARG:1为约会对象
	;记入检查（=0, 不显示、1, 显示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		PRINTFORMDL 
		RETURN 1
	ENDIF
ENDIF
RETURN 1

;==================================================
;早上起床时（早上好口上）
;==================================================
@M_KOJO_EVENT_K25_2(ARG,ARG:1)
CALL MESSAGE_GOODMONING(ARG,ARG:1)
;台词主人公为TARGET
;--------------------------------------------------
;记入检查（=0, 不显示、1, 显示）
LOCAL = 1
;-------------
IF LOCAL
	;-------------------------------------------------
	;小分支
	;-------------------------------------------------
	;非同居且对方先起床
	;记入检查（=0, 不显示、1, 显示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;-------------------------------------------------
		;TARGET私室里有MASTER（只有恋爱时陪睡时睡觉时？
		;记入检查（=0, 不显示、1, 显示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMDW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;MASTER私室里有TARGET（即使没有恋爱也有可能进入
		;记入检查（=0, 不显示、1, 显示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMDW 
			RETURN 1
		ENDIF
		;共通
		IF TALENT:恋慕 
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「哦，醒了」
				PRINTFORMDL 睁开眼睛，看到笑嘻嘻的蓝在近距离窥视着
				PRINTFORML 「呵呵呵，是不是稍微恶作剧过头了？」
				PRINTFORMDW 睡觉的时候她到底做了什么
			;想试试早上口交
			CASE 1
				SIF TALENT:MASTER:性別 != 2
					RETURN 1
				PRINTFORMDL 某种奇妙的感觉从背脊窜过，醒了过来
				PRINTFORMDL 传来啪叽啪叽的水声。似乎有人把脸埋在%CALLNAME:MASTER%的胯间，热心地舔舐着龟头
				PRINTFORML 「早上好，%CALLNAME:MASTER%。做了个好梦吗？」
				PRINTFORMDL 蓝用脸颊蹭着肉棒挑眼望过来，带着笑容用舌头舔舐着肉茎
				PRINTFORMDW 不知道睡觉的时候被她舔了多久。不久%CALLNAME:MASTER%就把睡醒的第一发射进了蓝的口中
				BASE:MASTER:精力 -= 100
			;也有舔蛋蛋
			CASE 2
				SIF TALENT:MASTER:性別 != 2
					RETURN 1
				PRINTFORMDL 醒来的瞬间%CALLNAME:MASTER%因为发痒而扭动身体
				PRINTFORMDL 意识迅速清醒，意识到有人在胯间恶作剧
				PRINTFORML 「嗯，早安」
				PRINTFORMDL 蓝含糊不清地说着，口中塞满的右边蛋蛋也随之震动
				PRINTFORMDL 从勃起的肉棒顶端滴下考珀液，拉成丝状落在了蓝的鼻梁上
				PRINTFORMDL 看来蓝是为了叫醒%CALLNAME:MASTER%，一直在不停地舔舐着%CALLNAME:MASTER%的睾丸……
				PRINTFORMDW 最后，蓝用喉咙深处接住了积攒已久喷涌而出的高昂精液
				BASE:MASTER:精力 -= 100
			ENDSELECT
		ELSE
			PRINTFORML 「早上好。睡得好吗，%CALLNAME:MASTER%」
			PRINTFORMDW 醒来后，蓝在旁边正坐着
		ENDIF
		PRINTFORMDW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同栖かつ相手が起きている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋慕 
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「哎呀，起来了」
				PRINTFORMDL 张开眼、蓝贴着脸偷偷的笑着
				PRINTFORML 「呵呵，有点恶作剧的太多了？」
				PRINTFORMDW 自己在睡觉的时候到底发生了什么
			;朝口交をやってみたかった
			CASE 1
				SIF TALENT:MASTER:性別 != 2
					RETURN 1
				PRINTFORMDL 有什么、奇怪的感觉朝脊梁爬了上来
				PRINTFORMDL 噗滋噗滋噗滋的水声。有人在%CALLNAME:MASTER%的两腿间舔舐着龟头
				PRINTFORML 「早上好、%CALLNAME:MASTER%。有做了美梦吗？」
				PRINTFORMDL 吸吮着肉棒的蓝、舔着侧杆抬头望着
				PRINTFORMDW 在我睡着的时间被吸了多久呢。不久%CALLNAME:MASTER%睡醒的第一发在蓝的嘴里射了出来
				BASE:MASTER:精力 -= 100
			;玉舐めもあるぞ
			CASE 2
				SIF TALENT:MASTER:性別 != 2
					RETURN 1
				PRINTFORMDL 要起床时%CALLNAME:MASTER%扭动了下身体
				PRINTFORMDL 突然意识清醒、注意到有人在股间恶作剧
				PRINTFORML 「唔，草肮。」
				PRINTFORMDL 蓝口齿不清的说着、将右边的阴囊含在口内
				PRINTFORMDL 汁液从高耸的肉棒前端滴下、牵着丝线落在蓝的鼻梁上
				PRINTFORMDL 看来蓝闭掉闹钟之后就一直对%CALLNAME:MASTER%的股间玩弄着……
				PRINTFORMDW 在最后、把积存在睾丸的精液朝蓝的喉咙深处灌下
				BASE:MASTER:精力 -= 100
			ENDSELECT
		ELSE
			PRINTFORML 「早上好，睡的还好吗？%CALLNAME:MASTER%」
			PRINTFORMDW 醒来就看到正坐着的蓝
		ENDIF
		PRINTFORMDW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;同栖かつ相手が寝ている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMDW 
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTERが起きていて、相手が起きてきた
	;添い寝时TARGET起床はこちら（TARGET衰弱or陪睡中TARGET睡魔负け后そのままMASTERが起き续けた上で相手が起床した场合限定
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		;-------------------------------------------------
		;非同栖
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && !ARG:1
			PRINTFORMDW 
			RETURN 1
		ENDIF
		;-------------------------------------------------
		;同栖中
		;记入チェック（=0, 非表示、1, 表示）
		LOCAL:1 = 0
		;-------------------------------------------------
		IF LOCAL:1 && ARG:1 == 1
			PRINTFORMDW 
			RETURN 1
		ENDIF
		;共通
		IF TALENT:恋慕
			PRINTFORML 「唔……」
			PRINTFORMDL 睡醒的蓝、刚醒来思绪还很迟钝
			PRINTFORMDW 往轻轻摇晃狐耳的头摸着
		ELSE
			PRINTFORML 「……早上好。你起得真早啊」
			PRINTFORML 「难道看到我的睡脸了吗……啊、没什么」
			PRINTFORMDW 蓝移开视綫、兴冲冲地开始打扮
		ENDIF
		PRINTFORMDW 
		RETURN 1
	ENDIF
	;其他
	PRINTFORMDL 
ENDIF
RETURN 1


;==================================================
;夜寝るとき（おやすみ口上）
;==================================================
@M_KOJO_EVENT_K25_3(ARG,ARG:1)
;台词主人公为TARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------

IF LOCAL
	PRINTL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;自室に返回（定时になって寝る）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		;神社住宿时専用と想定して记述
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「可要好好休息。%CALLNAME:MASTER%一直熬夜可不行」
			PRINTFORMDW 蓝躺下轻轻说
		CASE 1
			PRINTFORML 「那么、亲爱的。晚安」
			PRINTFORMDW 说完缓缓进入梦乡
		CASE 2
			PRINTFORML 「那么。明天见」
			PRINTFORMDW 亲吻过后、蓝缓缓闭上双眼
		ENDSELECT
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;眠そうにしている
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF TALENT:恋慕 || TALENT:思慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……………」
				PRINTFORMW 「抱歉。我刚才走神了」
			CASE 1
				PRINTFORMDL 表面上看起来和平时一样，但身体却很少动弹
				PRINTFORMDW 蓝似乎也无法战胜睡意
			CASE 2
				PRINTFORMDW 蓝的尾巴缓缓地垂了下去。这是她非常想睡觉的信号
			ENDSELECT
		ELSE
			PRINTFORML 「唔—嗯，嗯」
			PRINTFORMDW 她的应和很敷衍。看来差不多到了想睡觉的时间了
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;睡眠中（起きていないので会话というよりは寝言？）
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		IF TALENT:恋慕 || TALENT:思慕
		ELSE
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;恶作剧中
	;记入チェック（=0, 非表示、1, 表示）
	;LOCAL:1 = 0
	;-------------------------------------------------
	;IF LOCAL:1 && ARG == 4
		;PRINTFORMDW 
		;RETURN 1
	;ENDIF
	;-------------------------------------------------
	;衰弱中
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		IF TALENT:恋慕 || TALENT:思慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMDL 蓝无力地靠在%CALLNAME:MASTER%的肩膀上
				PRINTFORMDW 即使是她也到达了体力的极限
			CASE 1
				PRINTFORMDL 毛茸茸的毛发有些失去光泽，尾巴的角度也耷拉了下来
				PRINTFORMDW 现在已经能一眼看出蓝的状态了。我拉起了疲惫的她的手
			CASE 2
				PRINTFORML 「嗯，累了。让我稍微休息一下」
				PRINTFORMDW 虽然蓝不喜欢示弱，但%CALLNAME:MASTER%已经是个例外了
			ENDSELECT
		ELSE
			PRINTFORMW 「……呼。差不多到此为止吧」
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;MASTER倒下（被送到房间）
	;记入检查（=0, 不显示、1, 显示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		IF TALENT:恋人
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「晚安，%CALLNAME:MASTER%」
				PRINTFORMDW 没有抵抗睡意，%CALLNAME:MASTER%将身体交给了蓝的怀抱
			CASE 1
				PRINTFORML 「没关系。剩下的事情交给我处理」
				PRINTFORMDW 听到这话，%CALLNAME:MASTER%安心地闭上了眼睛
			CASE 2
				PRINTFORML 「辛苦了」
				PRINTFORMDW 感觉到背后拥抱的手臂，以及额头上嘴唇的触感
			ENDSELECT
		ELSEIF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「好啦好啦。我背你，抓住我」
				PRINTFORMDW 蓝轻轻松松地把%CALLNAME:MASTER%背了起来
			CASE 1
				PRINTFORML 「稍微让你勉强了。对不起」
				PRINTFORMDW 无数条尾巴轻轻地支撑着几乎要失去意识的%CALLNAME:MASTER%
			CASE 2
				PRINTFORML 「你很努力了」
				PRINTFORMDW 在失去意识之前，%CALLNAME:MASTER%看到了蓝温柔的笑容
			ENDSELECT
		ENDIF
		RETURN 1
	ENDIF
	;其他
	PRINTFORMDL 
ENDIF
RETURN 1

;==================================================
;移动时すれちがい
;==================================================
@M_KOJO_EVENT_K25_4(ARG,ARG:1)
;台词主人公为TARGET
;--------------------------------------------------
;记入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------

IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;立ち止まった
	;この后遭遇口上に接续
	;记入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
