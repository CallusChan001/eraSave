			ELSEIF PALAM:潤滑 > PALAMLV:3
				CALL HPH_PRINT,@"「……哈啊……哈啊………HPH」","W"
				PRINTFORML
				PRINTFORMW 从秘所溢出的爱液在梦子的%PANTSNAME(EQUIP:下半身内衣２, TARGET)%留下了大片的痕迹…
			ELSEIF PALAM:潤滑 > PALAMLV:1
				CALL HPH_PRINT,@"「这里…也想要了呢……？HPH」","W"
				PRINTFORML
				PRINTFORMW 从秘所溢出的爱液在梦子的%PANTSNAME(EQUIP:下半身内衣２, TARGET)%留下了大片的痕迹…
			ENDIF
		CASE 2
			;没穿内裤
			CALL HPH_PRINT,@"「………唔…HPH」","W"
			IF TEQUIP:TARGET:50 == MASTER
				CALL HPH_PRINT,@"「能看清么……我们结合的地方……HPH」","W"
			ELSEIF 充填率(TARGET,1) > 50 || 充填率(TARGET,2) > 50
				CALL HPH_PRINT,@"「%CALLNAME:MASTER%的%M_KOJO_K103_GET("精液")%、咕啾咕啾的……HPH」","W"
			ELSEIF PALAM:潤滑 > PALAMLV:3
				CALL HPH_PRINT,@"「这里…也想要了呢……？HPH」","W"
				PRINTFORML
				PRINTFORMW 爱液像口水一样从梦子的秘所垂落下来…
			ELSEIF PALAM:潤滑 > PALAMLV:1
				CALL HPH_PRINT,@"「这里…也想要了呢……？HPH」","W"
				PRINTFORML
				PRINTFORMW 梦子的秘所似乎有股湿气…
			ENDIF
		ENDSELECT
	ENDIF
RETURN 1

