@M_KOJO_Deter_EVENT_K54_19(ARG,ARG:1)
;口上主はARG
;%CALLNAME:拠点_家主% 即为主地图的家主
;--------------------------------------------------
;启用（=0, 非表示、1, 表示）
LOCAL = 1
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;被允许
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;不被允许
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;因为MASTER的私人房间变更而同居
	;这个只要搬家到角色的房间就可以触发
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SETCOLOR 0x9932CC
		PRINTL 
		PRINTFORML 「以后……就要一起生活了呢」
		PRINTFORMDW 
		RESETCOLOR
		RETURN 1
	ENDIF
ENDIF
RETURN 1
;==================================================
;回家时
;这部分是TARGET住在其他地区时触发的口上
;==================================================
@M_KOJO_Deter_EVENT_K54_20(ARG,ARG:1)
;口上主编号为LOCALS or RESULT
;--------------------------------------------------
;启用（=0, 非表示、1, 表示）
LOCAL = 1
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;通常回家
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		IF CFLAG:317
			IF TALENT:恋慕
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「哎呀……明明还想再抱%CALLNAME:MASTER%一会的呢……」
				PRINTFORMDL 帕秋莉恋恋不舍地放开了%CALLNAME:MASTER%、站起身来\@ EQUIP:0 ? 开始打理身上乱掉的衣服 # 穿上刚刚脱下的衣服 \@
				PRINTFORMDW 
				RESETCOLOR
			ELSE
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「必须得回去了呢……」
				PRINTFORMDL 帕秋莉放开了%CALLNAME:MASTER%、站起身来\@ EQUIP:0 ? 开始打理身上乱掉的衣服 # 穿上刚刚脱下的衣服 \@
				PRINTFORMDW 
				RESETCOLOR
			ENDIF
		ELSE
			IF TALENT:恋慕
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「要回去了呢……」
				PRINTFORMDL 帕秋莉恋恋不舍地看了一眼%CALLNAME:MASTER%、向着红魔馆的方向回去了
				PRINTFORMDW 
				RESETCOLOR
			ELSEIF TALENT:思慕
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「那么、也该回去了呢」
				PRINTFORMDL 帕秋莉向着%CALLNAME:MASTER%点点头之后、向着红魔馆的方向回去了
				PRINTFORMDW 
				RESETCOLOR
			ELSE
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「居然待了这么久……也该回去了」
				PRINTFORMDL 帕秋莉这样小声地喃喃自语着、向着红魔馆的方向回去了
				PRINTFORMDW 
				RESETCOLOR
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;约会中帰宅
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		IF CFLAG:317
			IF CFLAG:衰弱
				;诶嘿嘿时太累了所以回去了
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「呼~哈——我真的——不行了啊……」
				PRINTFORML 「%CALLNAME:MASTER%、下次再带我出来吧……」
				PRINTFORMDW 
				RESETCOLOR
			ELSE
				;诶嘿嘿时要睡觉所以回去了
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「呼~哈——我真的——很困了啊……」
				PRINTFORML 「%CALLNAME:MASTER%、下次再带我出来吧……」
				PRINTFORMDW 
				RESETCOLOR
			ENDIF
		ELSE
			IF TALENT:恋慕
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「虽然很想继续和%CALLNAME:MASTER%待在一起、但是现在真的很晚了呢」
				PRINTFORML 「%CALLNAME:MASTER%、下次再带我出来吧」
				PRINTFORMDW 
				RESETCOLOR
			ELSEIF TALENT:思慕
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「现在真的很晚了呢」
				PRINTFORML 「%CALLNAME:MASTER%、下次再带我出来吧」
				PRINTFORMDW 
				RESETCOLOR
			ELSE
				SETCOLOR 0x9932CC
				PRINTL 
				PRINTFORML 「现在真的很晚了呢」
				PRINTFORML 「那么、我先回去了」
				PRINTFORMDW 
				RESETCOLOR
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;在玄关处送行(角色的自家位置，或者角色的房间约会结束)
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		SETCOLOR 0x9932CC
		PRINTL 
		PRINTFORML 「要走了吗……？」
		PRINTFORML 「下次、也要继续过来哦」
		PRINTFORMDW 
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;约会结束时邀请MASTER来房间(从……开始邀请他来房间)
	;(只有其他地图来访角色，没有自己家的角色才能发生)
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		SETCOLOR 0x9932CC
		PRINTL 
		PRINTFORML 「%CALLNAME:MASTER%不想来我的房间吗？」
		PRINTFORML 「放心吧、一定不会不做什么的哦？」
		PRINTFORMDL 帕秋莉狡黠地看着%CALLNAME:MASTER%……
		PRINTFORMDW 
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;不来房间吗→选择“去”(在TARGET的邀请下，【MASTER】走向了【TARGET】的房间……)
	;（一旦发生，十有八九会被推倒。）
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		SETCOLOR 0x9932CC
		PRINTL 
		PRINTFORML 「那么、请进吧……」
		PRINTFORMDL 帕秋莉将%CALLNAME:MASTER%迎进了自己的房间
		PRINTFORMDW 
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;不来房间吗→选择“不来”(~是无心还是很寂寞……)
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 6
		SETCOLOR 0x9932CC
		PRINTL 
		PRINTFORML 「这样吗？随便你呢」
		PRINTFORMDL 帕秋莉耸了耸肩、自己一个人回房间去了
		PRINTFORMDW 
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET说今晚不想回去(~说今晚不想回去…)
	;（来访角色回家时欲求不满和欲望高的话发生）
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 7
		SETCOLOR 0x9932CC
		PRINTL 
		PRINTFORML 「今晚……想在%CALLNAME:MASTER%的房间睡呢……」
		PRINTFORML 「……可以吗？」
		PRINTFORMDW 
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今晚不想回去→选择“留宿”(~住在了【MASTER自己的房间】)
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 8
		SETCOLOR 0x9932CC
		PRINTL 
		PRINTFORML 「既然答应了……那接下来我们就来做些更有意思的事吧」
		PRINTFORMDW 
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;今晚不想回去→选择“不留宿”，或者家主设置了其他同居者(【MASTER】郑重拒绝了)
	;（家主在MASTER的私人房间里设定了其他角色的私人房间。）
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 9
		SETCOLOR 0x9932CC
		PRINTL 
		PRINTFORML 「这样啊……好吧…」
		PRINTFORMDW 
		RESETCOLOR
		RETURN 1
	ENDIF
	;-------------------------------------------------
	;TARGET累了回家(累坏了~回去了)
	;（送~到入口，TARGET筋疲力尽的时候发生。）
	;启用（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 10
		IF CFLAG:317
			;诶嘿嘿时太累了所以回去了
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
		RETURN 1
	ENDIF
ENDIF
RETURN 1
