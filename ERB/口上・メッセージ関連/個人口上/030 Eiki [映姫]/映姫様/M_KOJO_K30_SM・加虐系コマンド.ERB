@M_KOJO_MESSAGE_COM_K30_100_1
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;初めて
	;-------------------------------------------------
	;时奸中
	IF FLAG:70
		RETURN 1
	ELSEIF FIRSTTIME(SELECTCOM)
		PRINTFORML 「！？」
		PRINTFORML 「诶…突然要干什么啊！？」
		PRINTFORMDW 映姬比起疼痛，更多的是因为突然被打而受到了冲击…
		RETURN 1
	;マグロスイッチオン
	ELSEIF TCVAR:30:385 || TCVAR:30:强行 || TCVAR:30:烂醉
		CALL K30_GASPING_MAGURO
		RETURN 1
	;ABL:受虐属性はちゃんと机能してない？结局お尻を使たプレイになたし随意Ａ感觉で代用
	;不慣れ
	ELSEIF ABL:Ａ感覚 < 3
		PRINTFORML 「%TEXTR("/呜呜～/不要啊～/为什么")%……我、我不记得有做过会被这样对待的事情啊！？」
		PRINTFORML 「应该被打的…不如说是你才对吧…」
		PRINTFORMDW 映姬一边揉着泛红的屁股，一边发出不满的声音…
		RETURN 1
	;非插入时
	ELSEIF ALLSAMES(TEQUIP:50, TEQUIP:51, -1)
		PRINTDATAL
			DATAFORM 「嗯…♥　随心所欲地打了我一顿呢…」\n「总不会就这样…算了吧♥」
			DATAFORM 「真是过分的事情呢…♥」\n「这…需要立刻进行处理才行呢♥」
			DATAFORM 「屁股…火辣辣的…♥」\n「快…安慰我一下…♥」
		ENDDATA
		PRINTFORMDW 映姬左右摇晃着刚被打过的雪臀，似乎在期待着什么…
		IF RAND:2 == 1
			PRINTFORMDL %CALLNAME:MASTER%像安抚幼犬一样舔舐着泛红的屁股
			IF ABL:Ａ感覚 < 5 || RAND:2
				PRINTFORML 「恰啊～♥　等下…%K30_C_NAME(MASTER)%！？」
				PRINTFORML 「总觉得…酥麻的、奇怪的…感觉……嗯嗯～♥」
				PRINTFORMDW 映姬对%CALLNAME:MASTER%那变态般的折磨发出了难耐的声音…
			ELSE
				CALL HPH_PRINT, @"「哈啊HPH　那里HPH　只有那里，不要舔啊…HPH」", "L"
				CALL HPH_PRINT, @"「啊…啊啊HPH　不要…不要戏弄我…不要啊HPH」", "L"
				PRINTFORMDW 被用舌头来回舔舐着发烫的臀部，映姬发出了焦急的娇声…
			ENDIF
		ELSE
			PRINTFORMDL %CALLNAME:MASTER%用爱抚般的动作抚摸着映姬的屁股
			IF ABL:Ａ感覚 < 5 || RAND:2
				PRINTFORML 「啊呜♥　那个，好棒…火辣辣的感觉，飞走了…♥」
				PRINTFORML 「%K30_C_NAME(MASTER)%，我的屁股…想要你更加更加…疼爱我♥」
				PRINTFORMDW 映姬似乎完全沉迷于刺痛转变为快感的这种感觉了…
			ELSE
				CALL HPH_PRINT, @"「嗯啊…HPH　%K30_C_NAME(MASTER)%的手，好温柔，好舒服…但是…」", "L"
				PRINTFORMDL 随着不断抚摸发烫的屁股，股间的爱液也变得更多了
				CALL HPH_PRINT, @"「那个…稍微，有点不够…呜咿咿嗯HPHHPH」", "L"
				PRINTFORMDW 轻轻触碰着因期待而颤抖的花蕾，阎魔大人那不知羞耻的声音响彻四周…
			ENDIF
		ENDIF
		RETURN 1
	ENDIF
	;插入时
	IF PALAM:欲情 < PALAMLV:10 || !RAND:4
		STRDATA LOCALS
			DATAFORM 「啊呃～…♥」\n「现，现在…里面，非常的……」\n「哼～噫～♥」
			DATAFORM 「恰～♥」\n「…被打了，但是怎么这么舒服」\n「为什么，为什么…这样…？」
			DATAFORM 「啊呜…♥」\n「…屁股，发出了…不得了的声音」\n「比起疼痛…更多的是，好羞人…♥」
		ENDDATA
		SIF TEQUIP:51 == MASTER
			LOCALS '= REPLACE(LOCALS, "♥", "HPH")
		CALL HPH_PRINT, LOCALS, "L"
		PRINTFORMDL 稍微控制了力度，特意只是让声音显得响亮地拍打着屁股，煽动映姬的羞耻心，
		PRINTFORMDW 映姬虽然感到困惑，但还是通过\@ !TEQUIP:50 ? 雌 # 菊 \@穴的收缩程度，传达了她的感受…
	ELSE
		STRDATA LOCALS
			DATAFORM 「哈♥　不…不要，那个，不要啊♥」\n「两边都被……啪啪啪什么的，不要啊♥」
			DATAFORM 「啊～！　啊～♥　啊咿～♥」\n「屁股，被打了♥」\n「深♥　深处，感觉到了。♥」
			DATAFORM 「呼——～♥　呼——～♥　呼——～…」\n「…哼噫♥♥」\n「对不起～♥　屁股这么下流、对不起～♥」
		ENDDATA
		SIF TEQUIP:51 == MASTER
			LOCALS '= REPLACE(LOCALS, "♥", "HPH")
		CALL HPH_PRINT, LOCALS, "L"
		PRINTFORMDW 打屁股已经变成了提高\@ TEQUIP:51 == MASTER ? 肛门 # \@性交快感的催化剂、映姬已陷入忘乎所以的迷乱癫狂之中了…
	ENDIF
	PRINTFORML 
	;Ｖ插入/Ａ插入:感觉-低
	IF TEQUIP:50 == MASTER || ABL:Ａ感覚 < 5 || PALAM:欲情 < PALAMLV:10 || RAND:2
		PRINTFORMDL 突然%CALLNAME:MASTER%停止抽插温柔地抚摸映姬的屁股…
		IF TEQUIP:50 == MASTER
			PRINTFORML 「对，那里…火辣辣的…♥嗯啊♥」
			PRINTFORML 「啊啊…♥　好热…♥」
			PRINTFORML 「屁股…又来了，好热啊…♥」
		ELSE
			CALL HPH_PRINT, @"「啊啊啊HPH　不，不行！　屁股，好难过…好难受啊HPH」", "L"
			CALL HPH_PRINT, @"「%K30_C_NAME(MASTER)%的肉棒！　请HPH　快一点、插进来啊HPHHPH」", "L"
		ENDIF
		PRINTFORMDW 因刺痛而变得敏感的屁股被不停爱抚着，\@ !TEQUIP:50 ? 紧紧缠住肉棒 # 湿淋淋的 \@的蜜穴也涌出了新的蜜液…
	;Ａ插入:感觉-高
	ELSE
		PRINTFORMDL %CALLNAME:MASTER%更进一步地将肉棒顶入直肠深处，这次是狠狠地拍打了映姬的屁股
		CALL HPH_PRINT, @"「哈咿HPHHPH　屁股HPH　哈、哈HPH　好激烈HPH」", "L"
		CALL HPH_PRINT, @"「要坏掉了HPH　屁股、要坏掉啦呜呜HPH」", "W"
		PRINTFORMDW 沉醉于被%CALLNAME:MASTER%支配的感觉中，映姬嘴角流着口水不停喘息着…
	ENDIF
ENDIF
RETURN 1

;地の文改变
@K30_TRAIN_MESSAGE_100
CALL K30_D_LINE
IF GROUPMATCH(MASTER, TEQUIP:50, TEQUIP:51)
	PRINTFORMD 一边仔细品味着映姬的\@ !TEQUIP:50 ? 雌 # 菊 \@穴
ELSEIF TEQUIP:0 & 1 && (EQUIP:裙子 || EQUIP:下身衣服 || EQUIP:連衣裙 || EQUIP:和服)
	PRINTFORMD 掀开映姬的%SHOW_BOTTOM(TARGET,1)%
ELSE
	;PRINTFORMD 
ENDIF
PRINTFORMDL 用手掌啪地一声打向映姬
SIF EQUIP:TARGET:レオタード || EQUIP:TARGET:ボディースーツ
	PRINTFORMD %SHOW_下半身内衣(TARGET,2)%包裹之下
PRINTFORMDL 的%HIP(TARGET)%
PRINTFORMD 在%CALLNAME:MASTER%有缓有急地拍打了几次之后、
IF ABL:Ａ感覚 < 3
	PRINTFORMDL 映姬的屁股稍微透红了…
ELSEIF ALLSAMES(TEQUIP:50, TEQUIP:51, -1)
	PRINTFORMDL 映姬的胯间潮水泛滥了…
ELSE
	PRINTFORMDL 映姬的\@ !TEQUIP:50 ? 蜜穴 # 尻穴 \@似乎期待着更进一步的刺激，开始一抽一抽地榨紧着肉棒…
ENDIF
PRINTFORMW 
RETURN

