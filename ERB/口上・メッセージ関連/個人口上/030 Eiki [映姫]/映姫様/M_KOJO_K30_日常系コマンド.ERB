@M_KOJO_MESSAGE_COM_K30_626_1
#DIMS 品名
#DIMS 属性
;-------------------------------------------------
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;贈っていたら本体名と属性を取得しておく
	IF TFLAG:194 > 0
		CALL GIFTDATA_MAIN(GET_GIFTDATA(TCVAR:30:今天的礼物, "本体"))
		品名 = %RESULTS%
		IF STRFIND(RESULTS:1, "握住") >= 0
			属性 = 握住
			SIF STRFIND(RESULTS:1, "装飾") >= 0
				属性 = 装飾
		ELSEIF STRFIND(RESULTS:1, "抱住") >= 0
			属性 = 抱住
		ELSEIF STRFIND(RESULTS:1, "穿戴") >= 0
			属性 = 穿戴
		ELSE
			属性 = 觀賞
		ENDIF
	ENDIF
	;振動棒约会中
	IF TCVAR:30:364
		SELECTCASE TFLAG:194
			CASE -2
				STRDATA LOCALS
					DATAFORM 「…等等，现在…好舒服，抵到舒服的地方HPH」\n「好厉害的高潮、要、要来了…呢…HPH」
					DATAFORM 「…呼—HPH　呼—HPH　呼—HPH」\n「就，再来一下，就到了…HPH」
					DATAFORM 「…是的，我要去了哦HPH」\n「马上，就…就快…就要去了HPH所以说…唔唔HPH」
				ENDDATA
				CALL HPH_PRINT, LOCALS, "L"
				PRINTFORMDL 在%CALLNAME:MASTER%犹豫买什么的时候，映姬似乎一个人快要高潮了
				PRINTFORMDW 为了不被店员发现，%CALLNAME:MASTER%慌忙把映姬拉了出去…
			CASE -1
				PRINTFORMDL 遗憾的是没有找到好的礼物
				PRINTFORMDL %CALLNAME:MASTER%正要离开商店，但映姬没有跟上来…
				PRINTFORMW 
				CALL HPH_PRINT, @"%UNICODE(0x2026)*RAND(6,12)%%TEXTR("啊嗯/要、要去了…/啊唔唔")%HPH", "L"
				STRDATA LOCALS
					DATAFORM 「…嗯啊？　已经，购物…结束了吗？」\n「我这边…等多久都行哦HPH」
					DATAFORM 「…嗯HPH　我知道了…要，出去了呢」\n「要走到外面…了-啊嗯…HPH」
					DATAFORM 「…啊，不行HPH要是…现在开始走动的话…」\n「啊，啊呜呜…HPH」
				ENDDATA
				CALL HPH_PRINT, LOCALS, "L"
				PRINTFORMDL 映姬将购物抛诸脑后，沉溺于肛门的快感中
				PRINTFORMDW 在%CALLNAME:MASTER%的催促下，映姬脚步虚浮地走出店外…
			CASE IS > 0
				PRINTFORMDL %CALLNAME:MASTER%给映姬买了%品名%
				PRINTDATADL
					DATAFORM 映姬大腿内侧不断磨蹭着，非常别扭地微笑感谢…
					DATAFORM 映姬脸上带着融化般的神情，嘴角挂着口水向+++道谢…
					DATAFORM 映姬脸上泛着潮红，眼中含着兴奋的泪水向+++道谢…
				ENDDATA
				PRINTFORMW 
				CALL HPH_PRINT, "「啊HPH　谢-谢…您…嗯HPH」", "L"
				SELECTCASE 属性
					CASE "觀賞", "抱住"
						PRINTFORML 「但是，现在…可以帮我保管吗」
						CALL HPH_PRINT, "「如果，弄掉了…那可就，糟了…HPH」", "W"
					CASE "穿戴"
						CALL HPH_PRINT, "「但是，这个…可以把我保管吗HPH」", "L"
						CALL HPH_PRINT, "「我已经，戴着…很漂亮的饰品了潘HPH」", "W"
					CASE "握住"
						CALL HPH_PRINT, "「那么…我们、赶快试、试试吧…哦咿HPH」", "L"
						PRINTFORMDL 映姬紧紧地握住了%品名%
						PRINTFORMDW 看起来她想要拼命地用力来忍耐快感的冲击…
				ENDSELECT
		ENDSELECT
		RETURN 1
	ENDIF
	SELECTCASE TFLAG:194
		;ひやかし
		CASE -2
			IF TALENT:恋慕
				IF RAND:2 == 1
					PRINTFORMW 「…您认真挑选我很开心，但给店员添麻烦可不好哦」
				ELSE
					PRINTFORML 「…只是和%K30_C_NAME(MASTER)%一起看着就很有趣，连我也忍不住沉迷其中了」
					PRINTFORMW 「下次必须去向店员道歉呢…」
				ENDIF
			ELSE
				IF RAND:2 == 1
					PRINTFORMW 「…被骂也是理所当然的。有什么好犹豫的呢」
				ELSE
					PRINTFORML 「…有这么难以决定的事情吗？」
					PRINTFORMW 「我无法理解」
				ENDIF
			ENDIF
		;やっぱりやめる（見送り）
		CASE -1
			IF TALENT:恋慕
				IF RAND:2 == 1
					PRINTFORMW 「和%K30_C_NAME(MASTER)%一起看看也很开心」
					PRINTFORML 「呼呼呼…没想到我也会有闲逛挑选东西的时候呢」
					PRINTFORMW 「不过，这种使用时间的方式也很棒呢」
				ELSE
					PRINTFORML 「不用勉强挑选也可以哦。我总是收到您的好意」
					PRINTFORMW 「作为交换，之后我们再一起去很多地方吧」
				ENDIF
			ELSE
				IF RAND:2 == 1
					PRINTFORML 「无缘无故地收到礼物也很困扰」
					PRINTFORMW 「所以不用勉强挑选也没关系哦」
				ELSE
					PRINTFORML 「如果只是闲逛，那就适时地停下吧」
					PRINTFORMW 「不能妨碍到店家营业呢」
				ENDIF
			ENDIF
		;400点未満（BAD）
		CASE 1
			;日記フラグ
			SIF DIARY:30:14 == 0
				CALL K30_WRITE_DIARY(14)
			PRINTDATAL
				DATAFORM 「欸…你是在为我挑选礼物吗？」
				DATAFORM 「啊，哎呀？　给我，%品名%…吗？」
				DATAFORM 「呃，礼物是…%品名%……？」
			ENDDATA
			PRINTDATAW
				DATAFORM （我还以为是给其他女孩子的礼物呢…）
				DATAFORM （这个…收下的话会很困扰呢）
				DATAFORM （真伤脑筋…完全不是我喜欢的类型呢…）
			ENDDATA
			PRINTFORMW 「谢、谢谢您…」
		;400点～699点（NOMAL）
		CASE 2
			;日記フラグ
			SIF DIARY:30:13 == 0
				CALL K30_WRITE_DIARY(13)
			PRINTDATAL
				DATAFORM 「%品名%…真的可以吗？　我可以收下吗？」
				DATAFORM 「我以前就很在意%品名%了，可是一直…」\n「…难道说…您知道了？」
				DATAFORM 「我一直盯着%品名%看，被发现了吗…」\n「对不起，总觉得自己好像在撒娇一样呢」
			ENDDATA
			PRINTFORMW 「谢谢您。我会%TEXTR("珍惜/爱惜/")%的」
		;700点以上（GOOD）
		CASE 3
			;日記フラグ
			SIF DIARY:30:12 == 0
				CALL K30_WRITE_DIARY(12)
			;形容詞+本体名を取得
			CALL GIFTNAME(TCVAR:30:今天的礼物)
			PRINTFORML 「啊…%RESULTS%……这个，是给我的吗？」
			PRINTFORMW 「谢谢您。非常棒…」
			SELECTCASE 属性
				CASE "觀賞"
					IF 品名 == "手板"
						PRINTFORML 「…但是，怎么办呢，这个笏板…很想一直拿着呢」
						PRINTFORMW 「但我，也放不下悔悟棒呢……真伤脑筋…」
						PRINTFORMW 「欸？　二刀流…？　这也太……」
					ELSE
						PRINTFORML 「要摆在哪里好呢。必须找到最合适的位置…」
						PRINTFORMDW 映姬小心翼翼地将%品名%收回了包装盒里
					ENDIF
				CASE "抱住"
					PRINTFORML 「收到这么好的东西，回礼可就难办了呢…」
					PRINTFORMDW 映姬小心翼翼地抱住%品名%生怕磕碰了
				CASE "握住"
					PRINTFORML 「呼呼呼，必须好好珍惜呢」
					PRINTFORMDW 映姬珍视地将%品名%藏进怀里
				;映姫様がいいねしてくれる装飾グッズは出ないかもしれない=おそらく死に分岐
				CASE "穿戴"
					IF TALENT:恋慕
						PRINTFORML 
						PRINTFORML 「呃，那个…如果%K30_C_NAME(MASTER)%能……」
						PRINTFORMW 「如果%K30_C_NAME(MASTER)%能…为我戴上这个%品名%…我会很高兴的、可以吗……」
						PRINTFORMDL 映姬虽然羞耻，但还是主动撒起娇来
						PRINTFORMDW %CALLNAME:MASTER%欣然为她戴上了%品名%
						PRINTFORML 
						PRINTFORMW 「怎么样…适合我吗？」
						PRINTFORML 「……呼呼呼，谢谢。我很开心」
						PRINTFORMDW 映姬腼腆地微笑着
					ELSE
						PRINTFORML 
						PRINTFORML 「…这个%品名%，现在就戴上可以吗？」
						PRINTFORMDW 映姬似乎非常喜欢这份礼物
						PRINTFORML 「……感觉非常不错呢」
						PRINTFORMW 「%K30_C_NAME(MASTER)%，真的非常感谢您送这么好的东西」
					ENDIF
			ENDSELECT
	ENDSELECT
ENDIF
RETURN 1
