			IF DIARY:30:25 == 0
				TFLAG:193 = LOCAL < 2 ? 1 # 0
				CALL K30_WRITE_DIARY(25)
			ENDIF
		;保健体育
		CASE 7
			PRINTFORMW 「『%TEXTR("温柔的保健体育/宝宝从哪里来？/女孩子的身体构造/")%』……」
			IF GETBIT(CFLAG:30:既成事實, 1) && RAND:2 == 1
