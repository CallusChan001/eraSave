ELSE
	CALL TRAIN_MESSAGE
	CALL M_KOJO_MESSAGE_COM_K30_300_1
	RETURN RESULT
ENDIF
;TRAIN_MESSAGE抜きで口上メイン呼出
CALL K30_D_LINE
CALL M_KOJO_MESSAGE_COM_K30_300_1
RETURN RESULT

