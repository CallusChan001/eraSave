@キャラデータ103(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "夢子")
;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（背带裙内衣）套")

CASE "便装：帽子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("帽子", "女仆发箍"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "鞋子"))
CASE "便装：襪子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "长筒袜"))
CASE "便装：連衣裙"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("連衣裙", "围裙洋装"))

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "有着谜之重量和冰冷感的")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "梦子胖次")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "梦子胸罩")
CASE "固有内衣着用条件"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 2)

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "面包/牛奶/洋食/葉物/派/健康/淡白/豪華/上品/家庭的/软绵绵/魔力/魔界風/甜/甜/辣/辣/")
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "濃厚/野趣/咸/咸/青蛙/青蛙/青蛙/虫肉/虫肉/虫肉/謎料理/謎料理/謎料理/謎料理/謎料理/")
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "魔界風/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/美的/樸素/洋風/繊細/" + "神霊/刃物/" * 3 + "女僕的/魔界の/懐剣/護刀/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/和風/充滿力量/洛麗塔/" + "生体品/" * 3)
ENDSELECT

