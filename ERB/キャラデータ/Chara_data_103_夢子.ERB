;IMAGE_I103_特殊表情COM(CHARA, コマンド)-------------------------------------------------------
;直前のコマンドで変化するキャラクター特有の表情表示条件
;時間停止中は変化しない表情はこちら
;使用時注意点:
;	RESULTS:0に表情を格納
;	RESULTS:1にマウスオーバー時の表情を格納すること
;	"オナニー","愛撫","性交"は本編で実装済み
;-------------------------------------------------------------------------------------------
@IMAGE_I103_特殊表情COM(CHARA, コマンド)
#DIM CHARA
#DIM コマンド

SELECTCASE CFLAG:103:差し替え適用
	CASE 2
		IF CHECK_NUDE(CHARA)
		ELSE
			IF CURRENT_CLOTHES_SET(CHARA) == 55 || CURRENT_CLOTHES_SET(CHARA) == 66
			ELSEIF CURRENT_CLOTHES_SET(CHARA) == 74
			ELSEIF CURRENT_CLOTHES_SET(CHARA) == 33
			ELSEIF CHECK_CLO("bra_visible", CHARA)
			ELSE
				SELECTCASE コマンド
					CASE 305
						RESULTS = 膝枕
					CASE 330
						SELECTCASE TFLAG:194
							CASE 0
								RESULTS = 掀裙子1
							CASE 1
								IF RAND:2 + RAND:(1 + (ABL:CHARA:露出癖 > 5))
									RESULTS = 掀裙子2
								ELSE
									RESULTS = 掀裙子1
								ENDIF
						ENDSELECT
				ENDSELECT
			ENDIF
		ENDIF
	CASEELSE
		IF CHECK_NUDE(CHARA)
			;裸
			IF FLAG:立絵種類
				;顔絵
			ELSE
				;立ち絵
			ENDIF
		ELSE
			;服
			IF FLAG:立絵種類
				;顔絵
			ELSE
				;立ち絵
			ENDIF
		ENDIF
ENDSELECT
