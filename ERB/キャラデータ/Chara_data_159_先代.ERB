@CHARAMOVE_DATA_159(ARG, ARG:2)
LOCAL = 1
SELECTCASE ARG
	;連動設定（RETURN=依存する角色, RESULT:1～3=対等に連動する角色）
	CASE 1
	;特殊生活時間設定（CFLAG:LOCAL:生活時間補正を設定する）
	CASE 3
	;移動傾向（±～10=行きやすい/行きにくい, -100=行かない）
	CASE 4
		SELECTCASE ARG:2
			;よく居る
			CASE P2境内, P10走廊
				RETURN 5
			;他所
			CASE P30三妖精之家・風呂, P39夢幻遺跡・浴室, P56阿吽の棲家
				RETURN -100
		ENDSELECT
	;出現場所
	CASE 7
		SELECTCASE ARG:2
			CASE 桜花道的鳥居, 神社境内, 神社本堂, 神社走廊
				RETURN 1
			CASE 広場, 商家町, 呑屋小道, 長屋街
			    RETURN 1
			CASE 森林的入口, 香霖堂, 古木的大樹, 霧雨魔法店, 人偶的洋館, 再思之道, 無縁塚
				RETURN 1
			CASE 中有之道, 三途之川, 迷途之家, 白玉楼庭, 白玉楼
			RETURN 1
			CASE 幻想風穴, 地獄的深道, 旧地獄街道
				RETURN 1
		ENDSELECT
	;引力設定（CFLAG:LOCAL:引力点を設定する）
	CASE 8
ENDSELECT

@EXIST_キャラデータ159
#LOCALSIZE 1
#LOCALSSIZE 1

;オブジェクト本体
@キャラデータ159(ARG, O_DATA, V_NAME)
#FUNCTION
#LOCALSIZE 1
#LOCALSSIZE 1
#DIMS O_DATA
#DIMS V_NAME
SELECTCASE O_DATA
CASE "稱呼"
	CALLF MAKE_STR(V_NAME, "先代")

;こっから衣装設定
CASE "今天的睡衣"
	CALLF MAKE_STR(V_NAME, "睡衣（襦袢）套")

CASE "便装：飾品"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("飾品", "袖"))
CASE "便装：靴"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("靴", "鞋子"))
CASE "便装：襪子"
	IF FLAG:2 > 0
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "长筒袜"))
	ELSE
		CALLF MAKE_INT(V_NAME, CLOTHES_ID("襪子", "袜子"))
	ENDIF
CASE "便装：裙子"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("裙子", "裙子"))
CASE "便装：上身衣服１"
	CALLF MAKE_INT(V_NAME, CLOTHES_ID("上身衣服１", "博麗的巫女服"))
	
CASE "固有指令"
	CALLF MAKE_STR(V_NAME, "特殊训练")
CASE "固有指令実行可能"
	CALLF MAKE_INT(V_NAME, !FLAG:特殊训练)
	

;料理関連
CASE "料理：喜欢的味道"
	CALLF MAKE_STR(V_NAME, "米/辣/洋食/葉物/简易/淡白/質素/上品/家庭的/软绵绵/" + "甜/酸/" * 2 + "春物/煎餅/" * 3)
CASE "料理：讨厌的味道"
	CALLF MAKE_STR(V_NAME, "/妖力/" * 2 + "怪诞/謎料理/" * 5)
CASE "料理：助手効果"
	CALLF MAKE_STR(V_NAME, "薄味/得意料理＊和食＊/")
;贈り物関連
CASE "感性：好き"
	CALLF MAKE_STR(V_NAME, "赤/秩序/可愛的/和風/" + "神霊/巫術/护符/狐の/春の/永遠の/" * 3 + "蓮華蝶の/樱花の/巫女の/楽園の/封魔の/充滿力量/帥氣的/" * 5)
CASE "感性：嫌い"
	CALLF MAKE_STR(V_NAME, "混沌/洋風/洛麗塔/狂熱/" + "妖術/" * 3 + "烟袋/" * 5 + "守矢の/妖怪の/" * 10)

CASE "固有内衣形容詞"
	CALLF MAKE_STR(V_NAME, "放出奇怪的光泽")
CASE "固有下半身内衣名"
	CALLF MAKE_STR(V_NAME, "丝绸内裤")
CASE "固有上半身内衣"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有上半身内衣名"
	CALLF MAKE_STR(V_NAME, "丝绸内衣")
CASE "固有内衣着用条件"
		CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(前)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣半透明(後)"
	CALLF MAKE_INT(V_NAME, 1)
CASE "固有内衣布料厚度"
	CALLF MAKE_INT(V_NAME, 1)

;農民関連
;神社は土地持ってそう、神田
CASE "農民：説明"
	CALLF MAKE_STR(V_NAME, "米農家")
CASE "農民：作物"
	CALLF MAKE_STR(V_NAME, "大米")
CASE "農民：田的种类"
	CALLF MAKE_STR(V_NAME, "水田")
CASE "農民：畑の所在地"
	CALLF MAKE_STR(V_NAME, "博麗神社")

CASE "農民：MapID"
	CALLF MAKE_INT(V_NAME, 0)
CASE "農民：場所ID"
	CALLF MAKE_INT(V_NAME, 22)
CASE "農民：報酬"
	CALLF MAKE_INT(V_NAME, 800)
CASE "農民：金利"
	CALLF MAKE_INT(V_NAME, 5)

;ここは依頼IDが関連しない情報
;依頼発生率スカ2/一般3/固有5
CASE "没有依頼発生率"
	CALLF MAKE_INT(V_NAME, 2)
CASE "一般依頼発生率"
	CALLF MAKE_INT(V_NAME, 3)
CASE "固有依頼発生率"
	CALLF MAKE_INT(V_NAME, 5)
CASE "固有依頼最終要素"
	CALLF MAKE_INT(V_NAME, 102)
CASEELSE
	;固有依頼関連のデータにバトンタッチ
	SIF STRCOUNT(O_DATA, "依頼")
		CALLF 固有依頼159(ARG, O_DATA, V_NAME)

;固有Counterテスト


ENDSELECT


;IMAGE_I159_特殊服装(CHARA)-------------------------------------------------------------------
;キャラクター特有の服装表示条件
;エフェクトを共有する服装差分はこちらで指定する
;使用時注意点:
;	RESULTSに特殊服装を格納すること
;	書式は %服装%%特殊服装% 適宜"_"等入れること
;-------------------------------------------------------------------------------------------
@IMAGE_I159_特殊服装(CHARA)
#DIM CHARA
VARSET RESULTS

IF CFLAG:159:差し替え適用
	;差し替え画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
		ELSE
			;立ち絵
		ENDIF
	ENDIF
ELSE
	;通常画像
	IF CHECK_NUDE(CHARA)
		;裸
		IF FLAG:立絵種類
			;顔絵
			IF TALENT:159:胸圍 >= 1
				RESULTS = _爆乳
			ELSEIF CFLAG:159:妊娠経過日数 >= 40
				RESULTS = _妊娠
			ENDIF
		ELSE
			;立ち絵
			IF CFLAG:159:妊娠経過日数 >= 40 && TALENT:159:胸圍 >= 1
				RESULTS = _爆乳妊娠
			ELSEIF TALENT:159:胸圍 >= 1
				RESULTS = _爆乳
			ELSEIF CFLAG:159:妊娠経過日数 >= 40
				RESULTS = _妊娠
			ENDIF
		ENDIF
	ELSE
		;服
		IF FLAG:立絵種類
			;顔絵
			IF TALENT:159:胸圍 >= 1
				IF CURRENT_CLOTHES_SET(CHARA) == 55 || CURRENT_CLOTHES_SET(CHARA) == 66
					RESULTS = _爆乳バニー
				ELSEIF CHECK_CLO("bra_visible", CHARA)
					RESULTS = _爆乳内衣
				ELSE
					RESULTS = _爆乳
				ENDIF
			ELSEIF CFLAG:159:妊娠経過日数 >= 40
				IF CURRENT_CLOTHES_SET(CHARA) == 55 || CURRENT_CLOTHES_SET(CHARA) == 66
					RESULTS = _妊娠バニー
				ELSEIF CHECK_CLO("bra_visible", CHARA)
					RESULTS = _妊娠内衣
				ELSE
					RESULTS = _妊娠
				ENDIF
			ELSE
				IF CURRENT_CLOTHES_SET(CHARA) == 55 || CURRENT_CLOTHES_SET(CHARA) == 66
					RESULTS = _バニー
				ELSEIF CHECK_CLO("bra_visible", CHARA)
					RESULTS = _下着
				ENDIF
			ENDIF
		ELSE
			;立ち絵
			IF CFLAG:159:妊娠経過日数 >= 40 && TALENT:159:胸圍 >= 1
				IF CURRENT_CLOTHES_SET(CHARA) == 55 || CURRENT_CLOTHES_SET(CHARA) == 66
					RESULTS = _爆乳妊娠バニー
				ELSEIF CHECK_CLO("bra_visible", CHARA)
					RESULTS = _爆乳妊娠内衣
				ELSE
					RESULTS = _爆乳妊娠
				ENDIF
			ELSEIF TALENT:159:胸圍 >= 1
				IF CURRENT_CLOTHES_SET(CHARA) == 55 || CURRENT_CLOTHES_SET(CHARA) == 66
					RESULTS = _爆乳バニー
				ELSEIF CHECK_CLO("bra_visible", CHARA)
					RESULTS = _爆乳内衣
				ELSE
					RESULTS = _爆乳
				ENDIF
			ELSEIF CFLAG:159:妊娠経過日数 >= 40
				IF CURRENT_CLOTHES_SET(CHARA) == 55 || CURRENT_CLOTHES_SET(CHARA) == 66
					RESULTS = _妊娠バニー
				ELSEIF CHECK_CLO("bra_visible", CHARA)
					RESULTS = _妊娠内衣
				ELSE
					RESULTS = _妊娠
				ENDIF
			ELSE
				IF CURRENT_CLOTHES_SET(CHARA) == 55 || CURRENT_CLOTHES_SET(CHARA) == 66
					RESULTS = _バニー
				ELSEIF CHECK_CLO("bra_visible", CHARA)
					RESULTS = _下着
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF

