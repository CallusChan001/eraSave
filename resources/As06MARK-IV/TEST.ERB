@立绘测试15
#DIM DYNAMIC CID = 15
#DIMS DYNAMIC 类别 = "別立ち6", "別顔6"
#DIMS DYNAMIC 服装 = "服", "服_水手服", "服_兔女郎", "服_睡袍", "服_吊带", "服_下着", "服_丝带", "服_婚纱", "裸"
#DIMS DYNAMIC 表情 = "通常", "笑顔", "憤怒", "発情", "イキ顔", "睡眠"
FOR K, 0, VARSIZE("类别")
	FOR I, 0, VARSIZE("服装")
		FOR J, 0, VARSIZE("表情")
			CALL 画像セット(@"%类别:K%_%服装:I%_%表情:J%_{CID}", 0, 0, 120, 0, 0)
		NEXT
		CALL 画像一斉表示("左", 0, 1)
	NEXT
	WAIT
NEXT
